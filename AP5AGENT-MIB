      ACCESS      read-only
      STATUS      mandatory
      DESCRIPTION "The description of the AvailCounter"
      ::= {ap5IISAvailCounterEntryVH 1}


--******************************************************************************
--* IISPolledCounterGroup - Counters being polled
--******************************************************************************

   ap5IISPolledCounterTableEntries OBJECT-TYPE
      SYNTAX      INTEGER
      ACCESS      read-only
      STATUS      mandatory
      DESCRIPTION "The number of entries in the table."
      ::= {ap5IISPolledCounterGroup 1}

   ap5IISPolledCounterAdd OBJECT-TYPE
      SYNTAX      DisplayString
      ACCESS      write-only
      STATUS      mandatory
      DESCRIPTION "Add this entry to the polled group."
      ::= {ap5IISPolledCounterGroup 2}

   ap5IISPolledCounterDelete OBJECT-TYPE
      SYNTAX      DisplayString
      ACCESS      write-only
      STATUS      mandatory
      DESCRIPTION "Delete this entry from the polled group."
      ::= {ap5IISPolledCounterGroup 3}

   ap5IISPolledCounterStatus OBJECT-TYPE
      SYNTAX      INTEGER {normal(1), warning(2), critical(3)}
      ACCESS      read-only
      STATUS      mandatory
      DESCRIPTION "Summary status for counters."
      ::= {ap5IISPolledCounterGroup 4}

   ap5IISPolledCounterInterval OBJECT-TYPE
      SYNTAX      INTEGER
      ACCESS      read-write
      STATUS      mandatory
      DESCRIPTION "Seconds to poll for changes."
      ::= {ap5IISPolledCounterGroup 5}

   ap5IISPolledCounterTable OBJECT-TYPE
      SYNTAX      SEQUENCE OF ap5IISPolledCounterEntryS
      ACCESS      not-accessible
      STATUS      mandatory
      DESCRIPTION "This table contains a list of all monitored PolledCounters"
      ::= {ap5IISPolledCounterGroup 6}

   ap5IISPolledCounterWarningCount OBJECT-TYPE
      SYNTAX      INTEGER
      ACCESS      read-only
      STATUS      mandatory
      DESCRIPTION "Times warning threshold exceeded."
      ::= {ap5IISPolledCounterGroup 7}

   ap5IISPolledCounterCriticalCount OBJECT-TYPE
      SYNTAX      INTEGER
      ACCESS      read-only
      STATUS      mandatory
      DESCRIPTION "Times critical threshold exceeded."
      ::= {ap5IISPolledCounterGroup 8}

   ap5IISPolledCounterCmd OBJECT-TYPE
      SYNTAX      DisplayString
      ACCESS      read-write
      STATUS      mandatory
      DESCRIPTION "Modify,desc,warn_thrsh,crit_thrsh,warn_act, crit_act,enabled,descend"
      ::= {ap5IISPolledCounterGroup 9}

   ap5IISPolledCounterEntry OBJECT-TYPE
      SYNTAX      ap5IISPolledCounterEntryS
      ACCESS      not-accessible
      STATUS      mandatory
      DESCRIPTION "This is an entry in the IISPolledCounterTable"
      INDEX       { ap5IISRecDescription }
      ::= { ap5IISPolledCounterTable 1 }

   ap5IISPolledCounterEntryS ::= SEQUENCE
   {
	ap5IISRecDescription           	DisplayString,
	ap5IISRecAvailable             	INTEGER,
	ap5IISRecWarningThreshold      	DisplayString,
	ap5IISRecCriticalThreshold     	DisplayString,
	ap5IISRecValue                 	DisplayString,
	ap5IISRecWarningCount          	INTEGER,
	ap5IISRecCriticalCount         	INTEGER,
	ap5IISRecWarningAction         	DisplayString,
	ap5IISRecCriticalAction        	DisplayString,
	ap5IISRecStatus                	INTEGER,
	ap5IISRecEnableDisable         	INTEGER,
	ap5IISRecMinimum               	DisplayString,
	ap5IISRecMaximum               	DisplayString,
	ap5IISRecAverage               	DisplayString,
	ap5IISRecStdDev               	DisplayString,
	ap5IISRecDescendingCounter     	INTEGER,
	ap5IISRecPrevStatus            	INTEGER,
	ap5IISRecLagSetting		INTEGER,
	ap5IISRecLagValue		INTEGER
   }

   ap5IISRecDescription OBJECT-TYPE
      SYNTAX      DisplayString
      ACCESS      read-only
      STATUS      mandatory
      DESCRIPTION "The description of the PolledCounter"
      ::= {ap5IISPolledCounterEntry 1}

   ap5IISRecAvailable OBJECT-TYPE
      SYNTAX      INTEGER {available(1), notavailable(2)}
