        Ap5WCEntry ::= SEQUENCE
        {
           ap5WCURL                      DisplayString,
           ap5WCReferences               INTEGER,
           ap5WCBadIntLink               INTEGER,
           ap5WCBadExtLink               INTEGER,
           ap5WCTitle                    INTEGER,
           ap5WCBadAnchors               INTEGER,
           ap5WCBadImages                INTEGER,
           ap5WCAccessTS                 DisplayString
        }

        ap5WCURL OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Bad URL."
               ::=  {  ap5WCEntry 1   }

        ap5WCReferences OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Count of references to this bad URL."
               ::=  {  ap5WCEntry 2   }

        ap5WCBadIntLink OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Count of bad internal Links."
               ::=  {  ap5WCEntry 3   }

        ap5WCBadExtLink OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Count of bad external Links."
               ::=  {  ap5WCEntry 4   }

        ap5WCTitle OBJECT-TYPE
               SYNTAX  INTEGER{
                               ok(1),
                               missing(2)
                              }
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Does page have title."
               ::=  {  ap5WCEntry 5   }

        ap5WCBadAnchors OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Count of bad anchors on page."
               ::=  {  ap5WCEntry 6   }

        ap5WCBadImages OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Count of bad images on page."
               ::=  {  ap5WCEntry 7   }

        ap5WCAccessTS OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Time first access occurred (time row added to table)."
               ::=  {  ap5WCEntry 8   }


--******************************************************************************
