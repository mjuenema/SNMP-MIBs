--MibName=ap5Agent
--
--******************************************************************************
--*  WSA 5 Agent MIB prototype for Apache and IIS                              *
--*  edit ap5 to "ap5" prefix                                                   *
--*  edit Ap5 to "Ap5" prefix                                                   *
--*  edit AP5 to "AP5" prefix                                                   *
--*  edit 80 to OID                                                   	       *
--******************************************************************************

AP5AGENT-MIB DEFINITIONS ::= BEGIN

IMPORTS
	enterprises   FROM RFC1155-SMI
	OBJECT-TYPE   FROM RFC1155-SMI
	DisplayString FROM RFC1158-MIB;
        
-- Base OID  1.3.6.1.4.1.791.2.10.2.80

    cai         OBJECT IDENTIFIER ::= { enterprises 791 }

    caiSysMgr   OBJECT IDENTIFIER ::= { cai 2 }

    tng         OBJECT IDENTIFIER ::= { caiSysMgr 10 }

    unix-nt      OBJECT IDENTIFIER ::= { tng 2 }
                      
    ap5Agent   OBJECT IDENTIFIER ::= { unix-nt 80 }

    ap5ServerGroup           OBJECT IDENTIFIER   ::=  {  ap5Agent 1   }
    ap5ServiceGroup          OBJECT IDENTIFIER   ::=  {  ap5Agent 2   }
    ap5BadLinkGroup          OBJECT IDENTIFIER   ::=  {  ap5Agent 3   }
    ap5TopURLGroup           OBJECT IDENTIFIER   ::=  {  ap5Agent 4   }
    ap5URLAvailCounterGroup  OBJECT IDENTIFIER   ::=  {  ap5Agent 5   }
    ap5URLMntrGroup          OBJECT IDENTIFIER   ::=  {  ap5Agent 6   }
    ap5DiskGroup             OBJECT IDENTIFIER   ::=  {  ap5Agent 7   }
    ap5AvailCounterGroup     OBJECT IDENTIFIER   ::=  {  ap5Agent 8   }
    ap5PolledCounterGroup    OBJECT IDENTIFIER   ::=  {  ap5Agent 9   }
    ap5EventGroup            OBJECT IDENTIFIER   ::=  {  ap5Agent 10  }
    ap5AgentGroup            OBJECT IDENTIFIER   ::=  {  ap5Agent 11  }
    ap5WCGroup               OBJECT IDENTIFIER   ::=  {  ap5Agent 12  }
    ap5SHGroup               OBJECT IDENTIFIER   ::=  {  ap5Agent 13  }
    ap5LocalActionGroup      OBJECT IDENTIFIER   ::=  {  ap5Agent 14  }
    ap5ServLogGroup          OBJECT IDENTIFIER   ::=  {  ap5Agent 15  }
    ap5ExpressionGroup       OBJECT IDENTIFIER   ::=  {  ap5Agent 16  }
    ap5DetailGroup    	    OBJECT IDENTIFIER   ::=  {  ap5Agent 17  }
    ap5VHGroup    	    OBJECT IDENTIFIER   ::=  {  ap5Agent 18  }
    ap5IISAvailCounterGroup  OBJECT IDENTIFIER   ::=  {  ap5Agent 19  }
    ap5IISPolledCounterGroup OBJECT IDENTIFIER   ::=  {  ap5Agent 20  }

--******************************************************************************
--* ServerGroup - Site and summary information                               *
--******************************************************************************

        ap5ServerName OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The host name of the computer."
                ::=  {  ap5ServerGroup 1   }

        ap5ServerSummaryStatus OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    ok(1),
                    warning(2),
                    critical(3)
                }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The status of the monitored server."
                ::=  {  ap5ServerGroup 2   }

        ap5ServerPollingInterval OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Poll interval for the server group in seconds."
                ::=  {  ap5ServerGroup 3   }

        ap5ServerResetCounts OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    reset(1)
                }
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Resets all history counts to zero when set."
                ::=  {  ap5ServerGroup 4   }

        ap5ServerResetTS OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Time of last reset in ctime form."
                ::=  {  ap5ServerGroup 5   }

        ap5ServerResetTSEdit OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Time of last reset in display form."
                ::=  {  ap5ServerGroup 6   }

        ap5ServerResetDays OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Number of days between automatic history count resets."
                ::=  {  ap5ServerGroup 7   }

        ap5ServerVersion OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The version of the web server."
                ::=  {  ap5ServerGroup 8   }

        ap5ServerPort OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Port the server is listening on."
                ::=  {  ap5ServerGroup 9   }

        ap5ServerLBHost OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Host name for Agent Collector."
                ::=  {  ap5ServerGroup 10   }

        ap5ServerLBPort OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Agent Collector name."
                ::=  {  ap5ServerGroup 11   }

	ap5ServerLBEnaDis OBJECT-TYPE
		SYNTAX      INTEGER {enable(1), disable(2)}
		ACCESS      read-write
		STATUS      mandatory
		DESCRIPTION "Enable or disable the Agent Collector."
		::= {ap5ServerGroup 12}

        ap5SuperAgentRequest OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Super Agent service request."
                ::=  {  ap5ServerGroup 13   }

        ap5ServerHiLevelVersion OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Oracle server: The high level version number of the web server."
                ::=  {  ap5ServerGroup 14   }

        ap5ServerSetBaseLine OBJECT-TYPE
                SYNTAX  INTEGER {
                                set(1)
                               }
                ACCESS  write-only
                STATUS  mandatory
                DESCRIPTION
                "Immediately set Adaptive Configuration values."
                ::=  {  ap5ServerGroup 15   }

        ap5ServerBaseLineWarnWeight OBJECT-TYPE
                SYNTAX  INTEGER 
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Adaptive Configuration warning weighing factor."
                ::=  {  ap5ServerGroup 16   }

        ap5ServerBaseLineCritWeight OBJECT-TYPE
                SYNTAX  INTEGER 
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Adaptive Configuration critical weighing factor."
                ::=  {  ap5ServerGroup 17   }

	ap5ServerBaseLineEnaDis OBJECT-TYPE
		SYNTAX      INTEGER {disable(1), enable(2)}
		ACCESS      read-write
		STATUS      mandatory
		DESCRIPTION "Enable or disable Active Adaptive Configuration."
		::= {  ap5ServerGroup 18  }

	ap5ServerBaseLineTimer OBJECT-TYPE
		SYNTAX      INTEGER
		ACCESS      read-write
		STATUS      mandatory
		DESCRIPTION "Active Adaptive Configuration timer."
		::= {  ap5ServerGroup 19  }
                
        ap5ServerExportConfigSet OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Name of config to export to."
                ::=  {  ap5ServerGroup 20   }
                
        ap5ServerImportConfigSet OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Name of config to import from."
                ::=  {  ap5ServerGroup 21   }
               
        ap5retVHTableInterval OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Seconds between polls for the VHTable."
               ::=  { ap5ServerGroup 22 }
               

	ap5retVHGPEnaDis OBJECT-TYPE
		SYNTAX      INTEGER {enable(1), disable(2)}
		ACCESS      read-write
		STATUS      mandatory
		DESCRIPTION "Enable or disable for the VHGroup."
		::= { ap5ServerGroup 23 }


--******************************************************************************
--* ServiceGroup - Services                                                  *
--******************************************************************************

        ap5ServiceTableEntries OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The number of entries in the table."
                ::=  {  ap5ServiceGroup 1  }

        ap5ServiceSummaryStatus OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    normal(1),
                    warning(2),
                    critical(3)
                }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Summary status of the service group."
                ::=  {  ap5ServiceGroup 2  }

        ap5ServicePollingInterval OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Poll interval for the service group in seconds."
                ::=  {  ap5ServiceGroup 3  }

        -- Start, start service
        -- Stop, stop service, don't reset autorestart
        -- Recycle, stop and then restart service
        -- Stop no AutoRestart - stop and turn off autorestart
        ap5ServiceControl OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    start(1),
                    stop(2),
                    recycle(3),
                    stop-noautorestart(4)
                }
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Start and stop all services in the service table."
                ::=  {  ap5ServiceGroup 4  }

        ap5ServiceWarningCount OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Number of services in warning state."
                ::=  {  ap5ServiceGroup 5  }

        ap5ServiceCriticalCount OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Number of services in critical state."
                ::=  {  ap5ServiceGroup 6  }

        ap5NumberOfPolls OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Number of times services have been polled"
               ::=  {  ap5ServiceGroup 7}

        ap5ServiceTable OBJECT-TYPE
                SYNTAX  SEQUENCE OF Ap5ServiceEntry
                ACCESS  not-accessible
                STATUS  mandatory
                DESCRIPTION
                "The list of all monitored services."
                ::=  {  ap5ServiceGroup 8}

	ap5ServiceGPEnaDis OBJECT-TYPE
		SYNTAX      INTEGER {enable(1), disable(2)}
		ACCESS      read-write
		STATUS      mandatory
		DESCRIPTION "Enable or disable for the Group."
		::= { ap5ServiceGroup 9 }
		
        ap5ServiceEntry OBJECT-TYPE
                 SYNTAX  Ap5ServiceEntry
                 ACCESS  not-accessible
                 STATUS  mandatory
                 DESCRIPTION
                 "The ServiceTable entry."
                 INDEX { ap5ServiceName }
                     ::=  { ap5ServiceTable 1 }

        Ap5ServiceEntry ::= SEQUENCE
        {
            ap5ServiceName           DisplayString,
            ap5ServiceState          INTEGER,
            ap5AutoRestart           INTEGER,
            ap5AutoReboot            INTEGER,
            ap5RestartAttempts       INTEGER,
            ap5RestartCount          INTEGER,
            ap5RebootAttempts        INTEGER,
            ap5RebootCount           INTEGER,
            ap5LocalAction           DisplayString,
            ap5TotalRestarts         INTEGER,
            ap5TotalReboots          INTEGER,
            ap5ServiceStateStatus    INTEGER,
            ap5ServiceWarnings       INTEGER,
            ap5ServiceCriticals      INTEGER,
            ap5StateIfStopped        INTEGER,
            ap5ServiceCtrl           INTEGER,
            ap5ServiceAvail          DisplayString,
            ap5ServiceRunningPolls   INTEGER,
	    ap5ServicePorts	    DisplayString,
            ap5ServiceType	    DisplayString,
	    ap5ServiceLagSetting	    INTEGER,
	    ap5ServiceLagValue	    INTEGER
        }

        ap5ServiceName OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The name of the service."
                ::=  {  ap5ServiceEntry 1   }

        ap5ServiceState OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    stopped(1),
                    started(2),
                    notinstalled(3),
                    actionpending(4)
                }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The state of the service."
                ::=  {  ap5ServiceEntry 2   }

        ap5AutoRestart OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    yes(1),
                    no(2)
                }
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Flag indicating whether to attempt to restart the service if it's down."
                ::=  {  ap5ServiceEntry 3   }

        ap5AutoReboot OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    yes(1),
                    no(2)
                }
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Flag indicating whether to attempt to reboot the host if service is down."
                ::=  {  ap5ServiceEntry 4   }

        ap5RestartAttempts OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Reboot the host after this many restart attempts are unsuccessful."
                ::=  {  ap5ServiceEntry 5   }

        ap5RestartCount OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Number of service restarts attempted since last successful service restart."
                ::=  {  ap5ServiceEntry 6   }

        ap5RebootAttempts OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Execute local action after this many reboot attempts are unsuccessful."
                ::=  {  ap5ServiceEntry 7   }

        ap5RebootCount OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Number of reboots attempted since last successful service restart."
                ::=  {  ap5ServiceEntry 8   }

        ap5LocalAction OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Local action to perform when reboot attempts exceeded."
                ::=  {  ap5ServiceEntry 9   }

        ap5TotalRestarts OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Total number of service restarts attempted."
                ::=  {  ap5ServiceEntry 10   }

        ap5TotalReboots OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Total number of host reboots attempted."
                ::=  {  ap5ServiceEntry 11   }

        ap5ServiceStateStatus OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    normal(1),
                    warning(2),
                    critical(3)
                }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Status of this service."
                ::=  {  ap5ServiceEntry 12   }

        ap5ServiceWarnings OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Number of warnings generated by this service."
                ::=  {  ap5ServiceEntry 13   }

        ap5ServiceCriticals OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Number of criticals generated by this service."
                ::=  {  ap5ServiceEntry 14   }

        ap5StateIfStopped OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    normal(1),
                    warning(2),
                    critical(3)
                }
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Service status reported if service is stopped."
                ::=  {  ap5ServiceEntry 15   }

        ap5ServiceCtrl OBJECT-TYPE
               SYNTAX  INTEGER {
                                start(1),
                                stop(2),
                                recycle(3),
                                stop-noautorestart(4)
                               }
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Start and stop this service."
               ::=  {  ap5ServiceEntry 16   }

        ap5ServiceAvail OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "% of time service available (running) = running/number of polls"
               ::=  {  ap5ServiceEntry 17  }

        ap5ServiceRunningPolls OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Number of polls showing service running(used for availability calc)."
                ::=  {  ap5ServiceEntry 18   }

        ap5ServicePorts OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Additional PORTS to check for the service."
                ::=  {  ap5ServiceEntry 19   }

	 ap5ServiceType OBJECT-TYPE
	        SYNTAX      DisplayString
	        ACCESS      read-only
	        STATUS      mandatory
	        DESCRIPTION "Type of Service (Server, Admin, FTP)."
	        ::= { ap5ServiceEntry 20 }
		
        ap5ServiceLagSetting OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Lag setting for the service."
                ::=  {  ap5ServiceEntry 21   }
		
        ap5ServiceLagValue OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Current Lag value for the service."
                ::=  {  ap5ServiceEntry 22   }

--******************************************************************************
--* BadLinkGroup - URL links not found by requestors                            *
--******************************************************************************

        ap5BLTableEntries OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The number of entries in the table."
                ::=  {  ap5BadLinkGroup 1   }


        ap5BLClearTable OBJECT-TYPE
               SYNTAX  INTEGER {
                                reset(1)
                               }
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Clear the table when set."
               ::=  {  ap5BadLinkGroup 2   }

        ap5BLSummaryStatus OBJECT-TYPE
               SYNTAX  INTEGER {
                                normal(1),
                                warning(2),
                                critical(3)
                               }
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Summary status for bad links."
               ::=  {  ap5BadLinkGroup 3   }

        ap5BLInterval OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Seconds to poll for changes."
               ::=  {  ap5BadLinkGroup 4   }

        ap5BLCircularBuffSize OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Max entries in the table (default 50)(Not a circular buffer)."
               ::=  {  ap5BadLinkGroup 5   }

        ap5BLWarningThreshold OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Number of requests on a row before warning is generated."
               ::=  {  ap5BadLinkGroup 6   }

        ap5BLCriticalThreshold OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Number of requests on a row before critical is generated."
               ::=  {  ap5BadLinkGroup 7   }

        ap5BLWarningCount OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "The number of entries in the table with a warning status."
               ::=  {  ap5BadLinkGroup 8   }

        ap5BLCriticalCount OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "The number of entries in the table with a critical status."
               ::=  {  ap5BadLinkGroup 9   }

        ap5BLWarningAction OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Command to be executed when warning threshold is crossed on a row."
               ::=  {  ap5BadLinkGroup 10   }

        ap5BLCriticalAction OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Command to be executed when critical threshold is crossed on a row."
               ::=  {  ap5BadLinkGroup 11   }


        ap5BLCreateReportURL OBJECT-TYPE
               SYNTAX  INTEGER {
                                Create(1)
                               }
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "When triggered, create report using path and file in BLReportURL."
               ::=  {  ap5BadLinkGroup 12   }

        ap5BLReportURL OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Path and filename for report; Relative to DocumentRoot unless starts with '/'."
               ::=  {  ap5BadLinkGroup 13   }

        ap5BLTable OBJECT-TYPE
               SYNTAX  SEQUENCE OF Ap5BLEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This table contains a list bad links."
               ::=  {  ap5BadLinkGroup 14  }

	ap5BLGPEnaDis OBJECT-TYPE
		SYNTAX      INTEGER {enable(1), disable(2)}
		ACCESS      read-write
		STATUS      mandatory
		DESCRIPTION "Enable or disable for the Group."
		::= { ap5BadLinkGroup 15 }

        ap5BLEntry OBJECT-TYPE
               SYNTAX  Ap5BLEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This is an entry in the BLTable"
               INDEX { ap5BLURL, ap5BLReferringURL }
               ::=  { ap5BLTable 1 }

        Ap5BLEntry ::= SEQUENCE
        {
           ap5BLURL                              DisplayString,
           ap5BLReferringURL                     DisplayString,
           ap5BLFirstAccessTS                    DisplayString,
           ap5BLlastAccessTS                     DisplayString,
           ap5BLNumRequests                      INTEGER,
           ap5BLStatus                           INTEGER
        }

        ap5BLURL OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Bad URL."
               ::=  {  ap5BLEntry 1   }

        ap5BLReferringURL OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Referring URL to Bad URL."
               ::=  {  ap5BLEntry 2   }

        ap5BLFirstAccessTS OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Time first bad access occurred (time row added to table)."
               ::=  {  ap5BLEntry 3   }

        ap5BLlastAccessTS OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Time of last access to bad URL."
               ::=  {  ap5BLEntry 4   }

        ap5BLNumRequests OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Count of requests."
               ::=  {  ap5BLEntry 5   }

        ap5BLStatus OBJECT-TYPE
               SYNTAX  INTEGER {
                                normal(1),
                                warning(2),
                                critical(3)
                               }
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Summary status for BL."
               ::=  {  ap5BLEntry 6   }


--******************************************************************************
--* TopURLGroup - URLs with highest access rates                             *
--******************************************************************************

        ap5TopURLTableEntries OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "The number of entries in the table."
               ::=  {  ap5TopURLGroup 1   }

        ap5TopURLClearTable OBJECT-TYPE
               SYNTAX  INTEGER {
                                reset(1)
                               }
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Clear the table when set."
               ::=  {  ap5TopURLGroup 2   }

        ap5TopURLInterval OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Seconds to poll for changes."
               ::=  {  ap5TopURLGroup 3   }

        ap5TopURLCircularBuffSize OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Max entries in the table (default 50)."
               ::=  {  ap5TopURLGroup 4   }


        ap5TopURLTable OBJECT-TYPE
               SYNTAX  SEQUENCE OF Ap5TopURLEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This table contains a list of the top URLs by hit count."
               ::=  {  ap5TopURLGroup 5 }

        ap5TopURLExclude OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Exclude URLs with these extensions."
               ::=  {  ap5TopURLGroup 6   }

        ap5TopURLExcludeReset OBJECT-TYPE
               SYNTAX  INTEGER {
                                reset(1)
                               }
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Reset exclude list to defaults."
               ::=  {  ap5TopURLGroup 7   }

	ap5TopURLGPEnaDis OBJECT-TYPE
		SYNTAX      INTEGER {enable(1), disable(2)}
		ACCESS      read-write
		STATUS      mandatory
		DESCRIPTION "Enable or disable for the Group."
		::= { ap5TopURLGroup 8 }

        ap5TopURLEntry OBJECT-TYPE
               SYNTAX  Ap5TopURLEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This is an entry in the TopURLTable"
               INDEX { ap5TopURLURL}
               ::=  { ap5TopURLTable 1 }

        Ap5TopURLEntry ::= SEQUENCE
        {
           ap5TopURLURL          DisplayString,
           ap5TopURLNumHits      INTEGER,
           ap5TopURLRank         INTEGER
        }

        ap5TopURLURL OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "URL."
               ::=  {  ap5TopURLEntry 1   }

        ap5TopURLNumHits OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Number of hits."
               ::=  {  ap5TopURLEntry 2   }

        ap5TopURLRank OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "For easy sorting in Abrowser."
               ::=  {  ap5TopURLEntry 3   }


--******************************************************************************
--* URLAvailCounterGroup - Counters available for URL monitoring             *
--******************************************************************************

        ap5URLAvailCounterTableEntries OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "The number of entries in the table."
               ::=  {  ap5URLAvailCounterGroup 1   }

        ap5URLAvailCounterTable OBJECT-TYPE
           SYNTAX  SEQUENCE OF Ap5URLAvailCounterEntry
           ACCESS  not-accessible
           STATUS  mandatory
           DESCRIPTION
           "This table contains a list of all monitored AvailCounters"
           ::=  {  ap5URLAvailCounterGroup 2}

        ap5URLAvailCounterEntry OBJECT-TYPE
                                SYNTAX  Ap5URLAvailCounterEntry
                                ACCESS  not-accessible
                                STATUS  mandatory
                                DESCRIPTION
                                "This is an entry in the URLAvailCounterTable"
                                INDEX { ap5URLAvailCounterDescription }
                                        ::=  { ap5URLAvailCounterTable 1 }

        Ap5URLAvailCounterEntry ::= SEQUENCE {
                        ap5URLAvailCounterDescription    DisplayString
        }

        ap5URLAvailCounterDescription OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "The description of the AvailCounter"
               ::=  {  ap5URLAvailCounterEntry 1   }

--******************************************************************************
--* URLMntrGroup - Monitored URLs                                            *
--******************************************************************************

        ap5URLMntrTableEntries OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "The number of entries in the table."
               ::=  {  ap5URLMntrGroup 1   }

        ap5URLMntrInterval OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Seconds between polls for the counter."
               ::=  {  ap5URLMntrGroup 2   }

        ap5URLMntrHitRate OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Interval in seconds over which the counter is measured."
               ::=  {  ap5URLMntrGroup 3   }

--      Add a counter to the table.  The value must include at least the first four of
--      the 6 items listed, separated by commas.
--      <ap5URLAvailCounterDescription> is a value from ap5URLAvailCounterDescription in ap5URLAvailCounterTable.

        ap5URLMntrAdd OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  write-only
               STATUS  mandatory
               DESCRIPTION
               "Add this entry to the table: <url>,<URLAvailCounterDescription>,<warning threshold>,<critical threshold>,<warning action>,<critical action>."
               ::=  {  ap5URLMntrGroup 4   }

--      Delete a counter from the table.  The value must include the 2 items listed.
        ap5URLMntrDelete OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  write-only
               STATUS  mandatory
               DESCRIPTION
               "Delete entry from the table: <URLMntrDesc>,<url>."
               ::=  {  ap5URLMntrGroup 5   }

        ap5URLMntrUpdate OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  write-only
               STATUS  mandatory
	       DESCRIPTION "Update this entry in the table."
               ::=  {  ap5URLMntrGroup 6   }

        ap5URLMntrSummaryStatus OBJECT-TYPE
               SYNTAX  INTEGER {
                                normal(1),
                                warning(2),
                                critical(3)
                               }
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Most severe status of all rows in the table."
               ::=  {  ap5URLMntrGroup 7   }

        ap5URLMntrTable OBJECT-TYPE
               SYNTAX  SEQUENCE OF Ap5URLMntrEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This table contains a list of URLs to monitor."
               ::=  {  ap5URLMntrGroup 8}

        ap5URLMntrEntry OBJECT-TYPE
               SYNTAX  Ap5URLMntrEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This is an entry in the URLMntrTable"
               INDEX { ap5URLMntrURL, ap5URLMntrDesc}
               ::=  { ap5URLMntrTable 1 }

        Ap5URLMntrEntry ::= SEQUENCE
        {
           ap5URLMntrURL                 DisplayString,
           ap5URLMntrDesc                DisplayString,
           ap5URLMntrValue               DisplayString,
           ap5URLMntrWarningThreshold    INTEGER,
           ap5URLMntrCriticalThreshold   INTEGER,
           ap5URLMntrWarningAction       DisplayString,
           ap5URLMntrCriticalAction      DisplayString,
           ap5URLMntrStatus              INTEGER,
           ap5URLMntrEnableDisable       INTEGER,
           ap5URLMntrDescending          INTEGER,
           ap5URLMntrReset               INTEGER,
           ap5URLMntrResetTime           DisplayString,
	   ap5URLMntrValueReported	INTEGER,
           ap5URLMntrPrevStatus          INTEGER,
	   ap5URLMntrLagSetting		INTEGER,
	   ap5URLMntrLagValue		INTEGER
        }

        ap5URLMntrURL OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "URL to monitor."
               ::=  {  ap5URLMntrEntry 1   }

        ap5URLMntrDesc OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Description of the monitored value."
               ::=  {  ap5URLMntrEntry 2   }

        ap5URLMntrValue OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Peak number of hits per URLMntrHitRate seconds."
               ::=  {  ap5URLMntrEntry 3   }

        ap5URLMntrWarningThreshold OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Warning threshold in MS"
               ::=  {  ap5URLMntrEntry 4   }

        ap5URLMntrCriticalThreshold OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Critical threshold in MS"
               ::=  {  ap5URLMntrEntry 5   }

        ap5URLMntrWarningAction OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Command to be executed when warning threshold is exceeded."
               ::=  {  ap5URLMntrEntry 6   }

        ap5URLMntrCriticalAction OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Command to be executed when critical threshold is exceeded."
               ::=  {  ap5URLMntrEntry 7  }

        ap5URLMntrStatus OBJECT-TYPE
               SYNTAX  INTEGER {
                                normal(1),
                                warning(2),
                                critical(3)
                               }
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Status of this URL."
               ::=  {  ap5URLMntrEntry 8 }

        ap5URLMntrEnableDisable OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    enable(1),
                    disable(2)
                }
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Enable or disable alerts for this URL."
                ::=  {  ap5URLMntrEntry 9   }

        ap5URLMntrDescending OBJECT-TYPE
               SYNTAX  INTEGER {
                                no(1),
                                yes(2)
                               }
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Yes if lower value triggers thresholds."
               ::=  {  ap5URLMntrEntry 10 }

        ap5URLMntrReset OBJECT-TYPE
               SYNTAX  INTEGER {
                                reset(1)
                               }
               ACCESS  write-only
               STATUS  mandatory
               DESCRIPTION
               "Reset value for this counter."
               ::=  {  ap5URLMntrEntry 11 }

        ap5URLMntrResetTime OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Time stamp of when this counter was reset."
               ::=  {  ap5URLMntrEntry 12  }
	       

        ap5URLMntrValueReported OBJECT-TYPE
               SYNTAX  INTEGER {
                                summary(1),
                                average(2),
				vhost(3)
                               }
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "The reported value of the counter is either a summary or average of all servers, or virtaul host."
               ::=  {  ap5URLMntrEntry 13 }

        ap5URLMntrPrevStatus OBJECT-TYPE
               SYNTAX  INTEGER {
                                normal(1),
                                warning(2),
                                critical(3)
                               }
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Status of this URL."
               ::=  {  ap5URLMntrEntry 14 }
		
        ap5URLMntrLagSetting OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Lag setting for the counter."
                ::=  {  ap5URLMntrEntry 15   }
		
        ap5URLMntrLagValue OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Current Lag value for the counter."
                ::=  {  ap5URLMntrEntry 16   }


        ap5URLMntrDispTableEntries OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "The number of entries in the table."
               ::=  {  ap5URLMntrGroup 9   }

        ap5URLMntrDispTable OBJECT-TYPE
               SYNTAX  SEQUENCE OF Ap5URLMntrDispEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This table contains a list of URLs to monitor."
               ::=  {  ap5URLMntrGroup 10}

	ap5URLMntrGPEnaDis OBJECT-TYPE
		SYNTAX      INTEGER {enable(1), disable(2)}
		ACCESS      read-write
		STATUS      mandatory
		DESCRIPTION "Enable or disable for the Group."
		::= { ap5URLMntrGroup 11 }

        ap5URLMntrResetCmd OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  write-only
               STATUS  mandatory
               DESCRIPTION
               "Reset entry from the table: <URLMntrDesc>,<url>."
               ::=  {  ap5URLMntrGroup 12 }

