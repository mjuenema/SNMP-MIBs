--******************************************************************************
--* ap5PolledCounterGroup - Counters being polled                               *
--******************************************************************************

        ap5PolledCounterTableEntries OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The number of entries in the table."
                ::=  {  ap5PolledCounterGroup 1   }

        ap5PolledCounterAdd OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  write-only
                STATUS  mandatory
                DESCRIPTION
                "Add this counter to the polled counter table."
                ::=  {  ap5PolledCounterGroup 2   }

        ap5PolledCounterDelete OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  write-only
                STATUS  mandatory
                DESCRIPTION
                "Delete this counter from the polled counter table."
                ::=  {  ap5PolledCounterGroup 3   }

        ap5PolledSummaryStatus OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    normal(1),
                    warning(2),
                    critical(3)
                }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The summary status of all polled counters."
                ::=  {  ap5PolledCounterGroup 4   }

        ap5PollingInterval OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Poll interval for the polled counter group in seconds."
                ::=  {  ap5PolledCounterGroup 5   }

        ap5PolledWarningCount OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Number of polled counters in warning state."
                ::=  {  ap5PolledCounterGroup 6   }

        ap5PolledCriticalCount OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Number of polled counters in critical state."
                ::=  {  ap5PolledCounterGroup 7   }

        ap5PolledCounterTable OBJECT-TYPE
                SYNTAX  SEQUENCE OF Ap5PolledCounterEntry
                ACCESS  not-accessible
                STATUS  mandatory
                DESCRIPTION
                "The list of all polled counters."
                ::=  {  ap5PolledCounterGroup 8}

        ap5PolledCounterEntry OBJECT-TYPE
                 SYNTAX  Ap5PolledCounterEntry
                 ACCESS  not-accessible
                 STATUS  mandatory
                 DESCRIPTION
                 "The PolledCounterTable entry."
                 INDEX { ap5Description }
                     ::=  { ap5PolledCounterTable 1 }

        Ap5PolledCounterEntry ::= SEQUENCE {
            ap5Description               DisplayString,
            ap5Available                 INTEGER,
            ap5WarningThreshold          DisplayString,
            ap5CriticalThreshold         DisplayString,
            ap5Value                     DisplayString,
            ap5WarningCount              INTEGER,
            ap5CriticalCount             INTEGER,
            ap5WarningAction             DisplayString,
            ap5CriticalAction            DisplayString,
            ap5Status                    INTEGER,
            ap5EnableDisable             INTEGER,
            ap5DescendingCounter         INTEGER,
            ap5Minimum                   DisplayString,
            ap5Maximum                   DisplayString,
            ap5Average                   DisplayString,
            ap5StdDev                    DisplayString,
	    ap5ValueReported		INTEGER,
            ap5PrevStatus          	INTEGER,
	    ap5LagSetting		INTEGER,
	    ap5LagValue			INTEGER
        }

        ap5Description OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The description of the polled counter."
                ::=  {  ap5PolledCounterEntry 1   }

        ap5Available OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    available(1),
                    notavailable(2)
                }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Describes whether this counter is currently available."
                ::=  {  ap5PolledCounterEntry 2   }

        ap5WarningThreshold OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Counter value that will trigger warning status if value exceeds it."
                ::=  {  ap5PolledCounterEntry 3   }

        ap5CriticalThreshold OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Counter value that will trigger critical status if value exceeds it."
                ::=  {  ap5PolledCounterEntry 4   }

