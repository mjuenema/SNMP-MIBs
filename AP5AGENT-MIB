--******************************************************************************
--* DiskGroup - Primary disks used by the web server                         *
--******************************************************************************

        ap5DiskTableEntries OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The number of entries in the table."
                ::=  {  ap5DiskGroup 1   }

        ap5DiskSummaryStatus OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    ok(1),
                    warning(2),
                    critical(3)
                }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The summary status of all monitored disks."
                ::=  {  ap5DiskGroup 2   }

        ap5DiskPollingInterval OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Poll interval for the disk group in seconds."
                ::=  {  ap5DiskGroup 3   }

        ap5DiskWarningCount OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Number of disks in warning state."
                ::=  {  ap5DiskGroup 4   }

        ap5DiskCriticalCount OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Number of disks in critical state."
                ::=  {  ap5DiskGroup 5  }

        ap5DiskTable OBJECT-TYPE
                SYNTAX  SEQUENCE OF Ap5DiskEntry
                ACCESS  not-accessible
                STATUS  mandatory
                DESCRIPTION
                "The list of all monitored disks."
                ::=  {  ap5DiskGroup 6 }

	ap5DiskGPEnaDis OBJECT-TYPE
		SYNTAX      INTEGER {enable(1), disable(2)}
		ACCESS      read-write
		STATUS      mandatory
		DESCRIPTION "Enable or disable for the Group."
		::= { ap5DiskGroup 7 }

        ap5DiskEntry OBJECT-TYPE
                SYNTAX  Ap5DiskEntry
                ACCESS  not-accessible
                STATUS  mandatory
                DESCRIPTION
                "The DiskTable entry."
                INDEX { ap5DiskID }
                    ::=  { ap5DiskTable 1 }

        Ap5DiskEntry ::= SEQUENCE
        {
            ap5DiskID                 	DisplayString,
            ap5DiskSize               	DisplayString,
            ap5DiskUsed              	DisplayString,
            ap5DiskFree               	DisplayString,
            ap5DiskWarningThreshold   	INTEGER,
            ap5DiskCriticalThreshold  	INTEGER,
            ap5DiskStatus             	INTEGER,
            ap5WarningCounts          	INTEGER,
            ap5CriticalCounts         	INTEGER,
            ap5DiskUsage              	DisplayString,
            ap5PrevWarningCounts      	INTEGER,
            ap5PrevCriticalCounts     	INTEGER,
	    ap5DiskLagSetting		INTEGER,
	    ap5DiskLagValue		INTEGER
        }

        ap5DiskID OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The directory whose disk is measured."
                ::=  {  ap5DiskEntry 1   }

        ap5DiskSize OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Size of the disk in Mbytes."
                ::=  {  ap5DiskEntry 2   }

        ap5DiskUsed OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Space used on the disk in Mbytes"
                ::=  {  ap5DiskEntry 3   }

        ap5DiskFree OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Free space on the disk in Mbytes"
                ::=  {  ap5DiskEntry 4   }

        ap5DiskWarningThreshold OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Percentage of disk used which will trigger warning status."
                ::=  {  ap5DiskEntry 5   }

        ap5DiskCriticalThreshold OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Percentage of disk used which will trigger critical status."
                ::=  {  ap5DiskEntry 6   }

        ap5DiskStatus OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    ok(1),
                    warning(2),
                    critical(3)
                }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The status of the disk."
                ::=  {  ap5DiskEntry 7   }

        ap5WarningCounts OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Number of times warning alert generated."
                ::=  {  ap5DiskEntry 8   }

        ap5CriticalCounts OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Number of times critical alert generated."
                ::=  {  ap5DiskEntry 9   }

        ap5DiskUsage OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "What the web server uses this disk for."
                ::=  {  ap5DiskEntry 10   }

        ap5PrevWarningCounts OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Previous number of times warning alert generated."
               ::=  {  ap5DiskEntry 11  }

        ap5PrevCriticalCounts OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Previous number of times critical alert generated."
               ::=  {  ap5DiskEntry 12   }
		
        ap5DiskLagSetting OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Lag setting for the disk."
                ::=  {  ap5DiskEntry 13   }
		
        ap5DiskLagValue OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Current Lag value for the disk."
                ::=  {  ap5DiskEntry 14   }


