                ::=  {  ap5EventGroup 10  }

        ap5EventCriticalCount OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Number of critical events since last reset."
                ::=  {  ap5EventGroup 11 }

