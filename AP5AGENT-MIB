--******************************************************************************
--* Local action log - circular buffer of local actions performed              *
--* Dynamic get, not polled.                                                   *
--******************************************************************************

        ap5LocalActionTableEntries OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "The number of entries in the local action log table."
               ::=  {  ap5LocalActionGroup 1   }

        ap5LocalActionCircularBuffSize OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Max entries in the LocalActionLogTable (default 50)."
               ::=  {  ap5LocalActionGroup 2   }

        ap5LocalActionRefresh OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Refresh the table starting with the most recent action."
               ::=  {  ap5LocalActionGroup 3   }

        ap5LocalActionStartDateTime OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Refresh the table starting at the specified date and time."
               ::=  {  ap5LocalActionGroup 4   }

        ap5LocalActionScrollDown OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Read the next older set of log entries into the table."
               ::=  {  ap5LocalActionGroup 5   }

        ap5LocalActionScrollUp OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Read the next newer set of log entries into the table."
               ::=  {  ap5LocalActionGroup 6   }

        ap5LocalActionLogTable OBJECT-TYPE
               SYNTAX  SEQUENCE OF Ap5LocalActionLogEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This table contains a list of command actions taken in response to events."
               ::=  {  ap5LocalActionGroup 7 }

        ap5LocalActionLogEntry OBJECT-TYPE
                                SYNTAX  Ap5LocalActionLogEntry
                                ACCESS  not-accessible
                                STATUS  mandatory
                                DESCRIPTION
                                "This is an entry in the LocalActionTable"
                                INDEX { ap5LocalActionSequencer }
                                ::=  { ap5LocalActionLogTable 1 }

        Ap5LocalActionLogEntry ::= SEQUENCE
        {
           ap5LocalActionSequencer               INTEGER,
           ap5LocalActionTimestamp               DisplayString,
           ap5LocalActionSeverity                INTEGER,
           ap5LocalActionReason                  DisplayString,
           ap5LocalActionCommand                 DisplayString
        }

        ap5LocalActionSequencer OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Table sequencer"
               ::=  {  ap5LocalActionLogEntry 1   }

        ap5LocalActionTimestamp OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Time the command was executed (action taken)."
               ::=  {  ap5LocalActionLogEntry 2   }

-- The Warning and Critical values for severity must match CLocalAction::enum Severity.
        ap5LocalActionSeverity OBJECT-TYPE
               SYNTAX  INTEGER
               {
                   normal(1),
                   warning(2),
                   critical(3)
               }
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "The severity of the event which triggered the action."
               ::=  {  ap5LocalActionLogEntry 3   }

        ap5LocalActionReason OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "The reason the action was taken."
               ::=  {  ap5LocalActionLogEntry 4   }

        ap5LocalActionCommand OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Command executed (action taken)."
               ::=  {  ap5LocalActionLogEntry 5   }


