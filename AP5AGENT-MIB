        ap5EventEntry OBJECT-TYPE
                 SYNTAX  Ap5EventEntry
                 ACCESS  not-accessible
                 STATUS  mandatory
                 DESCRIPTION
                 "The EventTable entry."
                 INDEX { ap5EventID }
                 ::=  { ap5EventTable 1 }

        Ap5EventEntry ::= SEQUENCE
        {
                ap5EventID                    INTEGER,
                ap5EVDescription              DisplayString,
                ap5EVSeverity                 INTEGER,
                ap5EVSyslogText               DisplayString,
                ap5EVEnable                   INTEGER
        }

        ap5EventID OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "ID of the event."
                ::=  {  ap5EventEntry 1   }

        ap5EVDescription OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Description of the event."
                ::=  {  ap5EventEntry 2   }

        ap5EVSeverity OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    warning(1),
                    critical(2)
                }
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Severity level of the event."
                ::=  {  ap5EventEntry 3   }

        ap5EVSyslogText OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Text to search for in the syslog file that indicates this event occurred."
                ::=  {  ap5EventEntry 4   }

        ap5EVEnable OBJECT-TYPE
               SYNTAX  INTEGER {
                                dnabled(1),
                                disabled(2)
                               }
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Enable logging of this event."
               ::=  {  ap5EventEntry 5   }


        ap5EventLogEntry OBJECT-TYPE
                 SYNTAX  Ap5EventLogEntry
                 ACCESS  not-accessible
                 STATUS  mandatory
                 DESCRIPTION
                 "The EventTable entry."
                 INDEX { ap5EVLTimestamp, ap5EVLIndex }
                 ::=  { ap5EventLogTable 1 }

        Ap5EventLogEntry ::= SEQUENCE {
           ap5EVLTimestamp               DisplayString,
           ap5EVLID                      INTEGER,
           ap5EVLDescription             DisplayString,
           ap5EVLStatus                  INTEGER,
           ap5EVLIndex                   INTEGER
        }

        ap5EVLTimestamp OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Human-readable timestamp of when the event happened."
                ::=  {  ap5EventLogEntry 1   }

        ap5EVLID OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "ID of the event."
                ::=  {  ap5EventLogEntry 2   }

        ap5EVLDescription OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Description of the event."
                ::=  {  ap5EventLogEntry 3   }

        ap5EVLStatus OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    warning(1),
                    critical(2)
                }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Status caused by the event."
                ::=  {  ap5EventLogEntry 4   }

        ap5EVLIndex OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Index value that ensures uniqueness of events with same ID and timestamp."
                ::=  {  ap5EventLogEntry 5   }


