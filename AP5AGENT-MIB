               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Interval in seconds over which the counter is measured."
               ::=  {  ap5ExpressionGroup 3   }

--      Update this comment.
--      Add a counter to the table.  The value must include at least the first four of
--      the 6 items listed, separated by commas.
--      <ap5URLAvailCounterDescription> is a value from ap5URLAvailCounterDescription in ap5URLAvailCounterTable.

        ap5ExpressionAdd OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  write-only
               STATUS  mandatory
               DESCRIPTION
               "Add this entry to the table."
               ::=  {  ap5ExpressionGroup 4   }

--      Delete a counter from the table.  The value must include the 2 items listed.
        ap5ExpressionDelete OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  write-only
               STATUS  mandatory
               DESCRIPTION
               "Delete entry from the table: <ExpressionDesc>,<Expression>."
               ::=  {  ap5ExpressionGroup 5   }

--      For future use with bulk load.  Currently ignored.
        ap5ExpressionUpdate OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  write-only
               STATUS  mandatory
               DESCRIPTION
               "Reserved for future use."
               ::=  {  ap5ExpressionGroup 6   }

        ap5ExpressionSummaryStatus OBJECT-TYPE
               SYNTAX  INTEGER {
                                normal(1),
                                warning(2),
                                critical(3)
                               }
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Most severe status of all rows in the table."
               ::=  {  ap5ExpressionGroup 7   }

        ap5ExpressionTable OBJECT-TYPE
               SYNTAX  SEQUENCE OF Ap5ExpressionEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This table contains a list of expressions to monitor."
               ::=  {  ap5ExpressionGroup 8}

        ap5ExpressionEntry OBJECT-TYPE
               SYNTAX  Ap5ExpressionEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This is an entry in the ExpressionTable"
               INDEX { ap5ExpressionIdentifier}
               ::=  { ap5ExpressionTable 1 }

        Ap5ExpressionEntry ::= SEQUENCE
        {
           ap5Expression                                    DisplayString,
           ap5ExpressionDesc                                DisplayString,
           ap5ExpressionValue                               DisplayString,
           ap5ExpressionWarningThreshold                    INTEGER,
           ap5ExpressionCriticalThreshold                   INTEGER,
           ap5ExpressionWarningAction                       DisplayString,
           ap5ExpressionCriticalAction                      DisplayString,
           ap5ExpressionStatus                              INTEGER,
           ap5ExpressionEnableDisable                       INTEGER,
           ap5ExpressionDescending                          INTEGER,
           ap5ExpressionResultType                          INTEGER,
           ap5ExpressionBooleanAlertLevel                   INTEGER,
           ap5ExpressionEvalErrorAlertLevel                 INTEGER,
           ap5ExpressionNumericMonitor                      INTEGER,
           ap5ExpressionEntryInterval                       INTEGER,
           ap5ExpressionReset                               INTEGER,
           ap5ExpressionResetTime                           DisplayString,
           ap5ExpressionEntryWarningCount                   INTEGER,
           ap5ExpressionEntryCriticalCount                  INTEGER,
           ap5ExpressionIdentifier                          DisplayString,
           ap5ExpressionErrorMsg                            DisplayString,
           ap5ExpressionEvalStatus                          INTEGER,
           ap5ExpressionBoolAlertCondition                  INTEGER,
	   ap5ExpressionLagSetting	                   INTEGER,
	   ap5ExpressionLagValue	                           INTEGER
        }

--     Changed from read only to read write to allow introducing evaluation errors
        ap5Expression OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Expression to be monitored."
               ::=  {  ap5ExpressionEntry 1   }

        ap5ExpressionDesc OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Description of the monitored expression."
               ::=  {  ap5ExpressionEntry 2   }

        ap5ExpressionValue OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Expression evaluation."
               ::=  {  ap5ExpressionEntry 3   }

        ap5ExpressionWarningThreshold OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Warning threshold"
               ::=  {  ap5ExpressionEntry 4   }

        ap5ExpressionCriticalThreshold OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Critical threshold"
               ::=  {  ap5ExpressionEntry 5   }

        ap5ExpressionWarningAction OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Command to be executed when warning threshold is exceeded."
               ::=  {  ap5ExpressionEntry 6   }

        ap5ExpressionCriticalAction OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Command to be executed when critical threshold is exceeded."
               ::=  {  ap5ExpressionEntry 7  }

        ap5ExpressionStatus OBJECT-TYPE
               SYNTAX  INTEGER {
                                normal(1),
                                warning(2),
                                critical(3)
                               }
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Status of this expression."
               ::=  {  ap5ExpressionEntry 8 }

        ap5ExpressionEnableDisable OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    enable(1),
                    disable(2)
                }
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Enable or disable alerts for this expression."
                ::=  {  ap5ExpressionEntry 9   }

        ap5ExpressionDescending OBJECT-TYPE
               SYNTAX  INTEGER {
                                no(1),
                                yes(2)
                               }
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Yes if lower value triggers thresholds."
               ::=  {  ap5ExpressionEntry 10 }


        ap5ExpressionResultType OBJECT-TYPE
               SYNTAX  INTEGER {
                                numeric(1),
                                boolean(2)
                               }
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Result type of expression."
               ::=  {  ap5ExpressionEntry 11 }

        ap5ExpressionBooleanAlertLevel OBJECT-TYPE
                SYNTAX INTEGER {
                                warning-on-false(1),
                                critical-on-false(2)
                               }
                ACCESS read-write
                STATUS mandatory
                DESCRIPTION
                "Flag indicating for boolean expressions the level of alert to raise on false."
                DEFVAL { 2 }
                ::= { ap5ExpressionEntry 12 }

        ap5ExpressionEvalErrorAlertLevel OBJECT-TYPE
                SYNTAX INTEGER {
                                do-not-monitor(1),
                                warning-on-error(2),
                                critical-on-error(3)
                               }
                ACCESS read-write
                STATUS mandatory
                DESCRIPTION
                "Flag indicating the level of alert to raise on an evaluation error."
                DEFVAL { 3 }
                ::= { ap5ExpressionEntry 13 }

        ap5ExpressionNumericMonitor OBJECT-TYPE
                SYNTAX INTEGER {
                                warning-only(1),
                                critical-only(2),
                                both(3)
                               }
                ACCESS read-write
                STATUS mandatory
                DESCRIPTION
                "Flag indicating for numeric expressions which thresholds should be evaluated."
                DEFVAL { 3 }
                ::= { ap5ExpressionEntry 14 }

        ap5ExpressionEntryInterval OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
