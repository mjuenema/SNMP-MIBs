--******************************************************************************
--* ap5EventGroup - user defined event monitoring                               *
--******************************************************************************
--      In this agent, we'll put available events in wsEventTable ahead of time
--      and allow the user to enable them for monitoring.

        ap5EventTableEntries OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The number of entries in the event description table."
                ::=  {  ap5EventGroup 1   }

        ap5EventLogTableEntries OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The number of entries in the event log table."
                ::=  {  ap5EventGroup 2   }

        ap5EventAdd OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  write-only
                STATUS  mandatory
                DESCRIPTION
                "Event ID to be added to the event table."
                ::=  {  ap5EventGroup 3   }

        ap5EventDelete OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  write-only
                STATUS  mandatory
                DESCRIPTION
                "Event ID to be deleted from the event table."
                ::=  {  ap5EventGroup 4   }

        ap5EventSummaryStatus OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    normal(1),
                    warning(2),
                    critical(3)
                }
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Summary status for events. Not automatic, must be manually reset to normal."
                ::=  {  ap5EventGroup 5   }

        ap5EventInterval OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Poll interval for the Event group in seconds."
                ::=  {  ap5EventGroup 6   }

        ap5EventCircularBuffSize OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Maximum number of entries in the event log table.  When this size is
                exceeded, oldest events are deleted to make room for the newest events."
                ::=  {  ap5EventGroup 7   }

        ap5EventTable OBJECT-TYPE
                SYNTAX  SEQUENCE OF Ap5EventEntry
                ACCESS  not-accessible
                STATUS  mandatory
                DESCRIPTION
                "The table of events being monitored."
                ::=  {  ap5EventGroup 8}

        ap5EventLogTable OBJECT-TYPE
                SYNTAX  SEQUENCE OF Ap5EventLogEntry
                ACCESS  not-accessible
                STATUS  mandatory
                DESCRIPTION
                "The table of events that occurred."
                ::=  {  ap5EventGroup 9}

        ap5EventWarningCount OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
