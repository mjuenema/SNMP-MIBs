	ap5IISDispValue                  DisplayString,
	ap5IISDispWarningCount           INTEGER,
	ap5IISDispCriticalCount          INTEGER,
	ap5IISDispWarningAction          DisplayString,
	ap5IISDispCriticalAction         DisplayString,
	ap5IISDispStatus                 INTEGER,
	ap5IISDispEnableDisable          INTEGER,
	ap5IISDispDescendingCounter      INTEGER,
	ap5IISDispMinimum                DisplayString,
	ap5IISDispMaximum                DisplayString,
	ap5IISDispAverage                DisplayString,
	ap5IISDispStdDev                 DisplayString,
	ap5IISDispValueReported		INTEGER,
	ap5IISDispLagSetting		INTEGER,
	ap5IISDispLagValue		INTEGER
   }

