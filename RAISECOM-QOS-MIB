        SYNTAX SEQUENCE OF rcQosVlanCosServicePolicyEntry
        MAX-ACCESS not-accessible
        STATUS     current
        DESCRIPTION
            "This table specifies vlan and cos service policy."
        ::= { raisecomQosTrafficClass 9 }

rcQosVlanCosServicePolicyEntry OBJECT-TYPE
        SYNTAX rcQosVlanCosServicePolicyEntry
        MAX-ACCESS not-accessible
        STATUS     current
        DESCRIPTION
            "Table entry for vlan and cos service policy."
        INDEX { rcQosVlanCosServicePolicyPort,
                rcQosVlanCosServicePolicyVlan }
        ::= { rcQosVlanCosServicePolicyTable 1 }
    
rcQosVlanCosServicePolicyEntry ::= SEQUENCE {      
        rcQosVlanCosServicePolicyPort               INTEGER,
        rcQosVlanCosServicePolicyVlan               INTEGER,
        rcQosVlanCosServicePolicyMapName     		OCTET STRING,           
        rcQosVlanCosServicePolicyRowStatus		    RowStatus,
        rcQosVlanCosServicePolicyStatsEnable        EnableVar,
        rcQosVlanCosServicePolicyStatsClear         ClearVar
    }
rcQosVlanCosServicePolicyPort OBJECT-TYPE
        SYNTAX 		INTEGER
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The portnum of service policy. "
        ::= { rcQosVlanCosServicePolicyEntry 1 }  
        
rcQosVlanCosServicePolicyVlan OBJECT-TYPE
        SYNTAX 		INTEGER(1..4094|0)
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The vlan index of MplsQos PW HQos. "
        ::= { rcQosVlanCosServicePolicyEntry 2 } 
                     
rcQosVlanCosServicePolicyMapName OBJECT-TYPE
        SYNTAX   OCTET STRING (SIZE (1..16))
        MAX-ACCESS read-create
        STATUS     current
        DESCRIPTION
            "the name of policy map,which is the index of policy map entry."
        ::= { rcQosVlanCosServicePolicyEntry 3 }      
          
rcQosVlanCosServicePolicyRowStatus OBJECT-TYPE
        SYNTAX      RowStatus
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This object indicates the status of this entry."
        ::= { rcQosVlanCosServicePolicyEntry 4 }
 
