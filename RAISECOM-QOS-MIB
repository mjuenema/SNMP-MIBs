rcQosWredTcpConfigTable OBJECT-TYPE
        SYNTAX SEQUENCE OF rcQosWredTcpConfigEntry
        MAX-ACCESS not-accessible
        STATUS     current
        DESCRIPTION
            "This table describes shaping of queues."
        ::= { raisecomQosCfg 17 }

rcQosWredTcpConfigEntry OBJECT-TYPE
        SYNTAX rcQosWredTcpConfigEntry
        MAX-ACCESS not-accessible
        STATUS     current
        DESCRIPTION
            "Table entry for statistic of queues."
        INDEX { rcQosWredQueueId }
        ::= { rcQosWredTcpConfigTable 1 }
        
rcQosWredTcpConfigEntry ::= SEQUENCE { 
	    rcQosWredQueueId	           INTEGER,
        rcQosWredGreenDropStartPoint   INTEGER,  
        rcQosWredGreenDropEndPoint     INTEGER, 
		rcQosWredGreenDropProbability  INTEGER,
        rcQosWredYellowDropStartPoint  INTEGER,
        rcQosWredYellowDropEndPoint    INTEGER,  
        rcQosWredYellowDropProbability INTEGER,   
 		rcQosWredRedDropStartPoint     INTEGER,
        rcQosWredRedDropEndPoint	   INTEGER,
        rcQosWredRedDropProbability    INTEGER,  
        rcQosWredStatus                INTEGER 

    }
    
rcQosWredQueueId OBJECT-TYPE
        SYNTAX      INTEGER
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION 
        	"An index that uniquely identifies a configuration about qos."
        ::= { rcQosWredTcpConfigEntry 1}   
        
