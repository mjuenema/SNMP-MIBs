        ::= { rcQosPortSchedulerQueueEntry 2}
        
rcQosPortSchedulerWRR OBJECT-TYPE
        SYNTAX 		INTEGER
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
