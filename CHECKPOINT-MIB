	    --spam controls subtree
	    msSpamControls  OBJECT IDENTIFIER ::= { msSpam 4 }
	 
		msSpamControlsSpamEngine	OBJECT-TYPE
	      		SYNTAX  INTEGER
				ACCESS  read-only
				STATUS  mandatory 
				DESCRIPTION
					"Number of spam emails by Anti Spam engine"
