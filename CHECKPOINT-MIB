				"Anti-Virus next update description"
			::= {amwAVUpdate 3 }
			
	amwAVVersion OBJECT-TYPE
      		SYNTAX  DisplayString
			ACCESS  read-only
			STATUS  mandatory 
			DESCRIPTION
				"Anti-Virus database version"
			::= {amwAVUpdate 4 }
							
