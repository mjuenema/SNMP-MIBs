		hwBRASIfVlanIfIndex OBJECT-TYPE
			SYNTAX InterfaceIndex
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"
				This object indicates the index of a BRAS interface.
				"
			::= { hwBRASIfVlanEntry 1 }
		
		hwBRASIfVlanVlanId OBJECT-TYPE
			SYNTAX Integer32 (1..4094)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"
				This object indicates the VLAN ID. 
				Note that only the primary interface can be assigned to VLAN 0.
				"
			::= { hwBRASIfVlanEntry 2 }
		
		hwBRASIfVlanVlanNumber OBJECT-TYPE
			SYNTAX Integer32 (0..4094)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"
				This object indicates the number of consecutive VLANs.
				"
			::= { hwBRASIfVlanEntry 3 }
		
		hwBRASIfVlanAccessLimit OBJECT-TYPE
			SYNTAX Integer32 (0..49153)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"
				This object indicates the upper limit of access users. 
				If the value is set to 49153, it indicates that the default value is restored.
				"
			DEFVAL { 49152 }
			::= { hwBRASIfVlanEntry 4 }
		
		hwBRASIfVlanAccessNumber OBJECT-TYPE
			SYNTAX Integer32 (0..49152)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"
				The VLAN access number, default is 49152. 
				"
			::= { hwBRASIfVlanEntry 5 }
		
		hwBRASIfVlanBlock OBJECT-TYPE
