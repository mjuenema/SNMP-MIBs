		hwBRASIfCfgArpInterval OBJECT-TYPE
			SYNTAX Integer32 (0..121)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"
				This object indicates the ARP detection interval. 
				If the value is set to 121, it indicates that the default value is restored.
				"
			DEFVAL { 30 }
			::= { hwBRASIfCfgEntry 12 }
		
		hwBRASIfCfgArpFailTimes OBJECT-TYPE
			SYNTAX Integer32 (2..11)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"
				This object indicates the allowed number of continuous ARP detection failures. 
				If the value is set to 11, it indicates that the default value is restored.
				"
			DEFVAL { 5 }
			::= { hwBRASIfCfgEntry 13 }
		
		hwBRASIfCfgArpProxy OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"
				This object indicates whether proxy ARP is enabled.
				"
			DEFVAL { false }
			::= { hwBRASIfCfgEntry 14 }
		
