  --remote ifTable		    
	raisecomRemoteifTable OBJECT-TYPE
       SYNTAX      SEQUENCE OF raisecomRemoteIfEntry
       MAX-ACCESS  not-accessible
       STATUS      current
       DESCRIPTION
               "A list of remote device's interface entries.  The number of entries
               is given by the value of ifNumber."
       ::= { raisecomRemoteManagementLocalMibObjects 18 } 
       
	raisecomRemoteifEntry OBJECT-TYPE
       SYNTAX      raisecomRemoteIfEntry
       MAX-ACCESS  not-accessible
       STATUS      current
       DESCRIPTION
               "An entry containing management information applicable
               to a particular interface."
       INDEX   { raisecomRemoteIndex,raisecomRemotePortIndex }

       ::= { raisecomRemoteifTable 1 }

   raisecomRemoteIfEntry ::= SEQUENCE {
           raisecomRemoteIndex                   INTEGER,
		   raisecomRemotePortIndex		         Integer32,		    
           raisecomRemoteifDescr                 DisplayString,
           raisecomRemoteifType                  Integer32,
           raisecomRemoteifMtu                   Integer32,
           raisecomRemoteifSpeed                 Gauge32,
           raisecomRemoteifPhysAddress           MacAddress,
           raisecomRemoteifAdminStatus           INTEGER,
           raisecomRemoteifOperStatus            INTEGER,
           raisecomRemoteifLastChange            TimeTicks,
           raisecomRemoteifInOctets              Counter32,
           raisecomRemoteifInUcastPkts           Counter32,
           raisecomRemoteifInNUcastPkts          Counter32,  -- deprecated
           raisecomRemoteifInDiscards            Counter32,
           raisecomRemoteifInErrors              Counter32,
           raisecomRemoteifInUnknownProtos       Counter32,
           raisecomRemoteifOutOctets             Counter32,
           raisecomRemoteifOutUcastPkts          Counter32,
           raisecomRemoteifOutNUcastPkts         Counter32,  -- deprecated
           raisecomRemoteifOutDiscards           Counter32,
           raisecomRemoteifOutErrors             Counter32,
           raisecomRemoteifOutQLen               Gauge32,    -- deprecated
           raisecomRemoteifSpecific              OBJECT IDENTIFIER -- deprecated
       }
   
	raisecomRemoteIndex OBJECT-TYPE
		SYNTAX INTEGER
		ACCESS not-accessible
		STATUS current
		DESCRIPTION
		        "The ifIndex value based 1 which identifies the unique interface of local device."
		::= { raisecomRemoteifEntry 1 }
		       
	raisecomRemotePortIndex OBJECT-TYPE
       SYNTAX      Integer32
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
               "A unique value, greater than zero, for each remote
               interface."
       ::= { raisecomRemoteifEntry 2 } 
 
	raisecomRemoteifDescr OBJECT-TYPE
       SYNTAX      DisplayString (SIZE (0..255))
       MAX-ACCESS  read-only

       STATUS      current
       DESCRIPTION
               "A textual string containing information about the remote
               device interface.  This string should include the name of the
               manufacturer, the product name and the version of the
               interface hardware/software."
       ::= { raisecomRemoteifEntry 3 }

   raisecomRemoteifType OBJECT-TYPE
       SYNTAX      Integer32
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
               "The type of interface.  Additional values for ifType
               are assigned by the Internet Assigned Numbers
               Authority (IANA), through updating the syntax of the
               IANAifType textual convention."
       ::= { raisecomRemoteifEntry 4 }

   raisecomRemoteifMtu OBJECT-TYPE
       SYNTAX      Integer32
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
               "The size of the largest packet which can be
               sent/received on the interface, specified in octets."
       ::= { raisecomRemoteifEntry 5 }

   raisecomRemoteifSpeed OBJECT-TYPE
       SYNTAX      Gauge32
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
               "An estimate of the interface's current bandwidth in
               bits per second.  "
       ::= { raisecomRemoteifEntry 6 }

   raisecomRemoteifPhysAddress OBJECT-TYPE
       SYNTAX      MacAddress
       MAX-ACCESS  read-only
       STATUS      current

       DESCRIPTION
               "The remote device interface's address at its protocol sub-layer."
       ::= { raisecomRemoteifEntry 7 }

   raisecomRemoteifAdminStatus OBJECT-TYPE
       SYNTAX  INTEGER {
                   up(1),       -- ready to pass packets
                   down(2),
                   testing(3)   -- in some test mode
               }
       MAX-ACCESS  read-write
       STATUS      current
       DESCRIPTION
               "The desired state of the remote device interface."
       ::= { raisecomRemoteifEntry 8 }

   raisecomRemoteifOperStatus OBJECT-TYPE
       SYNTAX  INTEGER {
                   up(1),        -- ready to pass packets
                   down(2),
                   testing(3),   -- in some test mode
                   unknown(4),   -- status can not be determined
                                 -- for some reason.
                   dormant(5),
                   notPresent(6),    -- some component is missing
                   lowerLayerDown(7) -- down due to state of
                                     -- lower-layer interface(s)
               }

       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
               "The current operational state of the remote device interface."
       ::= { raisecomRemoteifEntry 9 }

   raisecomRemoteifLastChange OBJECT-TYPE
       SYNTAX      TimeTicks
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
               "The value of sysUpTime at the time the remote device interface
               entered its current operational state."
       ::= { raisecomRemoteifEntry 10 }

   raisecomRemoteifInOctets OBJECT-TYPE
       SYNTAX      Counter32
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
               "The total number of octets received on the remote device interface,
               including framing characters."
       ::= { raisecomRemoteifEntry 11 }

   raisecomRemoteifInUcastPkts OBJECT-TYPE
       SYNTAX      Counter32
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
               "The number of packets, delivered by this sub-layer to
               a higher (sub-)layer, which were not addressed to a
               multicast or broadcast address at this sub-layer."
       ::= { raisecomRemoteifEntry 12 }

   raisecomRemoteifInNUcastPkts OBJECT-TYPE
       SYNTAX  Counter32
       MAX-ACCESS  read-only
       STATUS      deprecated
       DESCRIPTION
               "The number of packets, delivered by this sub-layer to
               a higher (sub-)layer, which were addressed to a
               multicast or broadcast address at this sub-layer."
       ::= { raisecomRemoteifEntry 13 }

   raisecomRemoteifInDiscards OBJECT-TYPE
       SYNTAX      Counter32
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
               "The number of inbound packets which were chosen to be
               discarded even though no errors had been detected to
               prevent their being deliverable to a higher-layer
               protocol."
       ::= { raisecomRemoteifEntry 14 }

   raisecomRemoteifInErrors OBJECT-TYPE
       SYNTAX      Counter32
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
               "For packet-oriented interfaces, the number of inbound
               packets that contained errors preventing them from
               being deliverable to a higher-layer protocol.  For
               character-oriented or fixed-length interfaces, the
               number of inbound transmission units that contained
               errors preventing them from being deliverable to a
               higher-layer protocol."
       ::= { raisecomRemoteifEntry 15 }

   raisecomRemoteifInUnknownProtos OBJECT-TYPE
       SYNTAX      Counter32
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
               "For packet-oriented interfaces, the number of packets
               received via the interface which were discarded
               because of an unknown or unsupported protocol."
       ::= { raisecomRemoteifEntry 16 }

   raisecomRemoteifOutOctets OBJECT-TYPE
       SYNTAX      Counter32
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
               "The total number of octets transmitted out of the
               remote device interface, including framing characters."
       ::= { raisecomRemoteifEntry 17 }

   raisecomRemoteifOutUcastPkts OBJECT-TYPE
       SYNTAX      Counter32
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
               "The total number of packets that higher-level
               protocols requested be transmitted, and which were not
               addressed to a multicast or broadcast address at this
               sub-layer, including those that were discarded or not
               sent."
       ::= { raisecomRemoteifEntry 18 }

   raisecomRemoteifOutNUcastPkts OBJECT-TYPE
       SYNTAX      Counter32
       MAX-ACCESS  read-only
       STATUS      deprecated
       DESCRIPTION
               "The total number of packets that higher-level
               protocols requested be transmitted, and which were
               addressed to a multicast or broadcast address at this
               sub-layer, including those that were discarded or not
               sent."
       ::= { raisecomRemoteifEntry 19 }

   raisecomRemoteifOutDiscards OBJECT-TYPE
       SYNTAX      Counter32
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
               "The number of outbound packets which were chosen to
               be discarded even though no errors had been detected
               to prevent their being transmitted.  One possible
               reason for discarding such a packet could be to free
               up buffer space."
       ::= { raisecomRemoteifEntry 20 }

   raisecomRemoteifOutErrors OBJECT-TYPE
       SYNTAX      Counter32
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
               "For packet-oriented interfaces, the number of
               outbound packets that could not be transmitted because
               of errors.  For character-oriented or fixed-length
               interfaces, the number of outbound transmission units
               that could not be transmitted because of errors."
       ::= { raisecomRemoteifEntry 21 }

   raisecomRemoteifOutQLen OBJECT-TYPE
       SYNTAX      Gauge32
       MAX-ACCESS  read-only
       STATUS      deprecated
       DESCRIPTION
               "The length of the output packet queue (in packets)."
       ::= { raisecomRemoteifEntry 22 }

   raisecomRemoteifSpecific OBJECT-TYPE
       SYNTAX      OBJECT IDENTIFIER
       MAX-ACCESS  read-only
       STATUS      deprecated
       DESCRIPTION
               "A reference to MIB definitions specific to the
               particular media being used to realize the interface."
       ::= { raisecomRemoteifEntry 23 }
       
--remote device auto upload config
	RaisecomRemoteAutoUploadTable OBJECT-TYPE
       SYNTAX      SEQUENCE OF raisecomRemoteAutoUploadEntry
       MAX-ACCESS  not-accessible
       STATUS      current
       DESCRIPTION
               "A list of remote device's interface entries.  The number of entries
               is given by the value of ifNumber."
       ::= { raisecomRemoteManagementLocalMibObjects 19 } 
       
	raisecomRemoteAutoUploadEntry OBJECT-TYPE
       SYNTAX      raisecomRemoteAutoUploadEntry
       MAX-ACCESS  not-accessible
       STATUS      current
       DESCRIPTION
               "An entry containing management information applicable
               to a particular interface."
       INDEX   { raisecomRemoteAutoUploadDevice }
       ::= { RaisecomRemoteAutoUploadTable 1 }

	raisecomRemoteAutoUploadEntry ::= SEQUENCE {
           raisecomRemoteAutoUploadDevice     INTEGER,
		   raisecomRemoteAutoUploadEnable     EnableVar,		    
           raisecomRemoteAutoUploadPeriod     INTEGER       
           }
   
	raisecomRemoteAutoUploadDevice OBJECT-TYPE
		SYNTAX INTEGER
		ACCESS not-accessible
		STATUS current
		DESCRIPTION
		        "The ifIndex value based 1 which identifies the unique interface of local device."
		::= { raisecomRemoteAutoUploadEntry 1 }
 	
	raisecomRemoteAutoUploadEnable OBJECT-TYPE
		SYNTAX EnableVar
		ACCESS read-write
		STATUS current
		DESCRIPTION
		        "The ifIndex value based 1 which identifies the unique interface of local device."
		::= { raisecomRemoteAutoUploadEntry 2 }
		 
	raisecomRemoteAutoUploadPeriod OBJECT-TYPE
		SYNTAX INTEGER (1..1440)
		ACCESS read-write
		STATUS current
		DESCRIPTION
		        "The ifIndex value based 1 which identifies the unique interface of local device."
		::= { raisecomRemoteAutoUploadEntry 3 }		
     	
END
