TELDAT-TRAP-HSSI-MIB DEFINITIONS ::= BEGIN


IMPORTS
	MODULE-IDENTITY,OBJECT-TYPE,OBJECT-IDENTITY,NOTIFICATION-TYPE,Integer32,IpAddress
		FROM SNMPv2-SMI
	DisplayString
		FROM SNMPv2-TC
	telEventTraps
		FROM TELDAT-MIB;


hssiTrapsMIB MODULE-IDENTITY
    LAST-UPDATED "201401200930Z"
    ORGANIZATION "Teldat, S.A."
    CONTACT-INFO 
        "Teldat, S.A.
        Customer Service

        Postal: Parque Tecnologico de Madrid
                Tres Cantos 28760
                Madrid, SPAIN

        Tel.: +34 918 076 565

        http://www.teldat.com"
    DESCRIPTION
        "This Module Information Base file is defined for managing received
        SNMP notifications generated by the HSSI event subsystem."
    REVISION "201401200930Z"
    DESCRIPTION "Initial implementation"
        ::= { telEventTraps 177 }


hssiEventsGroup	OBJECT IDENTIFIER	::= { hssiTrapsMIB 1 }

hssiNotifications	OBJECT IDENTIFIER	::= { hssiTrapsMIB 2 }
hssiNotificationsPrefix	OBJECT IDENTIFIER	::= { hssiNotifications 0 }

hssiEventObjects1 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event HSSI.1
        A packet buffer was not available when the interface self-test
        needed one. "
    ::= { hssiEventsGroup 1 }

hssi1_interface OBJECT-TYPE
    SYNTAX DisplayString
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Parameter 'interface' in event HSSI.1"
    ::= { hssiEventObjects1 1 }

