    --Tabla con informacin de configuracin de los parﬂmetros de SAPs.

    telProdNpMonProtDLSwLLC2SAPTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TelProdNpMonProtDLSwLLC2SAPEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "Displays configuration information on the Saps parameters. There is an
            entry per SAP of which parameters have been modified. As the table
            index is the order number of the entry inside the table, to add a new
            entry (change a SAP parameter not previously changed) a set operation
            is performed to the variable corresponding to the parameter value that
            is going to be changed, and with a residue larger than the number of
            existent entries in that moment. It only admits new entries if the SAP
            number is not already included, in the case of giving a new value to
            a SAP parameter, the position of the associated entry has to be found
            and acceded using the order number as index (as residue)."
        ::={ telProdNpMonProtDLSwLLC2Group 2 }

    telProdNpMonProtDLSwLLC2SAPEntry OBJECT-TYPE
        SYNTAX TelProdNpMonProtDLSwLLC2SAPEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "Each entry consists of an index, that is the order number of the entry
            in the table, the SAP number, and the parameters t1 (Response timer),
            t2 (Received timer for Acknowledgment), ti (Inactive timer), n2 (Maximum
            number of retries value), n3 (Number of I frames received before
            sending Acknowledgment), tw (Transmission window), rw (Receive
            window), nw (Acknowledgments needed to increase Ww) and access priority
            (current LLC2 implementation does not use access priority)."
        INDEX { telProdNpMonProtDLSwLLC2SAPIndex }
        ::={ telProdNpMonProtDLSwLLC2SAPTable 1 }

    TelProdNpMonProtDLSwLLC2SAPEntry ::=
        SEQUENCE {
            telProdNpMonProtDLSwLLC2SAPIndex
                INTEGER,
            telProdNpMonProtDLSwLLC2SAPNumber
                INTEGER,
            telProdNpMonProtDLSwLLC2SAPT1
                INTEGER,
            telProdNpMonProtDLSwLLC2SAPT2
                INTEGER,
            telProdNpMonProtDLSwLLC2SAPTI
                INTEGER,
            telProdNpMonProtDLSwLLC2SAPN2
                INTEGER,
            telProdNpMonProtDLSwLLC2SAPN3
                INTEGER,
            telProdNpMonProtDLSwLLC2SAPTw
                INTEGER,
            telProdNpMonProtDLSwLLC2SAPRw
                INTEGER,
            telProdNpMonProtDLSwLLC2SAPNw
                INTEGER,
            telProdNpMonProtDLSwLLC2SAPAcc
                INTEGER
        }

    telProdNpMonProtDLSwLLC2SAPIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Order number of the entry in the table. It is the table index."
        ::={ telProdNpMonProtDLSwLLC2SAPEntry 1 }

     telProdNpMonProtDLSwLLC2SAPNumber OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "SAP number."
        ::={ telProdNpMonProtDLSwLLC2SAPEntry 2 }

    telProdNpMonProtDLSwLLC2SAPT1 OBJECT-TYPE
        SYNTAX INTEGER (1..256)
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Reply timer. (In seconds). If the variable is set to 0, it
            returns to its default value (1)."
        DEFVAL { 1 }
        ::={ telProdNpMonProtDLSwLLC2SAPEntry 3 }

    telProdNpMonProtDLSwLLC2SAPT2 OBJECT-TYPE
        SYNTAX INTEGER (1..2560)
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Receive Ack timer. (In 100millisec.). If the variable is set to 0, it
            returns to its default value (1)."
        DEFVAL { 1 }
        ::={ telProdNpMonProtDLSwLLC2SAPEntry 4 }

    telProdNpMonProtDLSwLLC2SAPTI OBJECT-TYPE
        SYNTAX INTEGER (1..256)
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Inactivity Timer. (In seconds). If the variable is set to 0, it
            returns to its default value (30)."
        DEFVAL { 30 }
        ::={ telProdNpMonProtDLSwLLC2SAPEntry 5 }

    telProdNpMonProtDLSwLLC2SAPN2 OBJECT-TYPE
        SYNTAX INTEGER (1..255)
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Maximum Retry value (retransmissions). If the variable is set to 0, the
            response is disabled. This is useful in SAP 0: users can disable
            retransmission of TEST and XID scanning."
        DEFVAL { 8 }
        ::={ telProdNpMonProtDLSwLLC2SAPEntry 6 }

    telProdNpMonProtDLSwLLC2SAPN3 OBJECT-TYPE
        SYNTAX INTEGER (1..255)
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Number I-frames received before sending ACK. If the variable is set
            to 0, it returns to its default value (1)."
        DEFVAL { 1 }
        ::={ telProdNpMonProtDLSwLLC2SAPEntry 7 }

    telProdNpMonProtDLSwLLC2SAPTw OBJECT-TYPE
        SYNTAX INTEGER (1..127)
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Transmit Window. If the variable is set to 0, it returns to its
            default value (2)."
        DEFVAL { 2 }
        ::={ telProdNpMonProtDLSwLLC2SAPEntry 8 }

    telProdNpMonProtDLSwLLC2SAPRw OBJECT-TYPE
        SYNTAX INTEGER (1..127)
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Receive Window . If the variable is set to 0, it returns to its
            default value (2)."
        DEFVAL { 2 }
        ::={ telProdNpMonProtDLSwLLC2SAPEntry 9 }

    telProdNpMonProtDLSwLLC2SAPNw OBJECT-TYPE
        SYNTAX INTEGER (1..127)
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Acks needed to increment Ww. If the variable is set to 0, it returns
            to its default value (1)."
        DEFVAL { 1 }
        ::={ telProdNpMonProtDLSwLLC2SAPEntry 10 }

    telProdNpMonProtDLSwLLC2SAPAcc OBJECT-TYPE
        SYNTAX INTEGER (0..7)
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Current LLC2 implementation does not use access priority. Default 0."
        DEFVAL { 0 }
        ::={ telProdNpMonProtDLSwLLC2SAPEntry 11 }


