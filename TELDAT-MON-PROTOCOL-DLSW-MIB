    --Tabla utilizada para poder eliminar sesiones.

    telProdNpMonProtDLSwSessDeleteTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TelProdNpMonProtDLSwSessDeleteEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "This table is used to delete one active session of the two previous
            tables (the table of active sessions and the table of parameters of
            detailed information over the active sessions."
        ::={ telProdNpMonProtDLSwSessionsGroup 3 }

    telProdNpMonProtDLSwSessDeleteEntry OBJECT-TYPE
        SYNTAX TelProdNpMonProtDLSwSessDeleteEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "Each entry consists of an index that is used for crossing the table,
            and that is the order in which the entry corresponding to the session
            that is wished to delete in the two previous tables (table of active
            sessions and table of detailed parameters of the active sessions)
            appears, and of an erasure control variable, that indicates the result
            of the last erasure operation in reading, and in writing it is useful
            for initializing the operation when its value is 1 (erase)."
        INDEX { telProdNpMonProtDLSwSessDeleteIndex }
        ::={ telProdNpMonProtDLSwSessDeleteTable 1 }

    TelProdNpMonProtDLSwSessDeleteEntry ::=
        SEQUENCE {
            telProdNpMonProtDLSwSessDeleteIndex
                INTEGER,
            telProdNpMonProtDLSwSessDeleteDo
                INTEGER
        }

    telProdNpMonProtDLSwSessDeleteIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Table index, that will be the order number of entry to previous
            tables (active session table and detailed parameters of active
            sessions table), and therefore identifies the session to delete."
        ::={ telProdNpMonProtDLSwSessDeleteEntry 1 }

    telProdNpMonProtDLSwSessDeleteDo OBJECT-TYPE
        SYNTAX INTEGER {
            undefined(4),
            error(3),
            ok(2),
            delete(1)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "If the variable is set to 1, the session corresponding to the entry
            of which order number is indicated by the index, and the entries
            corresponding to it in the previous table disappear (active session
            table and detailed parameters of active sessions table). In a read
            operation (get) the value 2 will be get if the last delete attempt
            succeeded, 3 if an error ocurred and 4 if no sessions delete operation
            has been performed."
        ::={ telProdNpMonProtDLSwSessDeleteEntry 2 }


    --Grupo que informa sobre prioridades.

    telProdNpMonProtDLSwPriorityGroup OBJECT IDENTIFIER ::= { telProdNpMonProtDLSw 5 }

    telProdNpMonProtDLSwPrioritySNA OBJECT-TYPE
        SYNTAX INTEGER {
            critical(4),
            high(3),
            medium(2),
            low(1),
            undefined(0)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Priority for SNA DLSw sessions."
        ::={ telProdNpMonProtDLSwPriorityGroup 1 }

    telProdNpMonProtDLSwPriorityNetBIOS OBJECT-TYPE
        SYNTAX INTEGER {
            critical(4),
            high(3),
            medium(2),
            low(1),
            undefined(0)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Priority for NetBIOS DLSw sessions."
        ::={ telProdNpMonProtDLSwPriorityGroup 2 }

    telProdNpMonProtDLSwPriorityCHML OBJECT-TYPE
        SYNTAX DisplayString (SIZE(0..4))
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Message allocation by priority. These 4 bytes indicate the maximum
            number of messages transmitted with a certain priority before
            transmitting messages with the following priority. Here they are shown
            as a 4 octect string, one per priority, and they correspond to C/H/M/L."
        ::={ telProdNpMonProtDLSwPriorityGroup 3 }

    telProdNpMonProtDLSwPriorityMaxFraSizNB OBJECT-TYPE
        SYNTAX INTEGER {
            very_high (4399),
            high (2052),
            medium (1470),
            low (516)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Maximum NetBIOS frame size. Any value can be assigned to this
            variable through a set operation, but it will be rounded up to 516 (low)
            if it is under 516 or between 516 and 1470, to 1470 (medium) if it
            is between 1470 and 2502, and to 2502 (high) if it is between 2502
            and 4399, or 4399 (too high) if it is bigger than this value."
        ::={ telProdNpMonProtDLSwPriorityGroup 4 }


    --Tabla para crear y eliminar SAPs.

    telProdNpMonProtDLSwSAPTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TelProdNpMonProtDLSwSAPEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "Enable the transmitting of data for the specified link SAP
            by the DLSw protocol. It should be executed on the router
            which resides on the session initiator side of the
            connection."
        ::={ telProdNpMonProtDLSw 6 }

    telProdNpMonProtDLSwSAPEntry OBJECT-TYPE
        SYNTAX TelProdNpMonProtDLSwSAPEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "Each entry consists of two index, the interface number and the order
            number of the SAP inside the interface, and a variable to open
            and close SAP."
        INDEX { telProdNpMonProtDLSwSAPIfcIndex, telProdNpMonProtDLSwSAPNumIndex }
        ::={ telProdNpMonProtDLSwSAPTable 1 }

    TelProdNpMonProtDLSwSAPEntry ::=
        SEQUENCE {
            telProdNpMonProtDLSwSAPIfcIndex
                INTEGER,
            telProdNpMonProtDLSwSAPNumIndex
                INTEGER,
            telProdNpMonProtDLSwSAPCommand
                INTEGER
        }

    telProdNpMonProtDLSwSAPIfcIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Interface number, from 1 to the maximum number of
            node interfaces (in console begin from 0 and here
            from 1). It is the first index."
        ::={ telProdNpMonProtDLSwSAPEntry 1 }

    telProdNpMonProtDLSwSAPNumIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "SAP in decimal. Possible values:
            - values ranging between 0 and F4 (being even number),
            - 245 that will open 'SNA', ports  04, 08 and 0C,
            - 240 that will open 'NB' port, F0 for NetBIOS,
            - 244 that will open 'LNM' port F4."
        ::={ telProdNpMonProtDLSwSAPEntry 2 }

    telProdNpMonProtDLSwSAPCommand OBJECT-TYPE
        SYNTAX INTEGER {
            not_valid(4),
            close_sap(3),
            open_sap(2),
            valid(1)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Variable used to open or close SAPs. In a set operation the values
            2 (open_sap) and 3 (close_sap) are accepted, and in a get operation
            the value 1 will be get if the open-SAP operation succeede, and 4
            (invalid) if the open-SAP operation failed."
        ::={ telProdNpMonProtDLSwSAPEntry 3 }


