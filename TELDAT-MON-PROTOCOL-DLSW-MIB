    telProdNpMonProtDLSwLLC2SessFlag OBJECT-TYPE
        SYNTAX INTEGER {
            sap_resolve_pending-start_by_xid-llcbusy-tcpbusy(15),
            sap_resolve_pending-start_by_xid-tcpbusy(14),
            sap_resolve_pending-start_by_xid-llcbusy(13),
            sap_resolve_pending-start_by_xid(12),
            sap_resolve_pending-llcbusy-tcpbusy(11),
            sap_resolve_pending-tcpbusy(10),
            sap_resolve_pending-llcbusy(9),
            sap_resolve_pending(8),
            start_by_xid-llcbusy-tcpbusy(7),
            start_by_xid-tcpbusy(6),
            start_by_xid-llcbusy(5),
            start_by_xid(4),
            llcbusy-tcpbusy(3),
            tcpbusy(2),
            llcbusy(1),
            nothing(0)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "One of the following values:
            NOTHING (0), LLCBUSY (1), TCPBUSY (2), LLCBUSY+TCPBUSY (3),
            START_BY_XID (4), START_BY_XID+LLCBUSY (5), START_BY_XID+TCPBUSY (6),
            START_BY_XID+LLCBUSY+TCPBUSY (7), SAP_RESOLVE_PENDING (8),
            SAP_RESOLVE_PENDING+LLCBUSY (9), SAP_RESOLVE_PENDING+TCPBUSY (10),
            SAP_RESOLVE_PENDING+LLCBUSY+TCPBUSY (11),
            SAP_RESOLVE_PENDING+START_BY_XID (12),
            SAP_RESOLVE_PENDING+START_BY_XID+LLCBUSY (13),
            SAP_RESOLVE_PENDING+START_BY_XID+TCPBUSY (14),
            SAP_RESOLVE_PENDING+START_BY_XID+LLCBUSY+TCPBUSY (15)."
        ::={ telProdNpMonProtDLSwLLC2SessionsEntry 7 }

    telProdNpMonProtDLSwLLC2SessRIF OBJECT-TYPE
        SYNTAX DisplayString (SIZE(0..29))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The information that is included in the RIF of the LLC TEST frame."
        ::={ telProdNpMonProtDLSwLLC2SessionsEntry 8 }


