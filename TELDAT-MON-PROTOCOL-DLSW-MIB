    --Tabla que contiene la informacin referida a todas las sesiones TCP
    --configuradas.

    telProdNpMonProtDLSwTCPConfigTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TelProdNpMonProtDLSwTCPConfigEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "Displays the information in all the configured TCP sessions."
        ::={ telProdNpMonProtDLSwTCPGroup 2 }

    telProdNpMonProtDLSwTCPConfigEntry OBJECT-TYPE
        SYNTAX TelProdNpMonProtDLSwTCPConfigEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "Each entry consists of an index, that will be the order number of
            the table entry, a variable to add or delete entries, neighbor
            IP Address, Transmit Buffer size, Receive Buffer size, maximum
            segment size, variable to activate or not the DLSw neighbor possibility
            to send keepalive messages, another that indicates the neighbor≥s
            priority, and neighbor connection mode."
        INDEX { telProdNpMonProtDLSwTCPConfIndex }
        ::={ telProdNpMonProtDLSwTCPConfigTable 1 }

    TelProdNpMonProtDLSwTCPConfigEntry ::=
        SEQUENCE {
            telProdNpMonProtDLSwTCPConfIndex
                INTEGER,
            telProdNpMonProtDLSwTCPConfCommand
                INTEGER,
            telProdNpMonProtDLSwTCPConfIpAddr
                IpAddress,
            telProdNpMonProtDLSwTCPConfTxBufsize
                INTEGER,
            telProdNpMonProtDLSwTCPConfRxBufsize
                INTEGER,
            telProdNpMonProtDLSwTCPConfMaxSegsize
                INTEGER,
            telProdNpMonProtDLSwTCPConfKeepAlive
                INTEGER,
            telProdNpMonProtDLSwTCPConfPriority
                INTEGER,
            telProdNpMonProtDLSwTCPConfCnxMode
                INTEGER,
            telProdNpMonProtDLSwTCPConfInacTime
                INTEGER,
            telProdNpMonProtDLSwTCPConfLowPriMax
                INTEGER,
            telProdNpMonProtDLSwTCPConfLowPriMin
                INTEGER,
            telProdNpMonProtDLSwTCPConfLowPriRetries
                INTEGER,
            telProdNpMonProtDLSwTCPConfLowPriTimeout
                INTEGER,
            telProdNpMonProtDLSwTCPConfLowPriMaxPercent
                INTEGER,
            telProdNpMonProtDLSwTCPConfLowPriMinPercent
                INTEGER
        }

    telProdNpMonProtDLSwTCPConfIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Table index: entry order number."
        ::={ telProdNpMonProtDLSwTCPConfigEntry 1 }

    telProdNpMonProtDLSwTCPConfCommand OBJECT-TYPE
        SYNTAX INTEGER {
            not_valid(4),
            delete(3),
            add(2),
            valid(1)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Variable used to add and delete new TCP neighbor entries.
            When writting it admits the value 2 (add) and 3 (delete), and
            when reading it will be got the value 1 (valid) if the last operation
            succeeded, and 4 (invalid) if failed."
        ::={ telProdNpMonProtDLSwTCPConfigEntry 2 }

    telProdNpMonProtDLSwTCPConfIpAddr OBJECT-TYPE
        SYNTAX IpAddress
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Neighbor IP Address."
        ::={ telProdNpMonProtDLSwTCPConfigEntry 3 }

    telProdNpMonProtDLSwTCPConfTxBufsize OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Transmit Buffer size. Possible values are within the interval
            (1024..65536), and the value will be 0 when it is
            a local connection-conversion."
        DEFVAL { 5120 }
        ::={ telProdNpMonProtDLSwTCPConfigEntry 4 }

    telProdNpMonProtDLSwTCPConfRxBufsize OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Receive Buffer size. Possible values are within the interval
            (1024..65536), and the value will be 0 when it is
            a local connection-conversion."
        DEFVAL { 5120 }
        ::={ telProdNpMonProtDLSwTCPConfigEntry 5 }

    telProdNpMonProtDLSwTCPConfMaxSegsize OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Maximum segment size. Possible values are within the interval
            (1024..65536), and the value will be 0 when it is
            a local connection-conversion."
        DEFVAL { 1024 }
        ::={ telProdNpMonProtDLSwTCPConfigEntry 6 }

    telProdNpMonProtDLSwTCPConfKeepAlive OBJECT-TYPE
        SYNTAX INTEGER {
            inactive(2),
            active(1),
            no_sense(0)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Indicates if we want the DLSw neighbor to send keepalive messages.
            Option default active. Possible values are: active (1), inactive (2)
            and no_sense (0), that indicates it is a local connection-conversion."
        DEFVAL { 1 }
        ::={ telProdNpMonProtDLSwTCPConfigEntry 7 }

    telProdNpMonProtDLSwTCPConfPriority OBJECT-TYPE
        SYNTAX INTEGER {
            high(3),
            medium(2),
            low(1)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Allows specifying neighbor priority through the values
            high (3), medium (2), low (1). DLSw protocol uses this
            parameter to determine which DLSw neighbor to choose
            when several can reach the destination station."
        DEFVAL { 2 }
        ::={ telProdNpMonProtDLSwTCPConfigEntry 8 }

    telProdNpMonProtDLSwTCPConfCnxMode OBJECT-TYPE
        SYNTAX INTEGER {
            promiscuous(5),
            passive(4),
            always(3),
            on_demand(2),
            default(1)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Allows specifying neighbor connection mode through the values
            promiscuous (5), passive (4), always (3), on demand (2), default (1).
            DLSw protocol uses this parameter to determine when connections must
            be connected to neigbors. Default value uses configured global value."
        DEFVAL { 1 }
        ::={ telProdNpMonProtDLSwTCPConfigEntry 9 }

    telProdNpMonProtDLSwTCPConfInacTime OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Allows specifying neighbor inactivity time without
            active circuits in order to disconnect the link.
            (0 = Disable feature)."
        DEFVAL { 0 }
        ::={ telProdNpMonProtDLSwTCPConfigEntry 10 }

    telProdNpMonProtDLSwTCPConfLowPriMax OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Allows specifying maximum number of active circuits.
            When the number of active circuits exceeds this parameter,
            the system will change the priority of this link to low value.
            (0 = Disable feature)."
        DEFVAL { 0 }
        ::={ telProdNpMonProtDLSwTCPConfigEntry 11 }

    telProdNpMonProtDLSwTCPConfLowPriMin OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Allows specifying minimum number of active circuits.
            When the number of active circuits is lower than this parameter,
            the system will change the priority of this link to configured value."
        DEFVAL { 0 }
        ::={ telProdNpMonProtDLSwTCPConfigEntry 12 }

    telProdNpMonProtDLSwTCPConfLowPriRetries OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Allows specifying maximum number of connection retries.
            When the number of unsuccessfull circuit connections exceeds this parameter,
            the system will change the priority of this link to low value.
            (0 = Disable feature)."
        DEFVAL { 0 }
        ::={ telProdNpMonProtDLSwTCPConfigEntry 13 }

    telProdNpMonProtDLSwTCPConfLowPriTimeout OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Allows specifying time to recover configured priority when
            the actual priority was fired by excess of number of unsuccessfull
             circuit connections. (0 = Priority will not changed)."
        DEFVAL { 0 }
        ::={ telProdNpMonProtDLSwTCPConfigEntry 14 }

    telProdNpMonProtDLSwTCPConfLowPriMaxPercent OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Allows specifying maximum percentage of active circuits.
            When the percentage of active circuits exceeds this parameter,
            the system will change the priority of this link to low value.
            (0 = Disable feature)."
        DEFVAL { 0 }
        ::={ telProdNpMonProtDLSwTCPConfigEntry 15 }

    telProdNpMonProtDLSwTCPConfLowPriMinPercent OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Allows specifying minimum percentage of active circuits.
            When the percentage of active circuits is lower than this parameter,
            the system will change the priority of this link to configured value."
        DEFVAL { 0 }
        ::={ telProdNpMonProtDLSwTCPConfigEntry 16 }


    --Tabla que contiene la versin, el nîmero de sesiones DLSw activas que
    --utilizan esta sesin TCP, y el nîmero de sesiones que alguna vez han
    --utilizado esta sesin.

    telProdNpMonProtDLSwTCPSessionsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TelProdNpMonProtDLSwTCPSessionsEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "Displays version, number of DLSw active sessions used by this
            TCP session, and the number of sessions that al least once have
            used this session."
        ::={ telProdNpMonProtDLSwTCPGroup 3 }

    telProdNpMonProtDLSwTCPSessionsEntry OBJECT-TYPE
        SYNTAX TelProdNpMonProtDLSwTCPSessionsEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "Each entry consists of an index, that will be the order number of
            the table entry, the group identifier, IP address of the TCP session,
            current state of the TCP session, version, number of DLSw sessions
            currently activated in the TCP session and the number of DLSw sessions
            used in this TCP session."
        INDEX { telProdNpMonProtDLSwTCPSessIndex }
        ::={ telProdNpMonProtDLSwTCPSessionsTable 1 }

    TelProdNpMonProtDLSwTCPSessionsEntry ::=
        SEQUENCE {
            telProdNpMonProtDLSwTCPSessIndex
                INTEGER,
            telProdNpMonProtDLSwTCPSessRole
                INTEGER,
            telProdNpMonProtDLSwTCPSessGroup
                INTEGER,
            telProdNpMonProtDLSwTCPSessIpAddr
                IpAddress,
            telProdNpMonProtDLSwTCPSessConnState
                INTEGER,
            telProdNpMonProtDLSwTCPSessVersion
                DisplayString,
            telProdNpMonProtDLSwTCPSessNumActSess
                INTEGER,
            telProdNpMonProtDLSwTCPSessNumCreates
                INTEGER
        }

    telProdNpMonProtDLSwTCPSessIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Table index: order number of the table entry."
        ::={ telProdNpMonProtDLSwTCPSessionsEntry 1 }

     telProdNpMonProtDLSwTCPSessRole OBJECT-TYPE
        SYNTAX INTEGER {
            peer-to-peer(3),
            server(2),
            client(1),
            undefined(0)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Group type: 1 for client, 2 for server and 3 for peer-to peer.
            A server forms a TCP connection with a client."
        ::={ telProdNpMonProtDLSwTCPSessionsEntry 2 }

    telProdNpMonProtDLSwTCPSessGroup OBJECT-TYPE
        SYNTAX INTEGER (0..69)
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Group identifier. If it is 0 the router does not belong to any
            group."
        ::={ telProdNpMonProtDLSwTCPSessionsEntry 3 }

    telProdNpMonProtDLSwTCPSessIpAddr OBJECT-TYPE
        SYNTAX IpAddress
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "IP address of the TCP session."
        ::={ telProdNpMonProtDLSwTCPSessionsEntry 4 }

    telProdNpMonProtDLSwTCPSessConnState OBJECT-TYPE
        SYNTAX INTEGER {
            reconnect_wait(9),
            closing(8),
            established(7),
            capex_pending(6),
            wr_est_rd_pend(5),
            pending_r_w(4),
            rd_est_wr_pend(3),
            down(2),
            capex_failed(1)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Current state of the TCP session."
        ::={ telProdNpMonProtDLSwTCPSessionsEntry 5 }

    telProdNpMonProtDLSwTCPSessVersion OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Version name."
        ::={ telProdNpMonProtDLSwTCPSessionsEntry 6 }

    telProdNpMonProtDLSwTCPSessNumActSess OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Number of DLSw sessions currently active in the TCP session."
        ::={ telProdNpMonProtDLSwTCPSessionsEntry 7 }

