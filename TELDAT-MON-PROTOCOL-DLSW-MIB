TELDAT-MON-PROTOCOL-DLSw-MIB DEFINITIONS ::= BEGIN

-- Tipos importados

        IMPORTS
            IpAddress, Counter
                FROM RFC1155-SMI

            OBJECT-TYPE
                FROM RFC-1212

            DisplayString
                FROM RFC1213-MIB

            telProdNpMonProtocols
                FROM TELDAT-SW-STRUCTURE-MIB;


    telProdNpMonProtDLSw                OBJECT IDENTIFIER ::= { telProdNpMonProtocols 7 }


    -- Grupo telProdNpMonProtDLSw

    telProdNpMonProtDLSwGlobalGroup     OBJECT IDENTIFIER ::= { telProdNpMonProtDLSw 1 }


    -- Grupo que engloba parametros generales.

    telProdNpMonProtDLSwGloParamGroup   OBJECT IDENTIFIER ::= { telProdNpMonProtDLSwGlobalGroup 1 }

    telProdNpMonProtDLSwGloParEnabled OBJECT-TYPE
        SYNTAX INTEGER {
            inactive(2),
            active(1)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "DLSw protocol status, active (1) or inactive (2)."
        DEFVAL { 1 }
        ::={ telProdNpMonProtDLSwGloParamGroup 1 }

    telProdNpMonProtDLSwGloParLLC2SendDisc OBJECT-TYPE
        SYNTAX INTEGER {
            inactive(2),
            active(1)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "It represents the router prevention status to finish a LLC2 connection
            from the TCP connection loss. Possible values: active (1) or inactive (2)."
        DEFVAL { 1 }
        ::={ telProdNpMonProtDLSwGloParamGroup 2 }

    telProdNpMonProtDLSwGloParAutoTCP OBJECT-TYPE
        SYNTAX INTEGER {
            passive(3),
            on-demand(2),
            always(1)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Enable/Disable the TCP stations automatic recovery when a session is
            interrupted, and when the router is being initialized. Possible values:
            active (1), it means to always connect; inactive (2), it means to
            connect when it is needed or on demand; passive (3), it means to
            no try connect."
        DEFVAL { 1 }
        ::={ telProdNpMonProtDLSwGloParamGroup 3 }

    telProdNpMonProtDLSwGloParSRBSegNum OBJECT-TYPE
        SYNTAX INTEGER (0..4095)
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "SRB Segment that identifies to DLSw in the RIF."
        DEFVAL { 0 }
        ::={ telProdNpMonProtDLSwGloParamGroup 4 }

    telProdNpMonProtDLSwGloParMapCacheSize OBJECT-TYPE
        SYNTAX INTEGER (4..65535)
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Cache size of association between MAC and IP addresses, to reduce
            exploration traffic."
        DEFVAL { 128 }
        ::={ telProdNpMonProtDLSwGloParamGroup 5 }

    telProdNpMonProtDLSwGloParMaxDLSwSessions OBJECT-TYPE
        SYNTAX INTEGER (1..60000)
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Maximum number of DLSw sessions that router is able to support."
        DEFVAL { 1000 }
        ::={ telProdNpMonProtDLSwGloParamGroup 6 }

    telProdNpMonProtDLSwGloParPromisc OBJECT-TYPE
        SYNTAX INTEGER {
            disabled(2),
            enabled(1)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Enable/Disable Promiscuous mode. This mode permits accept
            connection form any neighbor. Possible values:
            enabled (1), it means mode enabled; disabled (2), it means mode disabled."
        DEFVAL { 2 }
        ::={ telProdNpMonProtDLSwGloParamGroup 7 }


    --Subgrupo del global que informa sobre la memoria.

    telProdNpMonProtDLSwGloMemoryGroup OBJECT IDENTIFIER ::= { telProdNpMonProtDLSwGlobalGroup 2 }

    telProdNpMonProtDLSwGloMemDLSwGloAllot OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Maximum amount of memory that DLSw has allowed using. The minimum
            value ir 26624. If a set try to fix this variable to a less value,
            it will take its minimum value (26624)."
        DEFVAL { 141312 }
        ::={ telProdNpMonProtDLSwGloMemoryGroup 1 }

    telProdNpMonProtDLSwGloMemLLCSessAllot OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Maximum amount of memory that DLSw has allowed using in each LLC
            session. It must be zero or a positive integer."
        DEFVAL { 8192 }
        ::={ telProdNpMonProtDLSwGloMemoryGroup 2 }

    telProdNpMonProtDLSwGloMemSDLCSessAllot OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Maximum amount of memory that DLSw has allowed using in each
            SDLC/QLLC session. It must be zero or a positive integer."
            DEFVAL { 4096 }
        ::={ telProdNpMonProtDLSwGloMemoryGroup 3 }

    telProdNpMonProtDLSwGloMemNetBIOSUIFraAllot OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Number of bytes that router assigns as buffer for NetBIOS UI frames. It
            must be zero or a positive integer."
        DEFVAL { 40960 }
        ::={ telProdNpMonProtDLSwGloMemoryGroup 4 }


    --Subgrupo del global que informa sobre los temporizadores.

    telProdNpMonProtDLSwGloTimersGroup OBJECT IDENTIFIER ::= { telProdNpMonProtDLSwGlobalGroup 3 }

    telProdNpMonProtDLSwGloTimDatabaseAge OBJECT-TYPE
        SYNTAX INTEGER (1..10000)
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Maximum amount of time that database entries stay active."
        DEFVAL { 1200 }
        ::={ telProdNpMonProtDLSwGloTimersGroup 1 }

    telProdNpMonProtDLSwGloTimMaxICanReach OBJECT-TYPE
        SYNTAX INTEGER (1..1000)
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Wait time for an answer to a CanYouReach before giving up."
        DEFVAL { 20 }
        ::={ telProdNpMonProtDLSwGloTimersGroup 2 }

    telProdNpMonProtDLSwGloTimLLCTestResp OBJECT-TYPE
        SYNTAX INTEGER (1..1000)
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Maximum amount of time (in seconds) that router waits for a LLC test
            answer before relaying a LLC test frame."
        DEFVAL { 15 }
        ::={ telProdNpMonProtDLSwGloTimersGroup 3 }

    telProdNpMonProtDLSwGloTimSDLCTestResp OBJECT-TYPE
        SYNTAX INTEGER (1..1000)
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Maximum amount of time (in seconds) that router waits for a SDLC test
            answer before relaying a SDLC test frame."
        DEFVAL { 15 }
        ::={ telProdNpMonProtDLSwGloTimersGroup 4 }

    telProdNpMonProtDLSwGloTimJoinGroupInterv OBJECT-TYPE
        SYNTAX INTEGER (1..60000)
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Time (in seconds) between DLSw group advertisements transmission."
        DEFVAL { 900 }
        ::={ telProdNpMonProtDLSwGloTimersGroup 5 }

    telProdNpMonProtDLSwGloTimNeighPriority OBJECT-TYPE
        SYNTAX INTEGER (10..50)
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Wait time (in decimas of second) during the exploration before
            selecting a neighbor."
        DEFVAL { 20 }
        ::={ telProdNpMonProtDLSwGloTimersGroup 6 }

    --Tabla que contiene parﬂmetros relacionados con la Cache.

    telProdNpMonProtDLSwCacheTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TelProdNpMonProtDLSwCacheEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "It lists DLSw MAC addresses cache entries. This cache contains a
            database with the most recent conversions of IP neighbors to
            MAC addresses."
        ::={ telProdNpMonProtDLSw 2 }

    telProdNpMonProtDLSwCacheEntry OBJECT-TYPE
        SYNTAX TelProdNpMonProtDLSwCacheEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "It provides the MAC address, the life time (in seconds) within cache,
            the neighbor IP address and the greater size of frame, in
            addition to the index, that is the order number of the entry in
            the table."
        INDEX { telProdNpMonProtDLSwCacheIndex }
        ::={ telProdNpMonProtDLSwCacheTable 1 }

    TelProdNpMonProtDLSwCacheEntry ::=
        SEQUENCE {
            telProdNpMonProtDLSwCacheIndex
                INTEGER,
            telProdNpMonProtDLSwCacheMACAddress
                DisplayString,
            telProdNpMonProtDLSwCacheSecsToLive
                INTEGER,
            telProdNpMonProtDLSwCacheIpAddress
                IpAddress,
            telProdNpMonProtDLSwCacheLargestFra
                INTEGER
        }

    telProdNpMonProtDLSwCacheIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Entry order number. It is the table index."
        ::={ telProdNpMonProtDLSwCacheEntry 1 }

    telProdNpMonProtDLSwCacheMACAddress OBJECT-TYPE
        SYNTAX DisplayString (SIZE(0..17))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "MAC Address."
        ::={ telProdNpMonProtDLSwCacheEntry 2 }

    telProdNpMonProtDLSwCacheSecsToLive OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "LIfe time (in seconds) within cache."
        ::={ telProdNpMonProtDLSwCacheEntry 3 }

    telProdNpMonProtDLSwCacheIpAddress OBJECT-TYPE
        SYNTAX IpAddress
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Neighbor IP address."
        ::={ telProdNpMonProtDLSwCacheEntry 4 }

    telProdNpMonProtDLSwCacheLargestFra OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Frame of greater size."
        ::={ telProdNpMonProtDLSwCacheEntry 5 }


    --Grupo que informa sobre el reparto de memoria.

    telProdNpMonProtDLSwMemoryGroup OBJECT IDENTIFIER ::= { telProdNpMonProtDLSw 3 }


    --Subgrupo de parﬂmetros globales sobre el uso de memoria.

    telProdNpMonProtDLSwMemGlobalGroup OBJECT IDENTIFIER ::= { telProdNpMonProtDLSwMemoryGroup 1 }

    telProdNpMonProtDLSwMemGloTotBytesReq OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Total number of requested bytes."
        DEFVAL { 141312 }
        ::={ telProdNpMonProtDLSwMemGlobalGroup 1 }

    telProdNpMonProtDLSwMemGloRxPoolGranted OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Number of bytes assigned to reception global pool."
        ::={ telProdNpMonProtDLSwMemGlobalGroup 2 }

    telProdNpMonProtDLSwMemGloRxBytesUsed OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Number of bytes received at present."
        ::={ telProdNpMonProtDLSwMemGlobalGroup 3 }

    telProdNpMonProtDLSwMemGloTxPoolGranted OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Number of bytes assigned to transmission global pool."
        ::={ telProdNpMonProtDLSwMemGlobalGroup 4 }

    telProdNpMonProtDLSwMemGloTxBytesUsed OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Number of bytes transmitted at present."
        ::={ telProdNpMonProtDLSwMemGlobalGroup 5 }

    telProdNpMonProtDLSwMemGloNetBIOSTotPool OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Number of bytes reserved for NetBIOS UI frames pool. If NetBIOS is
            disabled, it will be zero by default."
        DEFVAL { 0 }
        ::={ telProdNpMonProtDLSwMemGlobalGroup 6 }

    telProdNpMonProtDLSwMemGloNetBIOSBytesUsed OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Number of bytes of the Netbios UI frames pool in use at present. If
            NetBIOS is disabled, it will be zero by default."
        DEFVAL { 0 }
        ::={ telProdNpMonProtDLSwMemGlobalGroup 7 }

    --Subgrupo de memoria para las sesiones (es una tabla).

    telProdNpMonProtDLSwMemSessTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TelProdNpMonProtDLSwMemSessEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "It lists all the existing DLSw sessions and the amount of memory used
            by each one."
        ::={ telProdNpMonProtDLSwMemoryGroup 2 }

    telProdNpMonProtDLSwMemSessEntry OBJECT-TYPE
        SYNTAX TelProdNpMonProtDLSwMemSessEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "Each entry consists of the index with that the table is crossed over,
            that is the position of the entry in it, the session identifier, MAC and
            SAP source, MAC and SAP end, assigned initial amount, amount of
            free memory at the present moment, congestion status and total number
            of frames glued in sdlcim or sdlc to be transmitted by a specific
            circuit."
        INDEX { telProdNpMonProtDLSwMemSessIndex }
        ::={ telProdNpMonProtDLSwMemSessTable 1 }

    TelProdNpMonProtDLSwMemSessEntry ::=
        SEQUENCE{
            telProdNpMonProtDLSwMemSessIndex
                INTEGER,
            telProdNpMonProtDLSwMemSessId
                INTEGER,
            telProdNpMonProtDLSwMemSessSourMAC
                DisplayString,
            telProdNpMonProtDLSwMemSessSourSAP
                INTEGER,
            telProdNpMonProtDLSwMemSessDestMAC
                DisplayString,
            telProdNpMonProtDLSwMemSessDestSAP
                INTEGER,
            telProdNpMonProtDLSwMemSessInitialAlloc
                INTEGER,
            telProdNpMonProtDLSwMemSessCurrFree
                INTEGER,
            telProdNpMonProtDLSwMemSessCongState
                INTEGER,
            telProdNpMonProtDLSwMemSessDLCTxQueu
                Counter
        }

    telProdNpMonProtDLSwMemSessIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Order Number of the entry (corresponding to a session) in the table.
            It is the table index."
        ::={ telProdNpMonProtDLSwMemSessEntry 1 }

    telProdNpMonProtDLSwMemSessId OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Session identifier."
        ::={ telProdNpMonProtDLSwMemSessEntry 2 }

    telProdNpMonProtDLSwMemSessSourMAC OBJECT-TYPE
        SYNTAX DisplayString (SIZE(0..17))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Local MAC address. If session is SDLC or QLLC, its value will be 'SDLC'
            or 'QLLC' respectively."
        ::={ telProdNpMonProtDLSwMemSessEntry 3 }

    telProdNpMonProtDLSwMemSessSourSAP OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Local SAP number."
        ::={ telProdNpMonProtDLSwMemSessEntry 4 }

    telProdNpMonProtDLSwMemSessDestMAC OBJECT-TYPE
        SYNTAX DisplayString (SIZE(0..17))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Remote MAC address."
        ::={ telProdNpMonProtDLSwMemSessEntry 5 }

    telProdNpMonProtDLSwMemSessDestSAP OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Remote MAC number."
        ::={ telProdNpMonProtDLSwMemSessEntry 6 }

    telProdNpMonProtDLSwMemSessInitialAlloc OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Initial amount of assigned memory. If it were not assigned, the value
            of this variable will be 0."
        ::={ telProdNpMonProtDLSwMemSessEntry 7 }

    telProdNpMonProtDLSwMemSessCurrFree OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Amount of free memory at the moment. If it were not assigned
            initially, the value of this variable will be 0."
        ::={ telProdNpMonProtDLSwMemSessEntry 8 }

    telProdNpMonProtDLSwMemSessCongState OBJECT-TYPE
        SYNTAX INTEGER {
            ses_gbl(4),
            global(3),
            session(2),
            ready(1),
            none(0)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Session current status. Possible values are: ready(1), session(2),
            global(3) and ses_gbl(4). If memory were not assigned initially, the
            value of this variable will be 0 (none)."
        ::={ telProdNpMonProtDLSwMemSessEntry 9 }

    telProdNpMonProtDLSwMemSessDLCTxQueu OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Total number of frames glued in sdlcim or sdlc to be transmitted by a
            specific circuit."
        ::={ telProdNpMonProtDLSwMemSessEntry 10 }


    --Grupo de parﬂmetros relacionados con las sesiones activas.

    telProdNpMonProtDLSwSessionsGroup OBJECT IDENTIFIER  ::= { telProdNpMonProtDLSw 4 }


    --Tabla de las sesiones activas. Una entrada por sesin.

    telProdNpMonProtDLSwActSessTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TelProdNpMonProtDLSwActSessEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "It shows information of active DLS sessions at that moment, including
            MAC and SAP source, MAC and SAP end, session status, flags, remote IP
            address, and ID (identifier)."
        ::={ telProdNpMonProtDLSwSessionsGroup 1 }

    telProdNpMonProtDLSwActSessEntry OBJECT-TYPE
        SYNTAX TelProdNpMonProtDLSwActSessEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "Each entry consists of the index with that the table is crossed over,
            that is the position of the entry in it, MAC and SAP source, MAC and SAP
            end, session current status, flags, remote IP address and session
            identifier."
        INDEX { telProdNpMonProtDLSwActSessIndex }
        ::={ telProdNpMonProtDLSwActSessTable 1 }

    TelProdNpMonProtDLSwActSessEntry ::=
        SEQUENCE {
            telProdNpMonProtDLSwActSessIndex
                INTEGER,
            telProdNpMonProtDLSwActSessLocalMAC
                DisplayString,
            telProdNpMonProtDLSwActSessLocalSAP
                INTEGER,
            telProdNpMonProtDLSwActSessRemMAC
                DisplayString,
            telProdNpMonProtDLSwActSessRemSAP
                INTEGER,
            telProdNpMonProtDLSwActSessState
                INTEGER,
            telProdNpMonProtDLSwActSessFlags
                DisplayString,
            telProdNpMonProtDLSwActSessRemIPAddr
                IpAddress,
            telProdNpMonProtDLSwActSessId
                INTEGER
        }

    telProdNpMonProtDLSwActSessIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Order Number of the entry (corresponding to an active session) in the
            table. It is the table index."
        ::={ telProdNpMonProtDLSwActSessEntry 1 }

    telProdNpMonProtDLSwActSessLocalMAC OBJECT-TYPE
        SYNTAX DisplayString (SIZE(0..17))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Source MAC address of the session. If session is SDLC or QLLC, its
            value will be 'SDLC' or 'QLLC' respectively, instead of the MAC address."
        ::={ telProdNpMonProtDLSwActSessEntry 2 }

    telProdNpMonProtDLSwActSessLocalSAP OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "SAP opened in the source for this session."
        ::={ telProdNpMonProtDLSwActSessEntry 3 }

    telProdNpMonProtDLSwActSessRemMAC OBJECT-TYPE
        SYNTAX DisplayString (SIZE(0..17))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Remote MAC address of the session."
        ::={ telProdNpMonProtDLSwActSessEntry 4 }

    telProdNpMonProtDLSwActSessRemSAP OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "SAP opened in the end for this session."
        ::={ telProdNpMonProtDLSwActSessEntry 5 }

    telProdNpMonProtDLSwActSessState OBJECT-TYPE
        SYNTAX INTEGER {
            hlt_noak(14),
            circ_strt(13),
            wait_noack(12),
            rest_pend(11),
            halt_pending(10),
            disc_pend(9),
            connect_state(8),
            cont_pend(7),
            conn_pend(6),
            cir_rstrt(5),
            circ_est(4),
            circ_pend(3),
            rslv_pend(2),
            disconnected(1)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Session current status:
            DISCONNECTED  - Initial status hasn't a circuit or an established
                            connection.
            RSLV_PEND     - The DLSw target is waiting for a SSP_started indication
                            after a SSP_start request.
            CIRC_PEND     - The DLSw target is waiting for a SSP_REACHACK answer
                            to a SSP_ICANREACH message.
            CIRC_EST      - Circuit has been established end to end.
            CIR_RSTRT     - The DLSw that originated reset is waiting for the reboot
            CONN_PEND     - The DLSw source is waiting for a SSP_CONTACTED answer
                            to a SSP_CONTACT message.
            CONT_PEND     - The DLSw end is waiting for a SSP_CONTACTED confirmation
                            to a SSP_CONTACT message.
            CONNECT_STATE -
            DISC_PEND     - The DLSw that originated the disconnection is waiting
                            for a SSP_HALTED answer to a SSP_HALT message.
            HALT_PENDING  - The DLSw is waiting for a SSP_HALTED indication after a
                            SSP_HALT request.
            RESTART_PEND  - The remote DLSw is waiting for a SSP_HALTED indication
                            after a SSP_HALT request.
            WAIT_NOACK    -
            CIRC_STRT     -
            HLT_NOACK     - "
        ::={ telProdNpMonProtDLSwActSessEntry 6 }

    telProdNpMonProtDLSwActSessFlags OBJECT-TYPE
        SYNTAX DisplayString(SIZE(6))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "it is a string of six characters, so that each one that is present
            indicates one of the following situations:
            (A) PENDING MSG CONTACT
            (B) PENDING SAP RESOLUTION
            (C) AWAITED CONGESTION FINAL
            (D) OCCUPIED TCP
            (E) PENDING DISHABILITATION
            (F) INACTIVE CIRCUIT
            If the corresponding situation is not occurring, a blank space is
            introduced in the position where the character would have to appear."
        ::={ telProdNpMonProtDLSwActSessEntry 7 }

    telProdNpMonProtDLSwActSessRemIPAddr OBJECT-TYPE
        SYNTAX IpAddress
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "IP address of the remote DLS neighbor. If it is a local connection,
            or if the status is DISCONNECTED, the IP address will be 0.0.0.0."
        ::={ telProdNpMonProtDLSwActSessEntry 8 }

    telProdNpMonProtDLSwActSessId OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Number used for identifying the session."
        ::={ telProdNpMonProtDLSwActSessEntry 9 }


    --Tabla de parﬂmetros de informacin detallada acerca de las sesiones
    --activas. Es un subgrupo del grupo de sesiones activas.

    telProdNpMonProtDLSwSessDetailTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TelProdNpMonProtDLSwSessDetailEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "It shows detailed information about DLS session."
        ::={ telProdNpMonProtDLSwSessionsGroup 2 }

    telProdNpMonProtDLSwSessDetailEntry OBJECT-TYPE
        SYNTAX TelProdNpMonProtDLSwSessDetailEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "Each entry consists of an index, which is the order number of the entry
            in the table (each entry is an active session), the session identifier,
            the function of the router in the connection (if it is a sender or
            a receiver), transmitted and received XIDs from the remote
            DLSw neighbor, transmitted and received datagrams, transmitted and
            received information frames, information included in the RIF of the
            TEST LLC frame."
        INDEX { telProdNpMonProtDLSwSessDetIndex }
        ::={ telProdNpMonProtDLSwSessDetailTable 1 }

    TelProdNpMonProtDLSwSessDetailEntry ::=
        SEQUENCE {
            telProdNpMonProtDLSwSessDetIndex
                INTEGER,
            telProdNpMonProtDLSwSessDetId
                INTEGER,
            telProdNpMonProtDLSwSessDetPersonality
                INTEGER,
            telProdNpMonProtDLSwSessDetXIDsSent
                Counter,
            telProdNpMonProtDLSwSessDetXIDsRcvd
                Counter,
            telProdNpMonProtDLSwSessDetDatagSent
                Counter,
            telProdNpMonProtDLSwSessDetDatagRcvd
                Counter,
            telProdNpMonProtDLSwSessDetInfoFraSent
                Counter,
            telProdNpMonProtDLSwSessDetInfoFraRcvd
                Counter,
            telProdNpMonProtDLSwSessDetRIF
                DisplayString,
            telProdNpMonProtDLSwSessDetLocalCIDCorr
                INTEGER,
            telProdNpMonProtDLSwSessDetLocalCIDPortId
                INTEGER,
            telProdNpMonProtDLSwSessDetRemCIDCorr
                INTEGER,
            telProdNpMonProtDLSwSessDetRemCIDPortId
                INTEGER,
            telProdNpMonProtDLSwSessDetPriority
                INTEGER
        }

    telProdNpMonProtDLSwSessDetIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Order number of the entry (corresponding with an active session) in
            the table. It is the table index."
        ::={ telProdNpMonProtDLSwSessDetailEntry 1 }

    telProdNpMonProtDLSwSessDetId OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
