        STATUS mandatory
        DESCRIPTION
            "Allows specifying minimum percentage of active circuits.
            When the percentage of active circuits is lower than this parameter,
            the system will change the priority of this link to configured value."
        DEFVAL { 0 }
        ::={ telProdNpMonProtDLSwTCPPromisGroup 12 }


    -- Tabla que engloba las MAC Address List anunciadas.

    telProdNpMonProtDLSwTCPCapMacListTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TelProdNpMonProtDLSwTCPCapMacListEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "Displays MAC Address Lists received form neighbors."
        ::={ telProdNpMonProtDLSwTCPGroup 6 }

    telProdNpMonProtDLSwTCPCapMacListEntry OBJECT-TYPE
        SYNTAX TelProdNpMonProtDLSwTCPCapMacListEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "Each entry consists of an index, that will be the order number of
            the table entry, DLSw neighbor IP Address, MAC address value
            and MAC address mask received from neighbors."
        INDEX { telProdNpMonProtDLSwTCPCapMacListIndex }
        ::={ telProdNpMonProtDLSwTCPCapMacListTable 1 }

    TelProdNpMonProtDLSwTCPCapMacListEntry ::=
        SEQUENCE {
            telProdNpMonProtDLSwTCPCapMacListIndex
                INTEGER,

            telProdNpMonProtDLSwTCPCapMacListIpAddr
                IpAddress,

            telProdNpMonProtDLSwTCPCapMacListMAC
                DisplayString,

            telProdNpMonProtDLSwTCPCapMacListMask
                DisplayString
        }

    telProdNpMonProtDLSwTCPCapMacListIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Table index: entry order number, corresponding to a TCP
            session and entry order in list."
        ::={ telProdNpMonProtDLSwTCPCapMacListEntry 1 }

    telProdNpMonProtDLSwTCPCapMacListIpAddr OBJECT-TYPE
        SYNTAX IpAddress
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "DLSw neighbor IP Address."
        ::={ telProdNpMonProtDLSwTCPCapMacListEntry 2 }

    telProdNpMonProtDLSwTCPCapMacListMAC OBJECT-TYPE
        SYNTAX DisplayString (SIZE(0..17))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "MAC Address value."
        ::={ telProdNpMonProtDLSwTCPCapMacListEntry 3 }

    telProdNpMonProtDLSwTCPCapMacListMask OBJECT-TYPE
        SYNTAX DisplayString (SIZE(0..17))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "MAC Address mask."
        ::={ telProdNpMonProtDLSwTCPCapMacListEntry 4 }


    -- Tabla que engloba las NetBIOS Name List anunciadas.

    telProdNpMonProtDLSwTCPCapNbListTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TelProdNpMonProtDLSwTCPCapNbListEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "Displays NetBIOS Name Lists received form neighbors."
        ::={ telProdNpMonProtDLSwTCPGroup 7 }

    telProdNpMonProtDLSwTCPCapNbListEntry OBJECT-TYPE
        SYNTAX TelProdNpMonProtDLSwTCPCapNbListEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "Each entry consists of an index, that will be the order number of
            the table entry, DLSw neighbor IP Address, NetBIOS Name Type
            and NetBIOS Name received from neighbors."
        INDEX { telProdNpMonProtDLSwTCPCapNbListIndex }
        ::={ telProdNpMonProtDLSwTCPCapNbListTable 1 }

    TelProdNpMonProtDLSwTCPCapNbListEntry ::=
        SEQUENCE {
            telProdNpMonProtDLSwTCPCapNbListIndex
                INTEGER,

            telProdNpMonProtDLSwTCPCapNbListIpAddr
                IpAddress,

            telProdNpMonProtDLSwTCPCapNbListType
                INTEGER,

            telProdNpMonProtDLSwTCPCapNbListName
                DisplayString
        }

    telProdNpMonProtDLSwTCPCapNbListIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Table index: entry order number, corresponding to a TCP
            session and entry order in list."
        ::={ telProdNpMonProtDLSwTCPCapNbListEntry 1 }

    telProdNpMonProtDLSwTCPCapNbListIpAddr OBJECT-TYPE
        SYNTAX IpAddress
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "DLSw neighbor IP Address."
        ::={ telProdNpMonProtDLSwTCPCapNbListEntry 2 }

    telProdNpMonProtDLSwTCPCapNbListType OBJECT-TYPE
        SYNTAX INTEGER {
            individual(0),
            group(1)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "NetBIOS Name Type. (Individual/Group)"
        ::={ telProdNpMonProtDLSwTCPCapNbListEntry 3 }

    telProdNpMonProtDLSwTCPCapNbListName OBJECT-TYPE
        SYNTAX DisplayString (SIZE(0..40))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "NetBIOS Name."
        ::={ telProdNpMonProtDLSwTCPCapNbListEntry 4 }


    --Tabla que contiene los parﬂmetros referentes a todos los grupos
    --configurados a los que el router pertenezca.

    telProdNpMonProtDLSwGroupsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TelProdNpMonProtDLSwGroupsEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "Group configuration."
        ::={ telProdNpMonProtDLSw 11 }

    telProdNpMonProtDLSwGroupsEntry OBJECT-TYPE
        SYNTAX TelProdNpMonProtDLSwGroupsEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "Each entry consists of an index, that will be the order number of
            the table entry, a variable to join a router to a specified group or
            remove the  router from it, group number, group type (server, client
            or peer to peer), transmit and receive buffer size, maximum size
            of the TCP segment, variable that shows whether the DLSw neighbor
            will send link keepalive messages or not, and another which specifies
            the neighbor priority."
        INDEX { telProdNpMonProtDLSwGroupsIndex }
        ::={ telProdNpMonProtDLSwGroupsTable 1 }

    TelProdNpMonProtDLSwGroupsEntry ::=
        SEQUENCE {
            telProdNpMonProtDLSwGroupsIndex
                INTEGER,
            telProdNpMonProtDLSwGroupsCommand
                INTEGER,
            telProdNpMonProtDLSwGroupsRole
                INTEGER,
            telProdNpMonProtDLSwGroupsTxBufsize
                INTEGER,
            telProdNpMonProtDLSwGroupsRxBufsize
                INTEGER,
            telProdNpMonProtDLSwGroupsMaxSegSize
                INTEGER,
            telProdNpMonProtDLSwGroupsKeepAlive
                INTEGER,
            telProdNpMonProtDLSwGroupsPriority
                INTEGER,
            telProdNpMonProtDLSwGroupsCnxMode
                INTEGER,
            telProdNpMonProtDLSwGroupsInacTime
                INTEGER,
            telProdNpMonProtDLSwGroupsLowPriMax
                INTEGER,
            telProdNpMonProtDLSwGroupsLowPriMin
                INTEGER,
            telProdNpMonProtDLSwGroupsLowPriRetries
                INTEGER,
            telProdNpMonProtDLSwGroupsLowPriTimeout
                INTEGER,
            telProdNpMonProtDLSwGroupsLowPriMaxPercent
                INTEGER,
            telProdNpMonProtDLSwGroupsLowPriMinPercent
                INTEGER
        }

    telProdNpMonProtDLSwGroupsIndex OBJECT-TYPE
        SYNTAX INTEGER (0..69)
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Table index: group number."
        ::={ telProdNpMonProtDLSwGroupsEntry 1 }

    telProdNpMonProtDLSwGroupsCommand OBJECT-TYPE
        SYNTAX INTEGER{
            error(4),
            leave_group(3),
            join_group(2),
            ok(1)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Use the JOIN-GROUP command to allow DLSw neighbors to find
            and to create TCP sessions with each other dynamically. This
            eliminates the need to define TCP neighbors with the ADD TCP
            command. In a get operation this variable can be 1 (ok) if
            the last JOIN-GROUP or LEAVE-GROUP operation succeeded and
            4 (error) if failed. In a set operation admits 1 (join_group) and 3
            (leave_group), that initiates the corresponding operations"
        ::={ telProdNpMonProtDLSwGroupsEntry 2 }

    telProdNpMonProtDLSwGroupsRole OBJECT-TYPE
        SYNTAX INTEGER {
            peer-to-peer(3),
            server(2),
            client(1)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "The type of group that you want to join the router: 1 for client,
            2 for server, and 3 for peer-to-peer. A server forms a TCP
            connection with a client."
        ::={ telProdNpMonProtDLSwGroupsEntry 3 }

    telProdNpMonProtDLSwGroupsTxBufsize OBJECT-TYPE
        SYNTAX INTEGER (1024..65536)
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "The size of the packet transmit buffer."
        DEFVAL { 5120 }
        ::={ telProdNpMonProtDLSwGroupsEntry 4 }

    telProdNpMonProtDLSwGroupsRxBufsize OBJECT-TYPE
        SYNTAX INTEGER (1024..65536)
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "The size of the packet receive buffer."
        DEFVAL { 5120 }
        ::={ telProdNpMonProtDLSwGroupsEntry 5 }

    telProdNpMonProtDLSwGroupsMaxSegSize OBJECT-TYPE
        SYNTAX INTEGER (64..16384)
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "The maximum size of the TCP segment."
        DEFVAL { 1024 }
        ::={ telProdNpMonProtDLSwGroupsEntry 6 }

    telProdNpMonProtDLSwGroupsKeepAlive OBJECT-TYPE
        SYNTAX INTEGER {
            inactive(2),
            active(1)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Indicates whether you want the DLSw neighbor to send
            link keepalive messages."
        DEFVAL { 1 }
        ::={ telProdNpMonProtDLSwGroupsEntry 7 }

    telProdNpMonProtDLSwGroupsPriority OBJECT-TYPE
        SYNTAX INTEGER {
            high(3),
            medium(2),
            low(1),
            none(0)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Specifies the neighbor priority as high (3), medium (2) or low (1).
            It should never be none (0). DLSw uses this parameter to determine
            which DLSw neighbor to choose when multiple neighbors can reach
            a target station."
        DEFVAL { 2 }
        ::={ telProdNpMonProtDLSwGroupsEntry 8 }

    telProdNpMonProtDLSwGroupsCnxMode OBJECT-TYPE
        SYNTAX INTEGER {
            promiscuous(5),
            passive(4),
            always(3),
            on_demand(2),
            default(1)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Allows specifying neighbor connection mode through the values
            promiscuous (5), passive (4), always (3), on demand (2), default (1).
            DLSw protocol uses this parameter to determine when connections must
            be connected to neigbors. Default value uses configured global value."
        DEFVAL { 1 }
        ::={ telProdNpMonProtDLSwGroupsEntry 9 }

    telProdNpMonProtDLSwGroupsInacTime OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Allows specifying neighbor inactivity time without
            active circuits in order to disconnect the link.
            (0 = Disable feature)."
        DEFVAL { 0 }
        ::={ telProdNpMonProtDLSwGroupsEntry 10 }

    telProdNpMonProtDLSwGroupsLowPriMax OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Allows specifying maximum number of active circuits.
            When the number of active circuits exceeds this parameter,
            the system will change the priority of this link to low value.
            (0 = Disable feature)."
        DEFVAL { 0 }
        ::={ telProdNpMonProtDLSwGroupsEntry 11 }

    telProdNpMonProtDLSwGroupsLowPriMin OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Allows specifying minimum number of active circuits.
            When the number of active circuits is lower than this parameter,
            the system will change the priority of this link to configured value."
        DEFVAL { 0 }
        ::={ telProdNpMonProtDLSwGroupsEntry 12 }

    telProdNpMonProtDLSwGroupsLowPriRetries OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Allows specifying maximum number of connection retries.
            When the number of unsuccessfull circuit connections exceeds this parameter,
            the system will change the priority of this link to low value.
            (0 = Disable feature)."
        DEFVAL { 0 }
        ::={ telProdNpMonProtDLSwGroupsEntry 13 }

    telProdNpMonProtDLSwGroupsLowPriTimeout OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Allows specifying time to recover configured priority when
            the actual priority was fired by excess of number of unsuccessfull
             circuit connections. (0 = Priority will not changed)."
        DEFVAL { 0 }
        ::={ telProdNpMonProtDLSwGroupsEntry 14 }

    telProdNpMonProtDLSwGroupsLowPriMaxPercent OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Allows specifying maximum percentage of active circuits.
            When the number of active circuits exceeds this parameter,
            the system will change the priority of this link to low value.
            (0 = Disable feature)."
        DEFVAL { 0 }
        ::={ telProdNpMonProtDLSwGroupsEntry 15 }

    telProdNpMonProtDLSwGroupsLowPriMinPercent OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Allows specifying minimum percentage of active circuits.
            When the percentage of active circuits is lower than this parameter,
            the system will change the priority of this link to configured value."
        DEFVAL { 0 }
        ::={ telProdNpMonProtDLSwGroupsEntry 16 }


    --Grupo BAN

    telProdNpMonProtDLSwBAN OBJECT IDENTIFIER ::= { telProdNpMonProtDLSw 12 }


    --Grupo NetBIOS sobre DLSw

    telProdNpMonProtDLSwNetBIOS OBJECT IDENTIFIER ::= { telProdNpMonProtDLSw 13 }


