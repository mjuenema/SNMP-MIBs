        ::={ telProdNpMonProtDLSwTCPCapabTable 1 }

    TelProdNpMonProtDLSwTCPCapabEntry ::=
        SEQUENCE {
            telProdNpMonProtDLSwTCPCapIndex
                INTEGER,
            telProdNpMonProtDLSwTCPCapIpAddr
                IpAddress,
            telProdNpMonProtDLSwTCPCapVendorID
                DisplayString,
            telProdNpMonProtDLSwTCPCapVenProVers
                DisplayString,
            telProdNpMonProtDLSwTCPCapIniPacWind
                INTEGER,
