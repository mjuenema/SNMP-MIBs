          ::= { hwLocalMirror 4 }
          

      hwLocalSlotMirrorEntry OBJECT-TYPE
          SYNTAX HwLocalSlotMirrorEntry
          MAX-ACCESS not-accessible
          STATUS current
          DESCRIPTION
              " The hwLocalFlowMirrorEntry lists local mirror characters. " 
          INDEX  { hwLocalSlotNo }    
          ::= { hwLocalSlotMirrorTable 1 }           

    HwLocalSlotMirrorEntry ::=
        SEQUENCE {
           hwLocalSlotNo                  
               Integer32,
           hwSlotObserveIndex            
               Integer32,
           hwLocalSlotMirrorRowStatus        
               RowStatus                                    
   }  
   
hwLocalSlotNo OBJECT-TYPE
      SYNTAX Integer32 (1..128)
      MAX-ACCESS not-accessible
      STATUS current
      DESCRIPTION
      "The value of this object identifies the slot ID."
           
      ::= { hwLocalSlotMirrorEntry 1 }


hwSlotObserveIndex OBJECT-TYPE
      SYNTAX Integer32 (1..128)
      MAX-ACCESS read-create
      STATUS current
      DESCRIPTION
      "The value of this object identifies the observing port index, ranging from 1 to 128."
           
      ::= { hwLocalSlotMirrorEntry 2 }

hwLocalSlotMirrorRowStatus OBJECT-TYPE
      SYNTAX RowStatus
      MAX-ACCESS read-create
      STATUS current
