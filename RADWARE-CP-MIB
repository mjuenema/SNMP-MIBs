rsCPRouterConnectionsTable OBJECT-TYPE
    SYNTAX SEQUENCE OF RsCPRouterConnectionEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
      "Table containing the list of routers to telnet."
    ::=  {rsCP 18}

rsCPRouterConnectionEntry OBJECT-TYPE
    SYNTAX RsCPRouterConnectionEntry
    ACCESS not-accessible
    STATUS mandatory
    INDEX { rsCPRouterConnectionRouterIPAddress }
    ::=  {rsCPRouterConnectionsTable 1}

RsCPRouterConnectionEntry ::= SEQUENCE {
  rsCPRouterConnectionRouterIPAddress	IpAddress,
  rsCPRouterConnectionUserName     	    DisplayString,
  rsCPRouterConnectionUserPassword   	DisplayString,
  rsCPRouterConnectionConfPassword      DisplayString,
  rsCPRouterConnectionProtocol          INTEGER,
  rsCPRouterConnectionCommunity	        DisplayString,--opt
  rsCPRouterConnectionPort              INTEGER,
  rsCPRouterConnectionEnableLevel       INTEGER,
  rsCPRouterConnectionTerminalMode      INTEGER,
  rsCPRouterConnectionLastSysUpTime     TimeTicks,--ronly
  rsCPRouterConnectionStatus            RowStatus
  }

rsCPRouterConnectionRouterIPAddress  OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "router's IP address."
    ::=  {rsCPRouterConnectionEntry 1}

rsCPRouterConnectionUserName  OBJECT-TYPE
    SYNTAX  DisplayString (SIZE(0..15))
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "user name for connection."
    ::=  {rsCPRouterConnectionEntry 2}

rsCPRouterConnectionUserPassword  OBJECT-TYPE
    SYNTAX  DisplayString (SIZE(1..15))
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "password for connection."
    ::=  {rsCPRouterConnectionEntry 3}

rsCPRouterConnectionConfPassword  OBJECT-TYPE
    SYNTAX  DisplayString (SIZE(1..16))
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "password for connection."
    ::=  {rsCPRouterConnectionEntry 4}

rsCPRouterConnectionProtocol  OBJECT-TYPE
    SYNTAX  INTEGER {telnet(1),ssh(2)}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "The protocol to communicate with the router."
    ::=  {rsCPRouterConnectionEntry 5}

rsCPRouterConnectionCommunity  OBJECT-TYPE
    SYNTAX  DisplayString (SIZE(0..20))
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "community password to use for this polled router."
    DEFVAL {"public"}
    ::=  {rsCPRouterConnectionEntry 6}

rsCPRouterConnectionPort  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "The used port for communicating with the router."
    ::=  {rsCPRouterConnectionEntry 7}

rsCPRouterConnectionEnableLevel  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "The level to use with the enable command - default 1000 means unused."
    ::=  {rsCPRouterConnectionEntry 8}

rsCPRouterConnectionTerminalMode  OBJECT-TYPE
    SYNTAX  INTEGER {charByCharMode(1), fullLineMode(2)}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "The telnet terminal mode - transmit a full line or char after char."
    ::=  {rsCPRouterConnectionEntry 9}

rsCPRouterConnectionLastSysUpTime OBJECT-TYPE
    SYNTAX  TimeTicks
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
       "The time (in hundredths of a second) as informed by the router since the
       network management portion of the router's system was last
       re-initialized."
    ::=  {rsCPRouterConnectionEntry 10}

rsCPRouterConnectionStatus OBJECT-TYPE
    SYNTAX RowStatus
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
       "This variable indicates the administrative status of this entry."
    ::=  {rsCPRouterConnectionEntry 11}



