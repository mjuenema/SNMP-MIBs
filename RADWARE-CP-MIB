rsCPCIDRsTable OBJECT-TYPE
    SYNTAX SEQUENCE OF RsCPCIDREntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
      "Table containing the list of CIDRs."
    ::=  {rsCP 1}

rsCPCIDREntry OBJECT-TYPE
    SYNTAX RsCPCIDREntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
       " The row definition for this table. The table is made of four fields:
	 1) The CIDR's subnet.
	 2) The mask for the CIDR.
     3) The static assignment to peer IP.
     4) The static assignment to router IP.
	 The table's key is made of fields 1+2.
     Fields 3+4 are 0.0.0.0 by default and should always both be zero or a real IP."
    INDEX { rsCPCIDRSubnet,
            rsCPCIDRMask }
    ::=  {rsCPCIDRsTable 1}

RsCPCIDREntry ::= SEQUENCE {
  rsCPCIDRSubnet	                IpAddress,
  rsCPCIDRMask  	                IpAddress,
  rsCPCIDRStaticNeighborIPAddress	IpAddress,--opt
  rsCPCIDRStaticRouterIPAddress     IpAddress,--opt
  rsCPCIDRAssignedNeighborIP        IpAddress,
  rsCPCIDRAssignedLocalRouterIP     IpAddress,
  rsCPCIDRPrefixListIndex           INTEGER,
  rsCPCIDRLastMinuteLoad            INTEGER,--ronly
  rsCPCIDRDescription               DisplayString,--opt
  rsCPCIDRStatsTimePeriod1          INTEGER,
  rsCPCIDRStatsTimePeriod2          INTEGER,
  rsCPCIDRStatsTimePeriod3          INTEGER,
  rsCPCIDRStatsTimePeriod4          INTEGER,
  rsCPCIDRStatsTimePeriod5          INTEGER,
  rsCPCIDRStatsTimePeriod6          INTEGER,
  rsCPCIDRStatus   	                RowStatus
  }

rsCPCIDRSubnet  OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The CIDR's subnet."
    ::=  {rsCPCIDREntry 1}

rsCPCIDRMask  OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The CIDR's mask represented as an IP."
    ::=  {rsCPCIDREntry 2}

rsCPCIDRStaticNeighborIPAddress  OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "The peer's IP Address the CIDR is statically assigned to."
    ::=  {rsCPCIDREntry 3}

rsCPCIDRStaticRouterIPAddress  OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "The router's IP Address the CIDR is statically assigned to."
    ::=  {rsCPCIDREntry 4}

rsCPCIDRAssignedNeighborIP  OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "The peer's IP Address the CIDR is statically assigned to."
    ::=  {rsCPCIDREntry 5}

rsCPCIDRAssignedLocalRouterIP  OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "The router's IP Address the CIDR is statically assigned to."
    ::=  {rsCPCIDREntry 6}

rsCPCIDRPrefixListIndex  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "The index in the access list."
    ::=  {rsCPCIDREntry 7}

rsCPCIDRLastMinuteLoad  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The last minute load of the CIDR."
    ::=  {rsCPCIDREntry 8}

rsCPCIDRDescription  OBJECT-TYPE
    SYNTAX  DisplayString (SIZE(0..19))
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "CIDR's description."
    ::=  {rsCPCIDREntry 9}

rsCPCIDRStatsTimePeriod1  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "Statistics for period."
    ::=  {rsCPCIDREntry 10}

rsCPCIDRStatsTimePeriod2  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "Statistics for period."
    ::=  {rsCPCIDREntry 11}

rsCPCIDRStatsTimePeriod3  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "Statistics for period."
    ::=  {rsCPCIDREntry 12}

rsCPCIDRStatsTimePeriod4  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "Statistics for period."
    ::=  {rsCPCIDREntry 13}

rsCPCIDRStatsTimePeriod5  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "Statistics for period."
    ::=  {rsCPCIDREntry 14}

rsCPCIDRStatsTimePeriod6  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "Statistics for period."
    ::=  {rsCPCIDREntry 15}

rsCPCIDRStatus OBJECT-TYPE
    SYNTAX RowStatus
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
       "This variable indicates the administrative status of this entry."
    ::=  {rsCPCIDREntry 16}



