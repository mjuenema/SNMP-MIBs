rsCPPriceTable OBJECT-TYPE
    SYNTAX SEQUENCE OF RsCPPriceEntry
    ACCESS not-accessible
    STATUS  mandatory
    DESCRIPTION
      "Table containing the  price levels for a given peer"
    ::=  {rsCP 31}

rsCPPriceEntry OBJECT-TYPE
    SYNTAX RsCPPriceEntry
    ACCESS not-accessible
    STATUS  mandatory
    DESCRIPTION
       " The row definition for this table. the table containing the  price levels for a given peer.
	 	 The table's key is made of the key of the peer(local router and neighbor IP) and the upper threshold of BW 
		 for that price level. "
   INDEX {rsCPPriceNeighborIPAddress, rsCPPriceRouterLocalIPAddress, rsCPPriceThreshold }
    ::=  {rsCPPriceTable 1}

RsCPPriceEntry ::= SEQUENCE {
  rsCPPriceNeighborIPAddress	IpAddress,
  rsCPPriceRouterLocalIPAddress	IpAddress,
  rsCPPriceThreshold			INTEGER,
  rsCPPriceMethod				INTEGER,
  rsCPPriceBWUnit				INTEGER,
  rsCPPricePrice				INTEGER,
  rsCPPriceStatus				INTEGER
  }

rsCPPriceNeighborIPAddress  OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The peer's IP address."
    ::=  {rsCPPriceEntry 1}

rsCPPriceRouterLocalIPAddress  OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The router's local IP."
    ::=  {rsCPPriceEntry 2}

rsCPPriceThreshold OBJECT-TYPE
    SYNTAX  INTEGER 
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The upper BW threshold of that price level in Kbps"
    ::=  {rsCPPriceEntry 3}

rsCPPriceMethod  OBJECT-TYPE
    SYNTAX INTEGER {
      absolute(1),
      perKbps(2)
    }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
     "Whether the price is in Absolute price units or price units per Kbps"
-- DEFVAL {2}
 ::=  {rsCPPriceEntry 4 }

rsCPPriceBWUnit OBJECT-TYPE
    SYNTAX  INTEGER {
      tenKbps(1),
      hundredKbps(2),
	  thousandKbps(3)
    } 
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      " For how many Kbps this price refer to "
-- DEFVAL {1}
    ::=  {rsCPPriceEntry 5}

rsCPPricePrice OBJECT-TYPE
    SYNTAX  INTEGER 
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "The price of this level"
    ::=  {rsCPPriceEntry 6}


rsCPPriceStatus OBJECT-TYPE
    SYNTAX INTEGER {
              active(1),
              notInService(2),
              notReady(3),
              createAndGo(4),
              createAndWait(5),
              destroy(6) }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
       "This variable indicates the administrative status of this entry."
    ::=  {rsCPPriceEntry 7}


