rsCPPrependInterval OBJECT-TYPE
    SYNTAX INTEGER(1..300)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "Determines the minimum time interval (in minutes) between changes 
       in the CIDRs distibution."
    DEFVAL  { 20 }
 ::=  {rsCP 4}




--;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
--;
--; Module : Peers
--; Date   : 14/06/01
--;
--;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

rsCPRedistributeInterval OBJECT-TYPE
    SYNTAX INTEGER(0..1000000)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "Indicates how long to wait with distribution change after a peer or interface status
       changed from up to down."
    DEFVAL  { 2 }
 ::=  {rsCP 5}



--;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
--;
--; Module : Peers
--; Date   : 14/06/01
--;
--;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

rsCPCriticalPeerLoadThreshold OBJECT-TYPE
    SYNTAX INTEGER(0..100)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "If reaching this bound - trigger reorganization of CIDRs.
       This value is kept as an integer - divide by 100 in code."
    DEFVAL  { 95 }
 ::=  {rsCP 6}



--;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
--;
--; Module : Peers
--; Date   : 14/06/01
--;
--;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

rsCPOptimalPeerLoad OBJECT-TYPE
    SYNTAX INTEGER(0..100)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "Above this bound the link is considered as too loaded.
       This value is kept as an integer - divide by 100 in code."
    DEFVAL  { 80 }
 ::=  {rsCP 7}



--;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
--;
--; Module : Peers
--; Date   : 14/06/01
--;
--;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

rsCPAdequateTotalPeakLoad OBJECT-TYPE
    SYNTAX INTEGER(101..1000)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "Time interval in which we calculate average load, 
	   later we remember the most loaded time interval as the pick load in the period.
       This value is kept as an integer - divide by 100 in code."
    DEFVAL  { 140 }
 ::=  {rsCP 8}



--;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
--;
--; Module : Peers
--; Date   : 14/06/01
--;
--;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

rsCPOptimalTotalPeakLoad OBJECT-TYPE
    SYNTAX INTEGER(101..1000)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "This value is kept as an integer - divide by 100 in code."
    DEFVAL  { 120 }
 ::=  {rsCP 9}



