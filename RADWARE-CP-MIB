rsCPPeersTable OBJECT-TYPE
    SYNTAX SEQUENCE OF RsCPPeerEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
      "Table containing the list of peers."
    ::=  {rsCP 2}

rsCPPeerEntry OBJECT-TYPE
    SYNTAX RsCPPeerEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
       " The row definition for this table. The table is made of three fields:
	 1) The peer's IP address.
	 2) The router's local IP for telnet & snmp.
     3) The carrier's name.
	 The table's key is made of fields 1+2."
    INDEX { rsCPPeerNeighborIPAddress,
            rsCPPeerRouterLocalIPAddress }
    ::=  {rsCPPeersTable 1}

RsCPPeerEntry ::= SEQUENCE {
  rsCPPeerNeighborIPAddress	    IpAddress,
  rsCPPeerRouterLocalIPAddress  IpAddress,
  rsCPPeerRoutemapName          DisplayString,
  rsCPPeerPeerState             INTEGER,--ronly
  rsCPPeerCurrentCapacity       INTEGER,--ronly
  rsCPPeerOriginalCapacity      INTEGER,--ronly
  rsCPPeerCurrentLoad           INTEGER,--ronly
  rsCPPeerStatus   	            RowStatus,
  rsCPPeerCriticalLoadThreshold	INTEGER,
  rsCPPeerOptimalLoad			INTEGER
  }

rsCPPeerNeighborIPAddress  OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The peer's IP address."
    ::=  {rsCPPeerEntry 1}

rsCPPeerRouterLocalIPAddress  OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The router's local IP for telnet & snmp."
    ::=  {rsCPPeerEntry 2}

rsCPPeerRoutemapName  OBJECT-TYPE
    SYNTAX  DisplayString (SIZE(1..20))
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "The carrier's name."
    ::=  {rsCPPeerEntry 3}

rsCPPeerPeerState OBJECT-TYPE
    SYNTAX INTEGER {
        unknown(0),
        active(1),
        inactive(2)
    }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The BGP peer connection state."
    ::=  {rsCPPeerEntry 4}

rsCPPeerCurrentCapacity OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The Sum of all current capacities of this Peer's interfaces."
    ::=  {rsCPPeerEntry 5}

rsCPPeerOriginalCapacity OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "sum of original capacities of the interfaces that belong to this peer."
    ::=  {rsCPPeerEntry 6}

rsCPPeerCurrentLoad OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "sum of rate load of interfaces that belong to this peer as informed by the router."
    ::=  {rsCPPeerEntry 7}

rsCPPeerStatus OBJECT-TYPE
    SYNTAX RowStatus
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
       "This variable indicates the administrative status of this entry."
    ::=  {rsCPPeerEntry 8}

rsCPPeerCriticalLoadThreshold  OBJECT-TYPE
    SYNTAX  INTEGER(0..100)
	ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "Critical Load Threshold"
    ::=  {rsCPPeerEntry 9}

rsCPPeerOptimalLoad  OBJECT-TYPE
    SYNTAX  INTEGER(0..100)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "Optimal Load Threshold"
    ::=  {rsCPPeerEntry 10}


