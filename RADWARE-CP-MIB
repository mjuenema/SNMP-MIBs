rsCPPeriodStartTimesTable OBJECT-TYPE
    SYNTAX SEQUENCE OF RsCPPeriodStartTimeEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
      "Table containing the list of period start times."
    ::=  {rsCP 14}

rsCPPeriodStartTimeEntry OBJECT-TYPE
    SYNTAX RsCPPeriodStartTimeEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
       " The row definition for this table. The table is made of three fields:
	 1) Hours.
	 2) Minutes.
	 The table's key is made of all fields."
    INDEX { rsCPPeriodStartTimeHours, rsCPPeriodStartTimeMinutes }
    ::=  {rsCPPeriodStartTimesTable 1}

RsCPPeriodStartTimeEntry ::= SEQUENCE {
  rsCPPeriodStartTimeHours          INTEGER,
  rsCPPeriodStartTimeMinutes	    INTEGER,
  rsCPPeriodStartTimeDescription    DisplayString,
  rsCPPeriodStartTimeStatus         RowStatus
  }

rsCPPeriodStartTimeHours  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    ::=  {rsCPPeriodStartTimeEntry 1}

rsCPPeriodStartTimeMinutes  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    ::=  {rsCPPeriodStartTimeEntry 2}

rsCPPeriodStartTimeDescription  OBJECT-TYPE
    SYNTAX  DisplayString (SIZE(0..19))
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "Time period's description."
    ::=  {rsCPPeriodStartTimeEntry 3}

rsCPPeriodStartTimeStatus OBJECT-TYPE
    SYNTAX RowStatus
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
       "This variable indicates the administrative status of this entry."
    ::=  {rsCPPeriodStartTimeEntry 4}



