rsCPCIDRPeerProhibitedMatchTable OBJECT-TYPE
    SYNTAX SEQUENCE OF RsCPCIDRPeerProhibitedMatchEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
      "Table containing the list of CIDR-Peer prohibited associations."
    ::=  {rsCP 29}

rsCPCIDRPeerProhibitedMatchEntry OBJECT-TYPE
    SYNTAX RsCPCIDRPeerProhibitedMatchEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
       " The row definition for this table."
    INDEX { rsCPCIDRPeerProhibitedMatchNeighborIP,
            rsCPCIDRPeerProhibitedMatchLocalRouterIP,
			rsCPCIDRPeerProhibitedMatchCIDRSubnet,
			rsCPCIDRPeerProhibitedMatchCIDRMask }
    ::=  {rsCPCIDRPeerProhibitedMatchTable 1}

RsCPCIDRPeerProhibitedMatchEntry ::= SEQUENCE {
  rsCPCIDRPeerProhibitedMatchNeighborIP		IpAddress,
  rsCPCIDRPeerProhibitedMatchLocalRouterIP  IpAddress,
  rsCPCIDRPeerProhibitedMatchCIDRSubnet		IpAddress,
  rsCPCIDRPeerProhibitedMatchCIDRMask		IpAddress,
  rsCPCIDRPeerProhibitedMatchDescription	DisplayString,
  rsCPCIDRPeerProhibitedMatchStatus   	    RowStatus
  }

rsCPCIDRPeerProhibitedMatchNeighborIP  OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The Peer's neighbor's address."
    ::=  {rsCPCIDRPeerProhibitedMatchEntry 1}

rsCPCIDRPeerProhibitedMatchLocalRouterIP  OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The Peer's local router's address."
    ::=  {rsCPCIDRPeerProhibitedMatchEntry 2}

rsCPCIDRPeerProhibitedMatchCIDRSubnet  OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The CIDR's subnet."
    ::=  {rsCPCIDRPeerProhibitedMatchEntry 3}

rsCPCIDRPeerProhibitedMatchCIDRMask  OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The CIDR's network mask."
    ::=  {rsCPCIDRPeerProhibitedMatchEntry 4}

rsCPCIDRPeerProhibitedMatchDescription  OBJECT-TYPE
    SYNTAX  DisplayString (SIZE(0..19))
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "The record's description."
    ::=  {rsCPCIDRPeerProhibitedMatchEntry 5}

rsCPCIDRPeerProhibitedMatchStatus OBJECT-TYPE
    SYNTAX RowStatus
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
       "This variable indicates the administrative status of this entry."
    ::=  {rsCPCIDRPeerProhibitedMatchEntry 6}


