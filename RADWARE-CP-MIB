rsCPPeersPhysicalInterfaceTable OBJECT-TYPE
    SYNTAX SEQUENCE OF RsCPPeerPhysicalInterfaceEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
      "Table containing the list of peer's physical interface."
    ::=  {rsCP 3}

rsCPPeerPhysicalInterfaceEntry OBJECT-TYPE
    SYNTAX RsCPPeerPhysicalInterfaceEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
       " The row definition for this table. The table is made of four fields:
	 1) The peer's IP address.
	 2) The router's local IP for telnet & snmp.
     3) The physical interface's IP address.
     4) The interface's original capacity.
	 The table's key is made of field 1."
    INDEX { rsCPPeerPhysicalInterfaceInterfaceIPAddress }
    ::=  {rsCPPeersPhysicalInterfaceTable 1}

RsCPPeerPhysicalInterfaceEntry ::= SEQUENCE {
  rsCPPeerPhysicalInterfaceInterfaceIPAddress  	IpAddress,
  rsCPPeerPhysicalInterfaceNeighborIPAddress    IpAddress,
  rsCPPeerPhysicalInterfaceLocalRouterIPAddress IpAddress,
  rsCPPeerPhysicalInterfaceCapacity             INTEGER,
  rsCPPeerPhysicalInterfaceIncomingLoad         INTEGER,--ronly
  rsCPPeerPhysicalInterfaceIndex                INTEGER,--ronly
  rsCPPeerPhysicalInterfaceOperStatus           INTEGER,--ronly
  rsCPPeerPhysicalInterfaceDescription          DisplayString,--opt
  rsCPPeerPhysicalInterfaceStatus               RowStatus
  }

rsCPPeerPhysicalInterfaceInterfaceIPAddress  OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The physical interface's IP address."
    ::=  {rsCPPeerPhysicalInterfaceEntry 1}

rsCPPeerPhysicalInterfaceNeighborIPAddress  OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "The peer's IP address."
    ::=  {rsCPPeerPhysicalInterfaceEntry 2}

rsCPPeerPhysicalInterfaceLocalRouterIPAddress  OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "The router's local IP for telnet & snmp."
    ::=  {rsCPPeerPhysicalInterfaceEntry 3}

rsCPPeerPhysicalInterfaceCapacity  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "The interface's original capacity."
    ::=  {rsCPPeerPhysicalInterfaceEntry 4}

rsCPPeerPhysicalInterfaceIncomingLoad  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The last polled interval's inbound load rate."
    ::=  {rsCPPeerPhysicalInterfaceEntry 5}

rsCPPeerPhysicalInterfaceIndex  OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "Physical index of the peer."
    ::=  {rsCPPeerPhysicalInterfaceEntry 6}

rsCPPeerPhysicalInterfaceOperStatus  OBJECT-TYPE
    SYNTAX INTEGER {
        unknown(0),
        active(1),
        inactive(2)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "Operational status of the interface."
    ::=  {rsCPPeerPhysicalInterfaceEntry 7}

rsCPPeerPhysicalInterfaceDescription  OBJECT-TYPE
    SYNTAX  DisplayString (SIZE(0..19))
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "A name the administrator can give the peer on interface."
    ::=  {rsCPPeerPhysicalInterfaceEntry 8}

rsCPPeerPhysicalInterfaceStatus OBJECT-TYPE
    SYNTAX RowStatus
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
       "This variable indicates the administrative status of this entry."
    ::=  {rsCPPeerPhysicalInterfaceEntry 9}



