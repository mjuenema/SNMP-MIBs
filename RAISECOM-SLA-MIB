----rcSlaExEthResultTable
rcSlaExEthResultTable  OBJECT-TYPE
    SYNTAX      SEQUENCE OF RcSlaExEthResultEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table contains sla eth result information."
    ::= { rcSlaScheduleGroup 5 }

rcSlaExEthResultEntry   OBJECT-TYPE
    SYNTAX      RcSlaExEthResultEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table contains sla eth result information."
	INDEX        { rcSlaExEthMdLevel, 
	               rcSlaExEthSVlanId,
	               rcSlaExEthDMac,
	               rcSlaExEthDMepId}    
    ::={ rcSlaExEthResultTable 1 }     
    
    RcSlaExEthResultEntry ::= SEQUENCE {
        rcSlaExEthMdLevel                         Dot1agCfmMDLevel,
        rcSlaExEthSVlanId                         VlanId,
        rcSlaExEthDMac                            MacAddress,
        rcSlaExEthDMepId                          Dot1agCfmMepId,
    	rcSlaExEthCurrDelay                       Counter32,      
	    rcSlaExEthCurrDelaySD                     Counter32,
	    rcSlaExEthCurrDelayDS                     Counter32,
	    rcSlaExEthCurrJitter                      Counter32,
	    rcSlaExEthCurrJitterSD                    Counter32,
	    rcSlaExEthCurrJitterDS                    Counter32,
        rcSlaExEthCurrPktLossRateSD               Counter32,
        rcSlaExEthCurrPktLossRateDS               Counter32,
}

rcSlaExEthMdLevel       OBJECT-TYPE 
	SYNTAX        Dot1agCfmMDLevel
	MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
            "This object specifies the maintenance domain level to be used by Y.1731 operation."
    ::= { rcSlaExEthResultEntry 1 }                    


rcSlaExEthSVlanId       OBJECT-TYPE 
	SYNTAX        VlanId
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
            "This object specifies the identifier of VLAN for MA to be used by Y.1731 operation.
				It is an unique identifier for a service instance 
				within a Maintenance Domain specified by rcSlaConfigMDLevel."
    ::= { rcSlaExEthResultEntry 2 }     
    
rcSlaExEthDMac       OBJECT-TYPE 
	SYNTAX        MacAddress
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
            "This object specifies the Target MAC Address Field to be used by Y.1731 operation."
    ::= { rcSlaExEthResultEntry 3 }     
    
rcSlaExEthDMepId       OBJECT-TYPE 
	SYNTAX        Dot1agCfmMepId
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
            "This object specifies the Target Remote MEP Identify to be used by Y.1731 operation."
    ::= { rcSlaExEthResultEntry 4 }     
 	    
rcSlaExEthCurrDelay       OBJECT-TYPE 
	SYNTAX      Counter32
	MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The RTT delay of the last test."
    ::= { rcSlaExEthResultEntry 5 }     
    
rcSlaExEthCurrDelaySD       OBJECT-TYPE 
	SYNTAX      Counter32
	MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The delay of the last test from source to destination."
    ::= { rcSlaExEthResultEntry 6}     
    
rcSlaExEthCurrDelayDS       OBJECT-TYPE 
	SYNTAX      Counter32
	MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The delay of the last test from destination to source."
    ::= { rcSlaExEthResultEntry 7 }     
    
rcSlaExEthCurrJitter       OBJECT-TYPE 
	SYNTAX      Counter32
	MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The RTT jitter of the last test."
    ::= { rcSlaExEthResultEntry 8 }     

rcSlaExEthCurrJitterSD       OBJECT-TYPE 
	SYNTAX      Counter32
	MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The jitter of the last test from source to destination."
    ::= { rcSlaExEthResultEntry 9 }     
    
rcSlaExEthCurrJitterDS       OBJECT-TYPE 
	SYNTAX      Counter32
	MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The jitter of the last test from destination to source."
    ::= { rcSlaExEthResultEntry 10 }     
    
rcSlaExEthCurrPktLossRateSD       OBJECT-TYPE 
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
