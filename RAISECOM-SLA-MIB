    ::= { rcSlaScheduleGroup 6 }

rcSlaExPwResultEntry   OBJECT-TYPE
    SYNTAX      RcSlaExPwResultEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table contains sla pw result information."
	INDEX        { rcSlaExPwMdLevel,
	               rcSlaExPwVcId,
	               rcSlaExPwPeerAddrType,
	               rcSlaExPwPeerAddr}    
    ::={ rcSlaExPwResultTable 1 }     
    
    RcSlaExPwResultEntry ::= SEQUENCE {
        rcSlaExPwMdLevel                         Dot1agCfmMDLevel,
        rcSlaExPwVcId                            Integer32 (1..2147483647 ),
        rcSlaExPwPeerAddrType                    InetAddressType,
        rcSlaExPwPeerAddr                        InetAddress,
    	rcSlaExPwCurrDelay                       Counter32,      
	    rcSlaExPwCurrDelaySD                     Counter32,
	    rcSlaExPwCurrDelayDS                     Counter32,
	    rcSlaExPwCurrJitter                      Counter32,
	    rcSlaExPwCurrJitterSD                    Counter32,
	    rcSlaExPwCurrJitterDS                    Counter32,
        rcSlaExPwCurrPktLossRateSD               Counter32,
        rcSlaExPwCurrPktLossRateDS               Counter32,
}

