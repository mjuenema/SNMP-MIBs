    CAhacmp-MIB DEFINITIONS ::= BEGIN
 
        IMPORTS
            OBJECT-TYPE            
                FROM RFC-1212            
            TRAP-TYPE            
                FROM RFC-1215            
            enterprises            
                FROM RFC1155-SMI            
            DisplayString            
                FROM RFC1213-MIB;
    
    
    
--
-- Node definitions
--
    
-- *****************************************************************************
-- OIDs of hacmp agent start with 1.3.6.1.4.1.546.16.51
-- *****************************************************************************
-- 
        empire OBJECT IDENTIFIER ::= { enterprises 546 }

        
--      1.3.6.1.4.1.546.1
        unix OBJECT IDENTIFIER ::= { empire 1 }

        
--      1.3.6.1.4.1.546.1.1
        sysmgmt OBJECT IDENTIFIER ::= { unix 1 }

        
--      1.3.6.1.4.1.546.1.1.9
        traps OBJECT IDENTIFIER ::= { empire 9 }

        
--        1.3.6.1.4.1.546.16
        applications OBJECT IDENTIFIER ::= { empire 16 }

        
--        1.3.6.1.4.1.546.16.56
        cahacmpaim OBJECT IDENTIFIER ::= { applications 55 }

        
--        1.3.6.1.4.1.546.16.51.1
        hacmpAimGroup OBJECT IDENTIFIER ::= { cahacmpaim 1 }

        
--        1.3.6.1.4.1.546.16.56.1.1
        hacmpAimInstanceCount OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Number of instances in hacmpAimInstanceTable."
            ::= { hacmpAimGroup 1 }

        
--        1.3.6.1.4.1.546.16.56.1.2
        hacmpAimInstanceTable OBJECT-TYPE
            SYNTAX SEQUENCE OF hacmpAimInstanceEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "A table containing the hacmp instances."
            ::= { hacmpAimGroup 2 }

        
--        1.3.6.1.4.1.546.16.56.1.2.1
        hacmpAimInstanceEntry OBJECT-TYPE
            SYNTAX hacmpAimInstanceEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "The hacmp instances table entry."
            INDEX { hacmpAimInstanceIndex }
            ::= { hacmpAimInstanceTable 1 }

        
        hacmpAimInstanceEntry ::=
            SEQUENCE { 
                hacmpAimInstanceIndex
                    INTEGER,
                hacmpAimInstanceName
                    DisplayString,
                hacmpAimInstanceUserName
                    DisplayString,
                hacmpAimInstancePassword
                    DisplayString,
                hacmpAimInstancePort
                    INTEGER,
                hacmpAimInstanceRowStatus
                    INTEGER,
                hacmpAimInstanceDataStatus
                    INTEGER
             }

--        1.3.6.1.4.1.546.16.51.1.2.1.1
        hacmpAimInstanceIndex OBJECT-TYPE
            SYNTAX INTEGER (1..2147483647)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                " The unique index of the instance."
            ::= { hacmpAimInstanceEntry 1 }

        
--        1.3.6.1.4.1.546.16.51.1.2.1.2
        hacmpAimInstanceName OBJECT-TYPE
            SYNTAX DisplayString
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Name of the instance"
            ::= { hacmpAimInstanceEntry 2 }

        
--        1.3.6.1.4.1.546.16.51.1.2.1.3
        hacmpAimInstanceUserName OBJECT-TYPE
            SYNTAX DisplayString
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "User name for connecting to the instance."
            ::= { hacmpAimInstanceEntry 3 }

        
--        1.3.6.1.4.1.546.16.51.1.2.1.4
        hacmpAimInstancePassword OBJECT-TYPE
            SYNTAX DisplayString
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Password for the user name provided to connect to the instance."
            ::= { hacmpAimInstanceEntry 4 }

        
--        1.3.6.1.4.1.546.16.51.1.2.1.4
        hacmpAimInstancePort OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Port used for connection to the cluster."
            ::= { hacmpAimInstanceEntry 5 }

        
--        1.3.6.1.4.1.546.16.51.1.2.1.5
        hacmpAimInstanceRowStatus OBJECT-TYPE
            SYNTAX INTEGER
                {
                active(1),
                notInService(2),
                notReady(3),
                createAndGo(4),
                createAndWait(5),
                destroy(6)
                }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "The status of this monitor entry according to the Textual Conventions for SNMPv2 (see RFC 1443).
                active(1) - the row is available for usage
                notInService(2) - setting to this value disables usage of the row
                notReady(3) - an error occurred during usage of the row
                createAndGo(4) - setting to this value creates the row with row status set to active(1)
                createAndWait(5) - setting to this value creates the row with row status set to notInService(2)
                destroy(6) - setting to this value deletes the row"
            DEFVAL { 5 }
            ::= { hacmpAimInstanceEntry 6 }

        
--        1.3.6.1.4.1.546.16.51.1.2.1.6
        hacmpAimInstanceDataStatus OBJECT-TYPE
            SYNTAX INTEGER
                {
                ready(1),
                notReady(2),
                hostUnreachable(3),
                hostIsSlave(4),
                authenticationFailed(5),
                unknownError(6)
                }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The status of data of the  hacmp Instance (Server), i.e. status of Aim caches for the instance.
                ready(1) - data are ready in Aim cache, caches were populated
                notReady(2)  - data are Not ready, caches were not populated yet
                hostUnreachable(3) - hacmp host unreachable, data are kept in Aim caches
                hostIsSlave(4) -  hacmp host is a slave server, data are kept in Aim caches
                authenticationFailed(5) - authentication failed for host, data are kept in Aim caches
                unknownError(6) - unknow error, data are kept in Aim caches"
            ::= { hacmpAimInstanceEntry 7 }

        
        hacmpAimInstanceUnusedIndex OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "An SNMP GET of this attribute returns an unused Instance UID. This
                is normally used to expedite safe table row creation."
            ::= { hacmpAimGroup 3 }

        
