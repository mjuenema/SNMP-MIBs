        hacmpAimInstanceAddedTrap TRAP-TYPE 
            ENTERPRISE sysmgmt
            VARIABLES { hacmpAimInstanceIndex, hacmpAimInstanceName, hacmpAimInstanceRowStatus, hacmpAimInstanceDataStatus }
            DESCRIPTION 
                "This trap is sent when a new Instance/Server is added.
                
                Parameters:
                hacmpAimInstanceIndex
                hacmpAimInstanceName
                hacmpAimInstanceRowStatus
                hacmpAimInstanceDataStatus"
            ::=  165800

        hacmpAimInstanceRemovedTrap TRAP-TYPE 
            ENTERPRISE sysmgmt
            VARIABLES { hacmpAimInstanceIndex, hacmpAimInstanceName, hacmpAimInstanceRowStatus, hacmpAimInstanceDataStatus }
            DESCRIPTION 
                "This trap is sent when an Instance/Server is removed.
                
                Parameters:
                hacmpAimInstanceIndex
                hacmpAimInstanceName
                hacmpAimInstanceRowStatus
                hacmpAimInstanceDataStatus"
            ::=  165801

