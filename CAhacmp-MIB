        hacmpAimStatResourceGroupStartupPolicy OBJECT-TYPE
            SYNTAX INTEGER
                {
                homeNodeOnly(1),
                firstAvailNode(2),
                usingDistPolicy(3),
                allAvailNodes(4)
                }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The Resource Group's Startup Policy:
                
                This can have the following values:
                Online On HomeNode Only  1
                Online On First Available Node  2
                Online Using Distribution Policy  3
                Online On All Available Nodes  4"
            ::= { hacmpAimStatResourceGroupEntry 10 }

        
        hacmpAimStatResourceGroupFalloverPolicy OBJECT-TYPE
            SYNTAX INTEGER
                {
                toNextPrioNode(5),
                usingDNP(6),
                bringOffline(7)
                }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The Resource Group's Fallover Policy
                This can have the following values:
                
                Fallover To Next Priority Node On the List  5
                Fallover Using DNP 6
                Bring Offline  7"
            ::= { hacmpAimStatResourceGroupEntry 11 }

        
        hacmpAimStatResourceGroupFallbackPolicy OBJECT-TYPE
            SYNTAX INTEGER
                {
                toHigherPrioNode(8),
                neverFallback(9)
                }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The Resource Group's Fallback Policy
                
                Fallback to Higher Priority Node in the List  8
                Never Fallback  9"
            ::= { hacmpAimStatResourceGroupEntry 12 }

        
