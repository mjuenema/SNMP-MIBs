TELDAT-TRAP-SCCP-MIB DEFINITIONS ::= BEGIN


IMPORTS
	MODULE-IDENTITY,OBJECT-TYPE,OBJECT-IDENTITY,NOTIFICATION-TYPE,Integer32,IpAddress
		FROM SNMPv2-SMI
	DisplayString
		FROM SNMPv2-TC
	telEventTraps
		FROM TELDAT-MIB;


sccpTrapsMIB MODULE-IDENTITY
    LAST-UPDATED "201401200930Z"
    ORGANIZATION "Teldat, S.A."
    CONTACT-INFO 
        "Teldat, S.A.
        Customer Service

        Postal: Parque Tecnologico de Madrid
                Tres Cantos 28760
                Madrid, SPAIN

        Tel.: +34 918 076 565

        http://www.teldat.com"
    DESCRIPTION
        "This Module Information Base file is defined for managing received
        SNMP notifications generated by the SCCP event subsystem."
    REVISION "201401200930Z"
    DESCRIPTION "Initial implementation"
        ::= { telEventTraps 210 }


sccpEventsGroup	OBJECT IDENTIFIER	::= { sccpTrapsMIB 1 }

sccpNotifications	OBJECT IDENTIFIER	::= { sccpTrapsMIB 2 }
sccpNotificationsPrefix	OBJECT IDENTIFIER	::= { sccpNotifications 0 }

sccpEventObjects1 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event SCCP.1
        Event related with tcp connection."
    ::= { sccpEventsGroup 1 }

sccp1_state OBJECT-TYPE
    SYNTAX DisplayString
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Parameter 'state' in event SCCP.1"
    ::= { sccpEventObjects1 1 }

sccp1_ip OBJECT-TYPE
    SYNTAX IpAddress
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Parameter 'ip' in event SCCP.1"
    ::= { sccpEventObjects1 2 }

