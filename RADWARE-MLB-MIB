    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
	"VPN rule table (SPDB)"
    INDEX {rsMLBVpnRuleLclSubnet, rsMLBVpnRuleLclSubnetMask,
	   rsMLBVpnRuleRemSubnet, rsMLBVpnRuleRemSubnetMask}
   ::= { rsMLBVpnRuleTable 1}				

  RsMLBVpnRuleEntry ::=  SEQUENCE {
	rsMLBVpnRuleLclSubnet			IpAddress,
	rsMLBVpnRuleLclSubnetMask		IpAddress,
	rsMLBVpnRuleRemSubnet			IpAddress,
	rsMLBVpnRuleRemSubnetMask 		IpAddress,
	rsMLBVpnRuleName			DisplayString,
	rsMLBVpnRuleIkeName			DisplayString,
	rsMLBVpnRuleLclGateway			IpAddress,
	rsMLBVpnRuleRemGateway			IpAddress,
	rsMLBVpnRuleFlowMode			INTEGER,
	rsMLBVpnRuleRemSrvcName			DisplayString,	
	rsMLBVpnRuleMode			INTEGER,
	rsMLBVpnRuleNhr1Addr	 		IpAddress,
	rsMLBVpnRuleNhr2Addr	 		IpAddress,
	rsMLBVpnRuleNhr3Addr	 		IpAddress,
	rsMLBVpnRuleNhr4Addr	 		IpAddress,
	rsMLBVpnRuleNhr5Addr	 		IpAddress,
	rsMLBVpnRuleAction			INTEGER,
	rsMLBVpnRuleOperStatus			INTEGER,	
	rsMLBVpnRuleLclGateway2			IpAddress,
	rsMLBVpnRuleRemGateway2			IpAddress,
	rsMLBVpnRuleLclGateway3			IpAddress,
	rsMLBVpnRuleRemGateway3			IpAddress,
	rsMLBVpnRuleLclGateway4			IpAddress,
	rsMLBVpnRuleRemGateway4			IpAddress,
	rsMLBVpnRuleRowStatus			INTEGER
  }

