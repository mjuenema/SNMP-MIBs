    SYNTAX INTEGER {
      enable(1),
      disable(2)
          }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "Enable\Disable proximity check through this router."
-- DEFVAL { 1 }
 ::=  {rsMLBApplicationServerEntry 22 }

