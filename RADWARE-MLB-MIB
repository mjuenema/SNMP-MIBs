    SYNTAX INTEGER
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "The maximal number of bytes per second dispatched to server."
-- DEFVAL {0}
    ::= { rsMLBApplicationServerEntry 27 }

