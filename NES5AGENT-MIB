           nes5URLMntrWarningThreshold                    INTEGER,
           nes5URLMntrCriticalThreshold                   INTEGER,
           nes5URLMntrWarningAction                       DisplayString,
           nes5URLMntrCriticalAction                      DisplayString,
           nes5URLMntrStatus                              INTEGER,
           nes5URLMntrEnableDisable                       INTEGER,
           nes5URLMntrDescending                          INTEGER,
           nes5URLMntrReset                               INTEGER,
           nes5URLMntrResetTime                           DisplayString,
	   nes5URLMntrValueReported			INTEGER,
           nes5URLMntrPrevStatus          INTEGER,
	   nes5URLMntrLagSetting		INTEGER,
	   nes5URLMntrLagValue		INTEGER
        }

