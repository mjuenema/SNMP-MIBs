               ::=  { nes5WCTable 1 }

        Nes5WCEntry ::= SEQUENCE
        {
           nes5WCURL                      DisplayString,
           nes5WCReferences               INTEGER,
           nes5WCBadIntLink               INTEGER,
           nes5WCBadExtLink               INTEGER,
           nes5WCTitle                    INTEGER,
           nes5WCBadAnchors               INTEGER,
           nes5WCBadImages                INTEGER,
           nes5WCAccessTS                 DisplayString
        }

