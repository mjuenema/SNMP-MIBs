--******************************************************************************
--* Local action log - circular buffer of local actions performed              *
--* Dynamic get, not polled.                                                   *
--******************************************************************************

        iis5LocalActionTableEntries OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "The number of entries in the local action log table."
               ::=  {  iis5LocalActionGroup 1   }

        iis5LocalActionCircularBuffSize OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Max entries in the LocalActionLogTable (default 50)."
               ::=  {  iis5LocalActionGroup 2   }

        iis5LocalActionRefresh OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Refresh the table starting with the most recent action."
               ::=  {  iis5LocalActionGroup 3   }

        iis5LocalActionStartDateTime OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Refresh the table starting at the specified date and time."
               ::=  {  iis5LocalActionGroup 4   }

        iis5LocalActionScrollDown OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Read the next older set of log entries into the table."
               ::=  {  iis5LocalActionGroup 5   }

        iis5LocalActionScrollUp OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Read the next newer set of log entries into the table."
               ::=  {  iis5LocalActionGroup 6   }

        iis5LocalActionLogTable OBJECT-TYPE
               SYNTAX  SEQUENCE OF Iis5LocalActionLogEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This table contains a list of command actions taken in response to events."
               ::=  {  iis5LocalActionGroup 7 }

        iis5LocalActionLogEntry OBJECT-TYPE
                                SYNTAX  Iis5LocalActionLogEntry
                                ACCESS  not-accessible
                                STATUS  mandatory
                                DESCRIPTION
                                "This is an entry in the LocalActionTable"
                                INDEX { iis5LocalActionSequencer }
                                ::=  { iis5LocalActionLogTable 1 }

        Iis5LocalActionLogEntry ::= SEQUENCE
        {
           iis5LocalActionSequencer               INTEGER,
           iis5LocalActionTimestamp               DisplayString,
           iis5LocalActionSeverity                INTEGER,
           iis5LocalActionReason                  DisplayString,
           iis5LocalActionCommand                 DisplayString
        }

        iis5LocalActionSequencer OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Table sequencer"
               ::=  {  iis5LocalActionLogEntry 1   }

        iis5LocalActionTimestamp OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Time the command was executed (action taken)."
               ::=  {  iis5LocalActionLogEntry 2   }

-- The Warning and Critical values for severity must match CLocalAction::enum Severity.
        iis5LocalActionSeverity OBJECT-TYPE
               SYNTAX  INTEGER
               {
                   normal(1),
                   warning(2),
                   critical(3)
               }
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "The severity of the event which triggered the action."
               ::=  {  iis5LocalActionLogEntry 3   }

        iis5LocalActionReason OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "The reason the action was taken."
               ::=  {  iis5LocalActionLogEntry 4   }

        iis5LocalActionCommand OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Command executed (action taken)."
               ::=  {  iis5LocalActionLogEntry 5   }


