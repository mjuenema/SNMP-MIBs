--******************************************************************************
--* URLMntrGroup - Monitored URLs                                            *
--******************************************************************************

        iis5URLMntrTableEntries OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "The number of entries in the table."
               ::=  {  iis5URLMntrGroup 1   }

        iis5URLMntrInterval OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Seconds between polls for the counter."
               ::=  {  iis5URLMntrGroup 2   }

        iis5URLMntrHitRate OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Interval in seconds over which the counter is measured."
               ::=  {  iis5URLMntrGroup 3   }

--      Add a counter to the table.  The value must include at least the first four of
--      the 6 items listed, separated by commas.
--      <iis5URLAvailCounterDescription> is a value from iis5URLAvailCounterDescription in iis5URLAvailCounterTable.

        iis5URLMntrAdd OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  write-only
               STATUS  mandatory
               DESCRIPTION
               "Add this entry to the table: <url>,<URLAvailCounterDescription>,<warning threshold>,<critical threshold>,<warning action>,<critical action>."
               ::=  {  iis5URLMntrGroup 4   }

--      Delete a counter from the table.  The value must include the 2 items listed.
        iis5URLMntrDelete OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  write-only
               STATUS  mandatory
               DESCRIPTION
               "Delete entry from the table: <URLMntrDesc>,<url>."
               ::=  {  iis5URLMntrGroup 5   }

        iis5URLMntrUpdate OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  write-only
               STATUS  mandatory
	       DESCRIPTION "Update this entry in the table."
               ::=  {  iis5URLMntrGroup 6   }

        iis5URLMntrSummaryStatus OBJECT-TYPE
               SYNTAX  INTEGER {
                                normal(1),
                                warning(2),
                                critical(3)
                               }
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Most severe status of all rows in the table."
               ::=  {  iis5URLMntrGroup 7   }

        iis5URLMntrTable OBJECT-TYPE
               SYNTAX  SEQUENCE OF Iis5URLMntrEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This table contains a list of URLs to monitor."
               ::=  {  iis5URLMntrGroup 8}

        iis5URLMntrEntry OBJECT-TYPE
               SYNTAX  Iis5URLMntrEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This is an entry in the URLMntrTable"
               INDEX { iis5URLMntrURL, iis5URLMntrDesc}
               ::=  { iis5URLMntrTable 1 }

        Iis5URLMntrEntry ::= SEQUENCE
        {
           iis5URLMntrURL                 DisplayString,
           iis5URLMntrDesc                DisplayString,
           iis5URLMntrValue               DisplayString,
           iis5URLMntrWarningThreshold    INTEGER,
           iis5URLMntrCriticalThreshold   INTEGER,
           iis5URLMntrWarningAction       DisplayString,
           iis5URLMntrCriticalAction      DisplayString,
           iis5URLMntrStatus              INTEGER,
           iis5URLMntrEnableDisable       INTEGER,
           iis5URLMntrDescending          INTEGER,
           iis5URLMntrReset               INTEGER,
           iis5URLMntrResetTime           DisplayString,
	   iis5URLMntrValueReported	INTEGER,
           iis5URLMntrPrevStatus          INTEGER,
	   iis5URLMntrLagSetting		INTEGER,
	   iis5URLMntrLagValue		INTEGER
        }

        iis5URLMntrURL OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "URL to monitor."
               ::=  {  iis5URLMntrEntry 1   }

        iis5URLMntrDesc OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Description of the monitored value."
               ::=  {  iis5URLMntrEntry 2   }

