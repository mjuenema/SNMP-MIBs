--******************************************************************************
--* iis5EventGroup - user defined event monitoring                               *
--******************************************************************************
--      In this agent, we'll put available events in wsEventTable ahead of time
--      and allow the user to enable them for monitoring.

        iis5EventTableEntries OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The number of entries in the event description table."
                ::=  {  iis5EventGroup 1   }

        iis5EventLogTableEntries OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The number of entries in the event log table."
                ::=  {  iis5EventGroup 2   }

        iis5EventAdd OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  write-only
                STATUS  mandatory
                DESCRIPTION
