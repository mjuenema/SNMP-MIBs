               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Interval in seconds over which the counter is measured."
               ::=  {  iis5ExpressionGroup 3   }

--      Update this comment.
--      Add a counter to the table.  The value must include at least the first four of
--      the 6 items listed, separated by commas.
--      <iis5URLAvailCounterDescription> is a value from iis5URLAvailCounterDescription in iis5URLAvailCounterTable.

        iis5ExpressionAdd OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  write-only
               STATUS  mandatory
               DESCRIPTION
               "Add this entry to the table."
               ::=  {  iis5ExpressionGroup 4   }

--      Delete a counter from the table.  The value must include the 2 items listed.
        iis5ExpressionDelete OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  write-only
               STATUS  mandatory
               DESCRIPTION
               "Delete entry from the table: <ExpressionDesc>,<Expression>."
               ::=  {  iis5ExpressionGroup 5   }

--      For future use with bulk load.  Currently ignored.
        iis5ExpressionUpdate OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  write-only
               STATUS  mandatory
               DESCRIPTION
               "Reserved for future use."
               ::=  {  iis5ExpressionGroup 6   }

        iis5ExpressionSummaryStatus OBJECT-TYPE
               SYNTAX  INTEGER {
                                normal(1),
                                warning(2),
                                critical(3)
                               }
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Most severe status of all rows in the table."
               ::=  {  iis5ExpressionGroup 7   }

        iis5ExpressionTable OBJECT-TYPE
               SYNTAX  SEQUENCE OF Iis5ExpressionEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This table contains a list of expressions to monitor."
               ::=  {  iis5ExpressionGroup 8}

        iis5ExpressionEntry OBJECT-TYPE
               SYNTAX  Iis5ExpressionEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This is an entry in the ExpressionTable"
               INDEX { iis5ExpressionIdentifier}
               ::=  { iis5ExpressionTable 1 }

        Iis5ExpressionEntry ::= SEQUENCE
        {
           iis5Expression                                    DisplayString,
           iis5ExpressionDesc                                DisplayString,
           iis5ExpressionValue                               DisplayString,
           iis5ExpressionWarningThreshold                    INTEGER,
           iis5ExpressionCriticalThreshold                   INTEGER,
           iis5ExpressionWarningAction                       DisplayString,
           iis5ExpressionCriticalAction                      DisplayString,
           iis5ExpressionStatus                              INTEGER,
           iis5ExpressionEnableDisable                       INTEGER,
           iis5ExpressionDescending                          INTEGER,
           iis5ExpressionResultType                          INTEGER,
           iis5ExpressionBooleanAlertLevel                   INTEGER,
           iis5ExpressionEvalErrorAlertLevel                 INTEGER,
           iis5ExpressionNumericMonitor                      INTEGER,
           iis5ExpressionEntryInterval                       INTEGER,
           iis5ExpressionReset                               INTEGER,
           iis5ExpressionResetTime                           DisplayString,
           iis5ExpressionEntryWarningCount                   INTEGER,
           iis5ExpressionEntryCriticalCount                  INTEGER,
           iis5ExpressionIdentifier                          DisplayString,
           iis5ExpressionErrorMsg                            DisplayString,
           iis5ExpressionEvalStatus                          INTEGER,
           iis5ExpressionBoolAlertCondition                  INTEGER,
	   iis5ExpressionLagSetting	                   INTEGER,
	   iis5ExpressionLagValue	                           INTEGER
        }

--     Changed from read only to read write to allow introducing evaluation errors
        iis5Expression OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Expression to be monitored."
               ::=  {  iis5ExpressionEntry 1   }

