--MibName=iis5Agent
--
--******************************************************************************
--*  WSA 5 Agent MIB prototype for Apache and IIS                              *
--*  edit iis5 to "ap5" prefix                                                   *
--*  edit Iis5 to "Ap5" prefix                                                   *
--*  edit IIS5 to "AP5" prefix                                                   *
--*  edit 77 to OID                                                   	       *
--******************************************************************************

IIS5AGENT-MIB DEFINITIONS ::= BEGIN

IMPORTS
	enterprises   FROM RFC1155-SMI
	OBJECT-TYPE   FROM RFC1155-SMI
	DisplayString FROM RFC1158-MIB;
        
-- Base OID  1.3.6.1.4.1.791.2.10.2.77

    cai         OBJECT IDENTIFIER ::= { enterprises 791 }

    caiSysMgr   OBJECT IDENTIFIER ::= { cai 2 }

    tng         OBJECT IDENTIFIER ::= { caiSysMgr 10 }

    unix-nt      OBJECT IDENTIFIER ::= { tng 2 }
                      
    iis5Agent   OBJECT IDENTIFIER ::= { unix-nt 77 }

    iis5ServerGroup           OBJECT IDENTIFIER   ::=  {  iis5Agent 1   }
    iis5ServiceGroup          OBJECT IDENTIFIER   ::=  {  iis5Agent 2   }
    iis5BadLinkGroup          OBJECT IDENTIFIER   ::=  {  iis5Agent 3   }
    iis5TopURLGroup           OBJECT IDENTIFIER   ::=  {  iis5Agent 4   }
    iis5URLAvailCounterGroup  OBJECT IDENTIFIER   ::=  {  iis5Agent 5   }
    iis5URLMntrGroup          OBJECT IDENTIFIER   ::=  {  iis5Agent 6   }
    iis5DiskGroup             OBJECT IDENTIFIER   ::=  {  iis5Agent 7   }
    iis5AvailCounterGroup     OBJECT IDENTIFIER   ::=  {  iis5Agent 8   }
    iis5PolledCounterGroup    OBJECT IDENTIFIER   ::=  {  iis5Agent 9   }
    iis5EventGroup            OBJECT IDENTIFIER   ::=  {  iis5Agent 10  }
    iis5AgentGroup            OBJECT IDENTIFIER   ::=  {  iis5Agent 11  }
    iis5WCGroup               OBJECT IDENTIFIER   ::=  {  iis5Agent 12  }
    iis5SHGroup               OBJECT IDENTIFIER   ::=  {  iis5Agent 13  }
    iis5LocalActionGroup      OBJECT IDENTIFIER   ::=  {  iis5Agent 14  }
    iis5ServLogGroup          OBJECT IDENTIFIER   ::=  {  iis5Agent 15  }
    iis5ExpressionGroup       OBJECT IDENTIFIER   ::=  {  iis5Agent 16  }
    iis5DetailGroup    	    OBJECT IDENTIFIER   ::=  {  iis5Agent 17  }
    iis5VHGroup    	    OBJECT IDENTIFIER   ::=  {  iis5Agent 18  }
    iis5IISAvailCounterGroup  OBJECT IDENTIFIER   ::=  {  iis5Agent 19  }
    iis5IISPolledCounterGroup OBJECT IDENTIFIER   ::=  {  iis5Agent 20  }

--******************************************************************************
--* ServerGroup - Site and summary information                               *
--******************************************************************************

        iis5ServerName OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The host name of the computer."
                ::=  {  iis5ServerGroup 1   }

        iis5ServerSummaryStatus OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    ok(1),
                    warning(2),
                    critical(3)
                }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The status of the monitored server."
                ::=  {  iis5ServerGroup 2   }

        iis5ServerPollingInterval OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Poll interval for the server group in seconds."
                ::=  {  iis5ServerGroup 3   }

        iis5ServerResetCounts OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    reset(1)
                }
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Resets all history counts to zero when set."
                ::=  {  iis5ServerGroup 4   }

        iis5ServerResetTS OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Time of last reset in ctime form."
                ::=  {  iis5ServerGroup 5   }

        iis5ServerResetTSEdit OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Time of last reset in display form."
                ::=  {  iis5ServerGroup 6   }

        iis5ServerResetDays OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Number of days between automatic history count resets."
                ::=  {  iis5ServerGroup 7   }

        iis5ServerVersion OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The version of the web server."
                ::=  {  iis5ServerGroup 8   }

        iis5ServerPort OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Port the server is listening on."
                ::=  {  iis5ServerGroup 9   }

        iis5ServerLBHost OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Host name for Agent Collector."
                ::=  {  iis5ServerGroup 10   }

        iis5ServerLBPort OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Agent Collector name."
                ::=  {  iis5ServerGroup 11   }

	iis5ServerLBEnaDis OBJECT-TYPE
		SYNTAX      INTEGER {enable(1), disable(2)}
		ACCESS      read-write
		STATUS      mandatory
		DESCRIPTION "Enable or disable the Agent Collector."
		::= {iis5ServerGroup 12}

        iis5SuperAgentRequest OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Super Agent service request."
                ::=  {  iis5ServerGroup 13   }

        iis5ServerHiLevelVersion OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Oracle server: The high level version number of the web server."
                ::=  {  iis5ServerGroup 14   }

        iis5ServerSetBaseLine OBJECT-TYPE
                SYNTAX  INTEGER {
                                set(1)
                               }
                ACCESS  write-only
                STATUS  mandatory
                DESCRIPTION
                "Immediately set Adaptive Configuration values."
                ::=  {  iis5ServerGroup 15   }

        iis5ServerBaseLineWarnWeight OBJECT-TYPE
                SYNTAX  INTEGER 
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Adaptive Configuration warning weighing factor."
                ::=  {  iis5ServerGroup 16   }

        iis5ServerBaseLineCritWeight OBJECT-TYPE
                SYNTAX  INTEGER 
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Adaptive Configuration critical weighing factor."
                ::=  {  iis5ServerGroup 17   }

	iis5ServerBaseLineEnaDis OBJECT-TYPE
		SYNTAX      INTEGER {disable(1), enable(2)}
		ACCESS      read-write
		STATUS      mandatory
		DESCRIPTION "Enable or disable Active Adaptive Configuration."
		::= {  iis5ServerGroup 18  }

	iis5ServerBaseLineTimer OBJECT-TYPE
		SYNTAX      INTEGER
		ACCESS      read-write
		STATUS      mandatory
		DESCRIPTION "Active Adaptive Configuration timer."
		::= {  iis5ServerGroup 19  }
                
        iis5ServerExportConfigSet OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Name of config to export to."
                ::=  {  iis5ServerGroup 20   }
                
        iis5ServerImportConfigSet OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Name of config to import from."
                ::=  {  iis5ServerGroup 21   }
               
        iis5retVHTableInterval OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Seconds between polls for the VHTable."
               ::=  { iis5ServerGroup 22 }
               

	iis5retVHGPEnaDis OBJECT-TYPE
		SYNTAX      INTEGER {enable(1), disable(2)}
		ACCESS      read-write
		STATUS      mandatory
		DESCRIPTION "Enable or disable for the VHGroup."
		::= { iis5ServerGroup 23 }


--******************************************************************************
--* ServiceGroup - Services                                                  *
--******************************************************************************

        iis5ServiceTableEntries OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The number of entries in the table."
                ::=  {  iis5ServiceGroup 1  }

        iis5ServiceSummaryStatus OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    normal(1),
                    warning(2),
                    critical(3)
                }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Summary status of the service group."
                ::=  {  iis5ServiceGroup 2  }

        iis5ServicePollingInterval OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Poll interval for the service group in seconds."
                ::=  {  iis5ServiceGroup 3  }

        -- Start, start service
        -- Stop, stop service, don't reset autorestart
        -- Recycle, stop and then restart service
        -- Stop no AutoRestart - stop and turn off autorestart
        iis5ServiceControl OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    start(1),
                    stop(2),
                    recycle(3),
                    stop-noautorestart(4)
                }
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Start and stop all services in the service table."
                ::=  {  iis5ServiceGroup 4  }

        iis5ServiceWarningCount OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Number of services in warning state."
                ::=  {  iis5ServiceGroup 5  }

        iis5ServiceCriticalCount OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Number of services in critical state."
                ::=  {  iis5ServiceGroup 6  }

        iis5NumberOfPolls OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Number of times services have been polled"
               ::=  {  iis5ServiceGroup 7}

        iis5ServiceTable OBJECT-TYPE
                SYNTAX  SEQUENCE OF Iis5ServiceEntry
                ACCESS  not-accessible
                STATUS  mandatory
                DESCRIPTION
                "The list of all monitored services."
                ::=  {  iis5ServiceGroup 8}

	iis5ServiceGPEnaDis OBJECT-TYPE
		SYNTAX      INTEGER {enable(1), disable(2)}
		ACCESS      read-write
		STATUS      mandatory
		DESCRIPTION "Enable or disable for the Group."
		::= { iis5ServiceGroup 9 }
		
        iis5ServiceEntry OBJECT-TYPE
                 SYNTAX  Iis5ServiceEntry
                 ACCESS  not-accessible
                 STATUS  mandatory
                 DESCRIPTION
                 "The ServiceTable entry."
                 INDEX { iis5ServiceName }
                     ::=  { iis5ServiceTable 1 }

        Iis5ServiceEntry ::= SEQUENCE
        {
            iis5ServiceName           DisplayString,
            iis5ServiceState          INTEGER,
            iis5AutoRestart           INTEGER,
            iis5AutoReboot            INTEGER,
            iis5RestartAttempts       INTEGER,
            iis5RestartCount          INTEGER,
            iis5RebootAttempts        INTEGER,
            iis5RebootCount           INTEGER,
            iis5LocalAction           DisplayString,
            iis5TotalRestarts         INTEGER,
            iis5TotalReboots          INTEGER,
            iis5ServiceStateStatus    INTEGER,
            iis5ServiceWarnings       INTEGER,
            iis5ServiceCriticals      INTEGER,
            iis5StateIfStopped        INTEGER,
            iis5ServiceCtrl           INTEGER,
            iis5ServiceAvail          DisplayString,
            iis5ServiceRunningPolls   INTEGER,
	    iis5ServicePorts	      DisplayString,
            iis5ServiceType	      DisplayString,
	    iis5ServiceLagSetting     INTEGER,
	    iis5ServiceLagValue	      INTEGER,
            iis5ServiceInUse          INTEGER
        }

        iis5ServiceName OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The name of the service."
                ::=  {  iis5ServiceEntry 1   }

        iis5ServiceState OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    stopped(1),
                    started(2),
                    notinstalled(3),
                    actionpending(4)
                }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The state of the service."
                ::=  {  iis5ServiceEntry 2   }

        iis5AutoRestart OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    yes(1),
                    no(2)
                }
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Flag indicating whether to attempt to restart the service if it's down."
                ::=  {  iis5ServiceEntry 3   }

        iis5AutoReboot OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    yes(1),
                    no(2)
                }
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Flag indicating whether to attempt to reboot the host if service is down."
                ::=  {  iis5ServiceEntry 4   }

        iis5RestartAttempts OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Reboot the host after this many restart attempts are unsuccessful."
                ::=  {  iis5ServiceEntry 5   }

        iis5RestartCount OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Number of service restarts attempted since last successful service restart."
                ::=  {  iis5ServiceEntry 6   }

        iis5RebootAttempts OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Execute local action after this many reboot attempts are unsuccessful."
                ::=  {  iis5ServiceEntry 7   }

        iis5RebootCount OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Number of reboots attempted since last successful service restart."
                ::=  {  iis5ServiceEntry 8   }

        iis5LocalAction OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Local action to perform when reboot attempts exceeded."
                ::=  {  iis5ServiceEntry 9   }

        iis5TotalRestarts OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Total number of service restarts attempted."
                ::=  {  iis5ServiceEntry 10   }

        iis5TotalReboots OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Total number of host reboots attempted."
                ::=  {  iis5ServiceEntry 11   }

        iis5ServiceStateStatus OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    normal(1),
                    warning(2),
                    critical(3)
                }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Status of this service."
                ::=  {  iis5ServiceEntry 12   }

        iis5ServiceWarnings OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Number of warnings generated by this service."
                ::=  {  iis5ServiceEntry 13   }

        iis5ServiceCriticals OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Number of criticals generated by this service."
                ::=  {  iis5ServiceEntry 14   }

        iis5StateIfStopped OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    normal(1),
                    warning(2),
                    critical(3)
                }
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Service status reported if service is stopped."
                ::=  {  iis5ServiceEntry 15   }

        iis5ServiceCtrl OBJECT-TYPE
               SYNTAX  INTEGER {
                                start(1),
                                stop(2),
                                recycle(3),
                                stop-noautorestart(4)
                               }
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Start and stop this service."
               ::=  {  iis5ServiceEntry 16   }

        iis5ServiceAvail OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "% of time service available (running) = running/number of polls"
               ::=  {  iis5ServiceEntry 17  }

        iis5ServiceRunningPolls OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Number of polls showing service running(used for availability calc)."
                ::=  {  iis5ServiceEntry 18   }

        iis5ServicePorts OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Additional PORTS to check for the service."
                ::=  {  iis5ServiceEntry 19   }

	 iis5ServiceType OBJECT-TYPE
	        SYNTAX      DisplayString
	        ACCESS      read-only
	        STATUS      mandatory
	        DESCRIPTION "Type of Service (Server, Admin, FTP)."
	        ::= { iis5ServiceEntry 20 }
		
        iis5ServiceLagSetting OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Lag setting for the service."
                ::=  {  iis5ServiceEntry 21   }
		
        iis5ServiceLagValue OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Current Lag value for the service."
                ::=  {  iis5ServiceEntry 22   }

        iis5ServiceInUse OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    false(1),
                    true(2)
                }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "InUse indicator."
                ::=  {  iis5ServiceEntry 23   }

--******************************************************************************
--* BadLinkGroup - URL links not found by requestors                            *
--******************************************************************************

        iis5BLTableEntries OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The number of entries in the table."
                ::=  {  iis5BadLinkGroup 1   }


        iis5BLClearTable OBJECT-TYPE
               SYNTAX  INTEGER {
                                reset(1)
                               }
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Clear the table when set."
               ::=  {  iis5BadLinkGroup 2   }

        iis5BLSummaryStatus OBJECT-TYPE
               SYNTAX  INTEGER {
                                normal(1),
                                warning(2),
                                critical(3)
                               }
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Summary status for bad links."
               ::=  {  iis5BadLinkGroup 3   }

        iis5BLInterval OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Seconds to poll for changes."
               ::=  {  iis5BadLinkGroup 4   }

        iis5BLCircularBuffSize OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Max entries in the table (default 50)(Not a circular buffer)."
               ::=  {  iis5BadLinkGroup 5   }

        iis5BLWarningThreshold OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Number of requests on a row before warning is generated."
               ::=  {  iis5BadLinkGroup 6   }

        iis5BLCriticalThreshold OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Number of requests on a row before critical is generated."
               ::=  {  iis5BadLinkGroup 7   }

        iis5BLWarningCount OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "The number of entries in the table with a warning status."
               ::=  {  iis5BadLinkGroup 8   }

        iis5BLCriticalCount OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "The number of entries in the table with a critical status."
               ::=  {  iis5BadLinkGroup 9   }

        iis5BLWarningAction OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Command to be executed when warning threshold is crossed on a row."
               ::=  {  iis5BadLinkGroup 10   }

        iis5BLCriticalAction OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Command to be executed when critical threshold is crossed on a row."
               ::=  {  iis5BadLinkGroup 11   }


        iis5BLCreateReportURL OBJECT-TYPE
               SYNTAX  INTEGER {
                                Create(1)
                               }
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "When triggered, create report using path and file in BLReportURL."
               ::=  {  iis5BadLinkGroup 12   }

        iis5BLReportURL OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Path and filename for report; Relative to DocumentRoot unless starts with '/'."
               ::=  {  iis5BadLinkGroup 13   }

        iis5BLTable OBJECT-TYPE
               SYNTAX  SEQUENCE OF Iis5BLEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This table contains a list bad links."
               ::=  {  iis5BadLinkGroup 14  }

	iis5BLGPEnaDis OBJECT-TYPE
		SYNTAX      INTEGER {enable(1), disable(2)}
		ACCESS      read-write
		STATUS      mandatory
		DESCRIPTION "Enable or disable for the Group."
		::= { iis5BadLinkGroup 15 }

        iis5BLEntry OBJECT-TYPE
               SYNTAX  Iis5BLEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This is an entry in the BLTable"
               INDEX { iis5BLURL, iis5BLReferringURL }
               ::=  { iis5BLTable 1 }

        Iis5BLEntry ::= SEQUENCE
        {
           iis5BLURL                              DisplayString,
           iis5BLReferringURL                     DisplayString,
           iis5BLFirstAccessTS                    DisplayString,
           iis5BLlastAccessTS                     DisplayString,
           iis5BLNumRequests                      INTEGER,
           iis5BLStatus                           INTEGER
        }

        iis5BLURL OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Bad URL."
               ::=  {  iis5BLEntry 1   }

        iis5BLReferringURL OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Referring URL to Bad URL."
               ::=  {  iis5BLEntry 2   }

        iis5BLFirstAccessTS OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Time first bad access occurred (time row added to table)."
               ::=  {  iis5BLEntry 3   }

        iis5BLlastAccessTS OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Time of last access to bad URL."
               ::=  {  iis5BLEntry 4   }

        iis5BLNumRequests OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Count of requests."
               ::=  {  iis5BLEntry 5   }

        iis5BLStatus OBJECT-TYPE
               SYNTAX  INTEGER {
                                normal(1),
                                warning(2),
                                critical(3)
                               }
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Summary status for BL."
               ::=  {  iis5BLEntry 6   }


--******************************************************************************
--* TopURLGroup - URLs with highest access rates                             *
--******************************************************************************

        iis5TopURLTableEntries OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "The number of entries in the table."
               ::=  {  iis5TopURLGroup 1   }

        iis5TopURLClearTable OBJECT-TYPE
               SYNTAX  INTEGER {
                                reset(1)
                               }
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Clear the table when set."
               ::=  {  iis5TopURLGroup 2   }

        iis5TopURLInterval OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Seconds to poll for changes."
               ::=  {  iis5TopURLGroup 3   }

        iis5TopURLCircularBuffSize OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Max entries in the table (default 50)."
               ::=  {  iis5TopURLGroup 4   }


        iis5TopURLTable OBJECT-TYPE
               SYNTAX  SEQUENCE OF Iis5TopURLEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This table contains a list of the top URLs by hit count."
               ::=  {  iis5TopURLGroup 5 }

        iis5TopURLExclude OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Exclude URLs with these extensions."
               ::=  {  iis5TopURLGroup 6   }

        iis5TopURLExcludeReset OBJECT-TYPE
               SYNTAX  INTEGER {
                                reset(1)
                               }
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Reset exclude list to defaults."
               ::=  {  iis5TopURLGroup 7   }

	iis5TopURLGPEnaDis OBJECT-TYPE
		SYNTAX      INTEGER {enable(1), disable(2)}
		ACCESS      read-write
		STATUS      mandatory
		DESCRIPTION "Enable or disable for the Group."
		::= { iis5TopURLGroup 8 }

        iis5TopURLEntry OBJECT-TYPE
               SYNTAX  Iis5TopURLEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This is an entry in the TopURLTable"
               INDEX { iis5TopURLURL}
               ::=  { iis5TopURLTable 1 }

        Iis5TopURLEntry ::= SEQUENCE
        {
           iis5TopURLURL          DisplayString,
           iis5TopURLNumHits      INTEGER,
           iis5TopURLRank         INTEGER
        }

        iis5TopURLURL OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "URL."
               ::=  {  iis5TopURLEntry 1   }

        iis5TopURLNumHits OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Number of hits."
               ::=  {  iis5TopURLEntry 2   }

