               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Expression evaluation."
               ::=  {  iis5ExpressionEntry 3   }

        iis5ExpressionWarningThreshold OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Warning threshold"
               ::=  {  iis5ExpressionEntry 4   }

        iis5ExpressionCriticalThreshold OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Critical threshold"
               ::=  {  iis5ExpressionEntry 5   }

        iis5ExpressionWarningAction OBJECT-TYPE
