TELDAT-TRAP-R2-MIB DEFINITIONS ::= BEGIN


IMPORTS
	MODULE-IDENTITY,OBJECT-TYPE,OBJECT-IDENTITY,NOTIFICATION-TYPE,Integer32,IpAddress
		FROM SNMPv2-SMI
	DisplayString
		FROM SNMPv2-TC
	telEventTraps
		FROM TELDAT-MIB;


r2TrapsMIB MODULE-IDENTITY
    LAST-UPDATED "201401200930Z"
    ORGANIZATION "Teldat, S.A."
    CONTACT-INFO 
        "Teldat, S.A.
        Customer Service

        Postal: Parque Tecnologico de Madrid
                Tres Cantos 28760
                Madrid, SPAIN

        Tel.: +34 918 076 565

        http://www.teldat.com"
    DESCRIPTION
        "This Module Information Base file is defined for managing received
        SNMP notifications generated by the R2 event subsystem."
    REVISION "201401200930Z"
    DESCRIPTION "Initial implementation"
        ::= { telEventTraps 100 }


r2EventsGroup	OBJECT IDENTIFIER	::= { r2TrapsMIB 1 }

r2Notifications	OBJECT IDENTIFIER	::= { r2TrapsMIB 2 }
r2NotificationsPrefix	OBJECT IDENTIFIER	::= { r2Notifications 0 }

r2EventObjects1 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event R2.1
        Message generated for each event received on the state machine that
        shows the effect produced."
    ::= { r2EventsGroup 1 }

