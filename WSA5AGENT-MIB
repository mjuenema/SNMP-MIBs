		SYNTAX      INTEGER {Enable(1), Disable(2)}
		ACCESS      read-write
		STATUS      mandatory
		DESCRIPTION "Enable or disable for the Group."
		::= { wsa5PolledIISCounterGroup 9 }


        wsa5PolledIISCounterUpdate OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  write-only
               STATUS  mandatory
	       DESCRIPTION "Update this entry in the table."
               ::=  {  wsa5PolledIISCounterGroup 10  }
               
        wsa5PolledIISCounterEntry OBJECT-TYPE
                 SYNTAX  Wsa5PolledIISCounterEntry
                 ACCESS  not-accessible
                 STATUS  mandatory
                 DESCRIPTION
                 "The wsa5PolledIISCounterTable entry."
                 INDEX { wsa5IISDescription }
                     ::=  { wsa5PolledIISCounterTable 1 }

        Wsa5PolledIISCounterEntry ::= SEQUENCE {
            wsa5IISDescription          DisplayString,
            wsa5IISAvailable            INTEGER,
            wsa5IISWarningThreshold     DisplayString,
            wsa5IISCriticalThreshold	DisplayString,
            wsa5IISValue                DisplayString,
            wsa5IISWarningCount         INTEGER,
            wsa5IISCriticalCount        INTEGER,
            wsa5IISWarningAction        DisplayString,
            wsa5IISCriticalAction       DisplayString,
            wsa5IISStatus               INTEGER,
            wsa5IISEnableDisable        INTEGER,
            wsa5IISDescendingCounter    INTEGER,
            wsa5IISMinimum              DisplayString,
            wsa5IISMaximum              DisplayString,
            wsa5IISAverage              DisplayString,
            wsa5IISStdDev               DisplayString,
	    wsa5IISValueReported	INTEGER,
	    wsa5IISLagSetting		INTEGER,
	    wsa5IISLagValue		INTEGER
        }

        wsa5IISDescription OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The description of the polled counter."
                ::=  {  wsa5PolledIISCounterEntry 1   }

        wsa5IISAvailable OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    Available(1),
                    NotAvailable(2)
                }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
