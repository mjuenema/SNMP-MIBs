--MibName=wsa5Agent
--
--******************************************************************************
--*  Web Server Group Agent MIB                                                *
--******************************************************************************

WSA5AGENT-MIB DEFINITIONS ::= BEGIN

    IMPORTS
        OBJECT-TYPE, DisplayString
        FROM RFC1155-SMI;

-- Base OID  1.3.6.1.4.1.791.2.10.2.83

    cai         OBJECT IDENTIFIER ::= { enterprises 791 }

    caiSysMgr   OBJECT IDENTIFIER ::= { cai 2 }

    tng         OBJECT IDENTIFIER ::= { caiSysMgr 10 }

    unix-nt     OBJECT IDENTIFIER ::= { tng 2 }

    wsa5Agent   OBJECT IDENTIFIER ::= { unix-nt 83 }

    wsa5ServerGroup           OBJECT IDENTIFIER   ::=  {  wsa5Agent 1   }
    wsa5ServersGroup          OBJECT IDENTIFIER   ::=  {  wsa5Agent 2   }
    wsa5BadLinkGroup          OBJECT IDENTIFIER   ::=  {  wsa5Agent 3   }
    wsa5TopURLGroup           OBJECT IDENTIFIER   ::=  {  wsa5Agent 4   }
    wsa5URLAvailCounterGroup  OBJECT IDENTIFIER   ::=  {  wsa5Agent 5   }
    wsa5URLMntrGroup          OBJECT IDENTIFIER   ::=  {  wsa5Agent 6   }
    wsa5AvailIISCounterGroup  OBJECT IDENTIFIER   ::=  {  wsa5Agent 7   }
    wsa5AvailCounterGroup     OBJECT IDENTIFIER   ::=  {  wsa5Agent 8   }
    wsa5PolledCounterGroup    OBJECT IDENTIFIER   ::=  {  wsa5Agent 9   }
    wsa5DetailGroup    	      OBJECT IDENTIFIER   ::=  {  wsa5Agent 10  }
    wsa5AgentGroup            OBJECT IDENTIFIER   ::=  {  wsa5Agent 11  }
    wsa5PolledIISCounterGroup OBJECT IDENTIFIER   ::=  {  wsa5Agent 12  }
    wsa5SHGroup               OBJECT IDENTIFIER   ::=  {  wsa5Agent 13  }
    wsa5LocalActionGroup      OBJECT IDENTIFIER   ::=  {  wsa5Agent 14  }

--******************************************************************************
--* wsa5ServerGroup - Site and summary information                               *
--******************************************************************************

        wsa5ServerName OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The host name of the computer."
                ::=  {  wsa5ServerGroup 1   }

        wsa5ServerSummaryStatus OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    Ok(1),
                    Warning(2),
                    Critical(3)
                }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The status of the monitored server."
                ::=  {  wsa5ServerGroup 2   }

        wsa5ServerPollingInterval OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Poll interval for the server group in seconds."
                ::=  {  wsa5ServerGroup 3   }

        wsa5ServerResetCounts OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    Reset(1)
                }
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Resets all history counts to zero when set."
                ::=  {  wsa5ServerGroup 4   }

        wsa5ServerResetTS OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Time of last reset in ctime form."
                ::=  {  wsa5ServerGroup 5   }

        wsa5ServerResetTSEdit OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Time of last reset in display form."
                ::=  {  wsa5ServerGroup 6   }

        wsa5ServerResetDays OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Number of days between automatic history count resets."
                ::=  {  wsa5ServerGroup 7   }

        wsa5ServerPort OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Port the server is listening on."
                ::=  {  wsa5ServerGroup 9   }

        wsa5ServerLBHost OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Host name for Agent Collector."
                ::=  {  wsa5ServerGroup 10   }

        wsa5ServerLBPort OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Agent Collector name."
                ::=  {  wsa5ServerGroup 11   }

	wsa5ServerLBEnaDis OBJECT-TYPE
		SYNTAX      INTEGER {Enable(1), Disable(2)}
		ACCESS      read-write
		STATUS      mandatory
		DESCRIPTION "Enable or disable the Agent Collector."
		::= {wsa5ServerGroup 12}

        wsa5SuperAgentRequest OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Super Agent service request."
                ::=  {  wsa5ServerGroup 13   }

        wsa5ServerSetBaseLine OBJECT-TYPE
                SYNTAX  INTEGER {
                                set(1)
                               }
                ACCESS  write-only
                STATUS  mandatory
                DESCRIPTION
                "Immediately set Adaptive Configuration values."
                ::=  {  wsa5ServerGroup 14   }

        wsa5ServerBaseLineWarnWeight OBJECT-TYPE
                SYNTAX  INTEGER 
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Adaptive Configuration warning weighing factor."
                ::=  {  wsa5ServerGroup 15   }

        wsa5ServerBaseLineCritWeight OBJECT-TYPE
                SYNTAX  INTEGER 
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Adaptive Configuration critical weighing factor."
                ::=  {  wsa5ServerGroup 16   }

	wsa5ServerBaseLineEnaDis OBJECT-TYPE
		SYNTAX      INTEGER {disable(1), enable(2)}
		ACCESS      read-write
		STATUS      mandatory
		DESCRIPTION "Enable or disable Active Adaptive Configuration."
		::= {  wsa5ServerGroup 17  }

	wsa5ServerBaseLineTimer OBJECT-TYPE
		SYNTAX      INTEGER
		ACCESS      read-write
		STATUS      mandatory
		DESCRIPTION "Active Adaptive Configuration timer."
		::= {  wsa5ServerGroup 18  }
                
        wsa5ServerExportConfigSet OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Name of config to export to."
                ::=  {  wsa5ServerGroup 19   }
                
        wsa5ServerImportConfigSet OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Name of config to import from."
                ::=  {  wsa5ServerGroup 20   }


--******************************************************************************
--* wsa5ServersGroup - Servers                                                  *
--******************************************************************************

        wsa5ServersTableEntries OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The number of entries in the table."
                ::=  {  wsa5ServersGroup 1  }

        wsa5ServersSummaryStatus OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    Normal(1),
                    Warning(2),
                    Critical(3)
                }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Summary status of the servers group."
                ::=  {  wsa5ServersGroup 2  }

        wsa5ServersPollingInterval OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Poll interval for the servers group in seconds."
                ::=  {  wsa5ServersGroup 3  }

        wsa5ServersWarningCount OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The number of entries in the table with warning status."
                ::=  {  wsa5ServersGroup 4  }

        wsa5ServersCriticalCount OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The number of entries in the table with critical status."
                ::=  {  wsa5ServersGroup 5  }


        wsa5ServersTable OBJECT-TYPE
                SYNTAX  SEQUENCE OF Wsa5ServersEntry
                ACCESS  not-accessible
                STATUS  mandatory
                DESCRIPTION
                "The list of all monitored servers."
                ::=  {  wsa5ServersGroup 6}

        wsa5ServersEntry OBJECT-TYPE
                 SYNTAX  Wsa5ServersEntry
                 ACCESS  not-accessible
                 STATUS  mandatory
                 DESCRIPTION
                 "The wsa5ServersTable entry."
                 INDEX { wsa5ServersMID, wsa5ServersAID, wsa5ServersIID }
                     ::=  { wsa5ServersTable 1 }

        Wsa5ServersEntry ::= SEQUENCE
        {
            wsa5ServersName           DisplayString,
            wsa5ServersStatus         INTEGER,
            wsa5ServersSStatus        INTEGER,
            wsa5ServersDStatus        INTEGER,
            wsa5ServersHStatus        INTEGER,
            wsa5ServersEStatus        INTEGER,
            wsa5ServersWStatus        INTEGER,
            wsa5ServersWLocalAction   DisplayString,
            wsa5ServersCLocalAction   DisplayString,
            wsa5ServersABCfg          DisplayString,
            wsa5ServersHost           DisplayString,
            wsa5ServersInst           DisplayString,
            wsa5ServersPrevStatus     INTEGER,
            wsa5ServersState          INTEGER,
	    wsa5ServersDelete	     INTEGER,
	    wsa5ServersStoppedStatus  INTEGER,
            wsa5ServersMID            DisplayString,
            wsa5ServersAID            DisplayString,
            wsa5ServersIID            DisplayString
        }

        wsa5ServersName OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The name of the server."
                ::=  {  wsa5ServersEntry 1   }

        wsa5ServersStatus OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    Normal(1),
                    Warning(2),
                    Critical(3)
                }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Summary status of the server."
                ::=  {  wsa5ServersEntry 2   }

        wsa5ServersSStatus OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    Normal(1),
                    Warning(2),
                    Critical(3)
                }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Summary status of the server services."
                ::=  {  wsa5ServersEntry 3   }

        wsa5ServersDStatus OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    Normal(1),
                    Warning(2),
                    Critical(3)
                }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Summary status of the server disks."
                ::=  {  wsa5ServersEntry 4   }

        wsa5ServersHStatus OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    Normal(1),
                    Warning(2),
                    Critical(3)
                }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Summary status of the server health."
                ::=  {  wsa5ServersEntry 5   }

        wsa5ServersEStatus OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    Normal(1),
                    Warning(2),
                    Critical(3)
                }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Summary status of the server events."
                ::=  {  wsa5ServersEntry 6   }

        wsa5ServersWStatus OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    Normal(1),
                    Warning(2),
                    Critical(3)
                }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Summary status of the server WebCrawler."
                ::=  {  wsa5ServersEntry 7   }
		
        wsa5ServersWLocalAction OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Local action to perform for Warning status."
                ::=  {  wsa5ServersEntry 8   }

        wsa5ServersCLocalAction OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Local action to perform for Critical."
                ::=  {  wsa5ServersEntry 9   }

        wsa5ServersABCfg OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Abrowser configuration name for this server."
                ::=  {  wsa5ServersEntry 10   }

        wsa5ServersHost OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Abrowser configuration name for this server."
                ::=  {  wsa5ServersEntry 11   }

        wsa5ServersInst OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Abrowser configuration name for this server."
                ::=  {  wsa5ServersEntry 12   }

        wsa5ServersPrevStatus OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    Normal(1),
                    Warning(2),
                    Critical(3)
                }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "Previous Summary status of the server."
                ::=  {  wsa5ServersEntry 13   }

        wsa5ServersState OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    Stopped(1),
                    Started(2)
                }
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The state of the server agent."
                ::=  {  wsa5ServersEntry 14   }

        wsa5ServersDelete OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    Delete(1)
                }
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Delete this server."
                ::=  {  wsa5ServersEntry 15   }
		
        wsa5ServersStoppedStatus OBJECT-TYPE
                SYNTAX  INTEGER
                {
                    Normal(1),
                    Warning(2),
                    Critical(3)
                }
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                "Group status if the server agent is stopped."
                ::=  {  wsa5ServersEntry 16   }

        wsa5ServersMID OBJECT-TYPE
                SYNTAX  DisplayString
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
