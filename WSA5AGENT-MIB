--******************************************************************************
--* wsa5BadLinkGroup - URL links not found by requestors                        *
--******************************************************************************

        wsa5BLTableEntries OBJECT-TYPE
                SYNTAX  INTEGER
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                "The number of entries in the table."
                ::=  {  wsa5BadLinkGroup 1   }


        wsa5BLClearTable OBJECT-TYPE
               SYNTAX  INTEGER {
                                Reset(1)
                               }
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Clear the table when set."
               ::=  {  wsa5BadLinkGroup 2   }

        wsa5BLSummaryStatus OBJECT-TYPE
               SYNTAX  INTEGER {
                                Normal(1),
                                Warning(2),
                                Critical(3)
                               }
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Summary status for bad links."
               ::=  {  wsa5BadLinkGroup 3   }

        wsa5BLInterval OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Seconds to poll for changes."
               ::=  {  wsa5BadLinkGroup 4   }

        wsa5BLCircularBuffSize OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Max entries in the table (default 50)(Not a circular buffer)."
               ::=  {  wsa5BadLinkGroup 5   }

        wsa5BLWarningThreshold OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Number of requests on a row before warning is generated."
               ::=  {  wsa5BadLinkGroup 6   }

        wsa5BLCriticalThreshold OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Number of requests on a row before critical is generated."
               ::=  {  wsa5BadLinkGroup 7   }

        wsa5BLWarningCount OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "The number of entries in the table with a warning status."
               ::=  {  wsa5BadLinkGroup 8   }

        wsa5BLCriticalCount OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "The number of entries in the table with a critical status."
               ::=  {  wsa5BadLinkGroup 9   }

        wsa5BLWarningAction OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Command to be executed when warning threshold is crossed on a row."
               ::=  {  wsa5BadLinkGroup 10   }

        wsa5BLCriticalAction OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Command to be executed when critical threshold is crossed on a row."
               ::=  {  wsa5BadLinkGroup 11   }


        wsa5BLCreateReportURL OBJECT-TYPE
               SYNTAX  INTEGER {
                                Create(1)
                               }
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "When triggered, create report using path and file in wsa5BLReportURL."
               ::=  {  wsa5BadLinkGroup 12   }

        wsa5BLReportURL OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Path and filename for report; Relative to DocumentRoot unless starts with '/'."
               ::=  {  wsa5BadLinkGroup 13   }

        wsa5BLTable OBJECT-TYPE
               SYNTAX  SEQUENCE OF Wsa5BLEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This table contains a list bad links."
               ::=  {  wsa5BadLinkGroup 14  }

	wsa5BLGPEnaDis OBJECT-TYPE
		SYNTAX      INTEGER {Enable(1), Disable(2)}
		ACCESS      read-write
		STATUS      mandatory
		DESCRIPTION "Enable or disable for the Group."
		::= { wsa5BadLinkGroup 15 }

        wsa5BLEntry OBJECT-TYPE
               SYNTAX  Wsa5BLEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This is an entry in the wsa5BLTable"
               INDEX { wsa5BLURL, wsa5BLReferringURL }
               ::=  { wsa5BLTable 1 }

        Wsa5BLEntry ::= SEQUENCE
        {
           wsa5BLURL                              DisplayString,
           wsa5BLReferringURL                     DisplayString,
           wsa5BLFirstAccessTS                    DisplayString,
           wsa5BLlastAccessTS                     DisplayString,
           wsa5BLNumRequests                      INTEGER,
           wsa5BLStatus                           INTEGER
        }

        wsa5BLURL OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Bad URL."
               ::=  {  wsa5BLEntry 1   }

        wsa5BLReferringURL OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Referring URL to Bad URL."
               ::=  {  wsa5BLEntry 2   }

        wsa5BLFirstAccessTS OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Time first bad access occurred (time row added to table)."
               ::=  {  wsa5BLEntry 3   }

        wsa5BLlastAccessTS OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Time of last access to bad URL."
               ::=  {  wsa5BLEntry 4   }

        wsa5BLNumRequests OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Count of requests."
               ::=  {  wsa5BLEntry 5   }

        wsa5BLStatus OBJECT-TYPE
               SYNTAX  INTEGER {
                                Normal(1),
                                Warning(2),
                                Critical(3)
                               }
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Summary status for BL."
               ::=  {  wsa5BLEntry 6   }


--******************************************************************************
--* wsa5TopURLGroup - URLs with highest access rates                             *
--******************************************************************************

        wsa5TopURLTableEntries OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "The number of entries in the table."
               ::=  {  wsa5TopURLGroup 1   }

        wsa5TopURLClearTable OBJECT-TYPE
               SYNTAX  INTEGER {
                                Reset(1)
                               }
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Clear the table when set."
               ::=  {  wsa5TopURLGroup 2   }

        wsa5TopURLInterval OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Seconds to poll for changes."
               ::=  {  wsa5TopURLGroup 3   }

        wsa5TopURLCircularBuffSize OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Max entries in the table (default 50)."
               ::=  {  wsa5TopURLGroup 4   }


        wsa5TopURLTable OBJECT-TYPE
               SYNTAX  SEQUENCE OF Wsa5TopURLEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This table contains a list of the top URLs by hit count."
               ::=  {  wsa5TopURLGroup 5}

        wsa5TopURLExclude OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Exclude URLs with these extensions."
               ::=  {  wsa5TopURLGroup 6   }

        wsa5TopURLExcludeReset OBJECT-TYPE
               SYNTAX  INTEGER {
                                Reset(1)
                               }
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Reset exclude list to defaults."
               ::=  {  wsa5TopURLGroup 7   }

	wsa5TopURLGPEnaDis OBJECT-TYPE
		SYNTAX      INTEGER {Enable(1), Disable(2)}
		ACCESS      read-write
		STATUS      mandatory
		DESCRIPTION "Enable or disable for the Group."
		::= { wsa5TopURLGroup 8 }

        wsa5TopURLEntry OBJECT-TYPE
               SYNTAX  Wsa5TopURLEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This is an entry in the wsa5TopURLTable"
               INDEX { wsa5TopURLRank }
               ::=  { wsa5TopURLTable 1 }

        Wsa5TopURLEntry ::= SEQUENCE
        {
           wsa5TopURLURL          DisplayString,
           wsa5TopURLNumHits      INTEGER,
           wsa5TopURLRank         INTEGER
        }

        wsa5TopURLURL OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "URL."
               ::=  {  wsa5TopURLEntry 1   }

        wsa5TopURLNumHits OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Number of hits."
               ::=  {  wsa5TopURLEntry 2   }

