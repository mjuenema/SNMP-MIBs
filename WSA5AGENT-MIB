--******************************************************************************
--* Local action log - circular buffer of local actions performed              *
--* Dynamic get, not polled.                                                   *
--******************************************************************************

        wsa5LocalActionTableEntries OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "The number of entries in the local action log table."
               ::=  {  wsa5LocalActionGroup 1   }

        wsa5LocalActionCircularBuffSize OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Max entries in the wsa5LocalActionLogTable (default 50)."
               ::=  {  wsa5LocalActionGroup 2   }

        wsa5LocalActionRefresh OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Refresh the table starting with the most recent action."
               ::=  {  wsa5LocalActionGroup 3   }

        wsa5LocalActionStartDateTime OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Refresh the table starting at the specified date and time."
               ::=  {  wsa5LocalActionGroup 4   }

        wsa5LocalActionScrollDown OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Read the next older set of log entries into the table."
               ::=  {  wsa5LocalActionGroup 5   }

        wsa5LocalActionScrollUp OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Read the next newer set of log entries into the table."
               ::=  {  wsa5LocalActionGroup 6   }

        wsa5LocalActionLogTable OBJECT-TYPE
               SYNTAX  SEQUENCE OF Wsa5LocalActionLogEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This table contains a list of command actions taken in response to events."
               ::=  {  wsa5LocalActionGroup 7 }

        wsa5LocalActionLogEntry OBJECT-TYPE
                                SYNTAX  Wsa5LocalActionLogEntry
                                ACCESS  not-accessible
                                STATUS  mandatory
                                DESCRIPTION
                                "This is an entry in the wsa5LocalActionTable"
                                INDEX { wsa5LocalActionSequencer }
                                ::=  { wsa5LocalActionLogTable 1 }

        Wsa5LocalActionLogEntry ::= SEQUENCE
        {
           wsa5LocalActionSequencer               INTEGER,
           wsa5LocalActionTimestamp               DisplayString,
           wsa5LocalActionSeverity                INTEGER,
           wsa5LocalActionReason                  DisplayString,
           wsa5LocalActionCommand                 DisplayString
        }

        wsa5LocalActionSequencer OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Table sequencer"
               ::=  {  wsa5LocalActionLogEntry 1   }

        wsa5LocalActionTimestamp OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Time the command was executed (action taken)."
               ::=  {  wsa5LocalActionLogEntry 2   }

-- The Warning and Critical values for severity must match CLocalAction::enum Severity.
        wsa5LocalActionSeverity OBJECT-TYPE
               SYNTAX  INTEGER
               {
                   Normal(1),
                   Warning(2),
                   Critical(3)
               }
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "The severity of the event which triggered the action."
               ::=  {  wsa5LocalActionLogEntry 3   }

        wsa5LocalActionReason OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "The reason the action was taken."
               ::=  {  wsa5LocalActionLogEntry 4   }

        wsa5LocalActionCommand OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Command executed (action taken)."
               ::=  {  wsa5LocalActionLogEntry 5   }



--******************************************************************************
--*  Traps                                                                     *
--******************************************************************************

    statusNormal TRAP-TYPE
        ENTERPRISE wsa5Agent
        DESCRIPTION "Set status to normal"
        ::= 1

    statusWarning TRAP-TYPE
        ENTERPRISE wsa5Agent
        DESCRIPTION "Set status to warning"
        ::= 2

    statusCritical TRAP-TYPE
        ENTERPRISE wsa5Agent
        DESCRIPTION "Set status to critical"
        ::= 3

    statusDetailTrap TRAP-TYPE
        ENTERPRISE wsa5Agent
--        VARIABLES   {szStatus}
        DESCRIPTION "Status of group or table (row) changed"
        ::= 4
END
