--******************************************************************************
--* wsa5URLMntrGroup - Monitored URLs                                            *
--******************************************************************************

        wsa5URLMntrTableEntries OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "The number of entries in the table."
               ::=  {  wsa5URLMntrGroup 1   }

        wsa5URLMntrInterval OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Seconds between polls for the counter."
               ::=  {  wsa5URLMntrGroup 2   }

        wsa5URLMntrHitRate OBJECT-TYPE
               SYNTAX  INTEGER
               ACCESS  read-write
               STATUS  mandatory
               DESCRIPTION
               "Interval in seconds over which the counter is measured."
               ::=  {  wsa5URLMntrGroup 3   }

--      Add a counter to the table.  The value must include at least the first four of
--      the 6 items listed, separated by commas.
--      <wsa5URLAvailCounterDescription> is a value from wsa5URLAvailCounterDescription in wsa5URLAvailCounterTable.

        wsa5URLMntrAdd OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  write-only
               STATUS  mandatory
               DESCRIPTION
               "Add this entry to the table: <url>,<wsa5URLAvailCounterDescription>,<warning threshold>,<critical threshold>,<warning action>,<critical action>."
               ::=  {  wsa5URLMntrGroup 4   }

--      Delete a counter from the table.  The value must include the 2 items listed.
        wsa5URLMntrDelete OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  write-only
               STATUS  mandatory
               DESCRIPTION
               "Delete entry from the table: <wsa5URLMntrDesc>,<url>."
               ::=  {  wsa5URLMntrGroup 5   }

        wsa5URLMntrUpdate OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  write-only
               STATUS  mandatory
               DESCRIPTION
               "Reserved for future use."
               ::=  {  wsa5URLMntrGroup 6   }

        wsa5URLMntrSummaryStatus OBJECT-TYPE
               SYNTAX  INTEGER {
                                Normal(1),
                                Warning(2),
                                Critical(3)
                               }
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "Most severe status of all rows in the table."
               ::=  {  wsa5URLMntrGroup 7   }

        wsa5URLMntrTable OBJECT-TYPE
               SYNTAX  SEQUENCE OF Wsa5URLMntrEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This table contains a list of URLs to monitor."
               ::=  {  wsa5URLMntrGroup 8}

        wsa5URLMntrEntry OBJECT-TYPE
               SYNTAX  Wsa5URLMntrEntry
               ACCESS  not-accessible
               STATUS  mandatory
               DESCRIPTION
               "This is an entry in the wsa5URLMntrTable"
               INDEX { wsa5URLMntrURL, wsa5URLMntrDesc}
               ::=  { wsa5URLMntrTable 1 }

        Wsa5URLMntrEntry ::= SEQUENCE
        {
           wsa5URLMntrURL                       DisplayString,
           wsa5URLMntrDesc                      DisplayString,
           wsa5URLMntrValue                     DisplayString,
           wsa5URLMntrWarningThreshold          INTEGER,
           wsa5URLMntrCriticalThreshold         INTEGER,
           wsa5URLMntrWarningAction             DisplayString,
           wsa5URLMntrCriticalAction            DisplayString,
           wsa5URLMntrStatus                    INTEGER,
           wsa5URLMntrEnableDisable             INTEGER,
           wsa5URLMntrDescending                INTEGER,
           wsa5URLMntrReset                     INTEGER,
           wsa5URLMntrResetTime                 DisplayString,
	   wsa5URLMntrValueReported		INTEGER,
	   wsa5URLMntrLagSetting		INTEGER,
	   wsa5URLMntrLagValue			INTEGER
        }

        wsa5URLMntrURL OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
               "URL to monitor."
               ::=  {  wsa5URLMntrEntry 1   }

        wsa5URLMntrDesc OBJECT-TYPE
               SYNTAX  DisplayString
               ACCESS  read-only
               STATUS  mandatory
               DESCRIPTION
