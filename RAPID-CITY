        FROM SNMPv2-TC
    InterfaceIndex
        FROM IF-MIB
    SnmpAdminString,SnmpSecurityLevel,SnmpSecurityModel
        FROM SNMP-FRAMEWORK-MIB
    Ipv6Address
        FROM IPV6-TC;

