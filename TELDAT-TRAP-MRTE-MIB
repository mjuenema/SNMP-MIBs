mrteNotification1 NOTIFICATION-TYPE
    OBJECTS { 
              mrte1_source-address, 
              mrte1_destination-address,
              mrte1_incoming-interface,
              mrte1_ttl 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event MRTE.1

        Message: 'forward packet from $1 to $2, received at incoming interface $3 ttl $4'

        Parameters list:
              $1: mrte1_source-address
              $2: mrte1_destination-address
              $3: mrte1_incoming-interface
              $4: mrte1_ttl

        Multicast packet to be forwarded."
    ::= { mrteNotificationsPrefix 1 }

mrteNotification2 NOTIFICATION-TYPE
    OBJECTS { 
              mrte2_source-address, 
              mrte2_destination-address 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event MRTE.2

        Message: 'MFC entry not found from $1 to $2. Trying to create it.'

        Parameters list:
              $1: mrte2_source-address
              $2: mrte2_destination-address

        MFC entry not found at first lookup"
    ::= { mrteNotificationsPrefix 2 }

mrteNotification3 NOTIFICATION-TYPE
    OBJECTS { 
              mrte3_source-address, 
              mrte3_destination-address,
              mrte3_outgoing-interface 
            }
    STATUS current
    DESCRIPTION
