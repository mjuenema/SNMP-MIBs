TELDAT-TRAP-MRTE-MIB DEFINITIONS ::= BEGIN


IMPORTS
	MODULE-IDENTITY,OBJECT-TYPE,OBJECT-IDENTITY,NOTIFICATION-TYPE,Integer32,IpAddress
		FROM SNMPv2-SMI
	DisplayString
		FROM SNMPv2-TC
	telEventTraps
		FROM TELDAT-MIB;


mrteTrapsMIB MODULE-IDENTITY
    LAST-UPDATED "201401200930Z"
    ORGANIZATION "Teldat, S.A."
    CONTACT-INFO 
        "Teldat, S.A.
        Customer Service

        Postal: Parque Tecnologico de Madrid
                Tres Cantos 28760
                Madrid, SPAIN

        Tel.: +34 918 076 565

        http://www.teldat.com"
    DESCRIPTION
        "This Module Information Base file is defined for managing received
        SNMP notifications generated by the MRTE event subsystem."
    REVISION "201401200930Z"
    DESCRIPTION "Initial implementation"
        ::= { telEventTraps 239 }


mrteEventsGroup	OBJECT IDENTIFIER	::= { mrteTrapsMIB 1 }

mrteNotifications	OBJECT IDENTIFIER	::= { mrteTrapsMIB 2 }
mrteNotificationsPrefix	OBJECT IDENTIFIER	::= { mrteNotifications 0 }

mrteEventObjects1 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event MRTE.1
        Multicast packet to be forwarded."
    ::= { mrteEventsGroup 1 }

