    -- The History Vlan Port Control Group

          rcRateLimitVlanPortHistoryControlTable OBJECT-TYPE
          SYNTAX    SEQUENCE OF RcRateLimitVlanPortHistoryControlEntry
          MAX-ACCESS  not-accessible
          STATUS      current
          DESCRIPTION
          "A table of vlan Port History Control."
          ::= { rcRateLimit 7}

          rcRateLimitVlanPortHistoryControlEntry OBJECT-TYPE
          SYNTAX      RcRateLimitVlanPortHistoryControlEntry
          MAX-ACCESS  not-accessible
          STATUS      current
          DESCRIPTION
          "Table entry for vlan Port History Control."
          INDEX { rcRateLimitVlanPortHistoryControlVlanIndex,  rcRateLimitVlanPortHistoryControlPortIndex}
          ::= { rcRateLimitVlanPortHistoryControlTable 1 }
          
          RcRateLimitVlanPortHistoryControlEntry ::= SEQUENCE 
          {
              rcRateLimitVlanPortHistoryControlVlanIndex     Counter,
              rcRateLimitVlanPortHistoryControlPortIndex     Counter,
              rcRateLimitVlanPortHistoryControlSVlan     INTEGER (0..4094),
              rcRateLimitVlanPortHistoryControlCVlan     INTEGER (0..4094),
              rcRateLimitVlanPortHistoryControlSCoS      INTEGER (0..8),
              rcRateLimitVlanPortHistoryControlCCoS      INTEGER (0..8),
              rcRateLimitVlanPortHistoryControlPort      INTEGER,
              rcRateLimitVlanPortHistoryControlStatus    EntryStatus
          }
          
              rcRateLimitVlanPortHistoryControlVlanIndex OBJECT-TYPE
              SYNTAX Counter
              ACCESS read-create
              STATUS current
              DESCRIPTION
                  "An index that uniquely identifies an entry in the
                  historyControl table.  Each such entry defines a
                  set of samples at a particular interval for an
                  interface on the device."
              ::= { rcRateLimitVlanPortHistoryControlEntry 1 }
              
              rcRateLimitVlanPortHistoryControlPortIndex OBJECT-TYPE
              SYNTAX Counter
              ACCESS read-create
              STATUS current
              DESCRIPTION
                  "An index that uniquely identifies an entry in the
                  historyControl table.  Each such entry defines a
                  set of samples at a particular interval for an
                  interface on the device."
              ::= { rcRateLimitVlanPortHistoryControlEntry 2 }
              
              rcRateLimitVlanPortHistoryControlSVlan OBJECT-TYPE
              SYNTAX INTEGER (0..4094)
              ACCESS read-only
              STATUS current
              DESCRIPTION
                  "SVlan ID."
              ::= { rcRateLimitVlanPortHistoryControlEntry 3 }
              
              rcRateLimitVlanPortHistoryControlCVlan OBJECT-TYPE
              SYNTAX INTEGER (0..4094)
              ACCESS read-only
              STATUS current
              DESCRIPTION
                  "CVlan ID."
              ::= { rcRateLimitVlanPortHistoryControlEntry 4 }
              
              rcRateLimitVlanPortHistoryControlSCoS OBJECT-TYPE
              SYNTAX INTEGER (0..8)
              ACCESS read-only
              STATUS current
              DESCRIPTION
                  "SCoS value."
              ::= { rcRateLimitVlanPortHistoryControlEntry 5 }
              
              rcRateLimitVlanPortHistoryControlCCoS OBJECT-TYPE
              SYNTAX INTEGER (0..8)
              ACCESS read-only
              STATUS current
              DESCRIPTION
                  "CCoS value."
              ::= { rcRateLimitVlanPortHistoryControlEntry 6 }
              
              rcRateLimitVlanPortHistoryControlPort OBJECT-TYPE
              SYNTAX INTEGER
              ACCESS read-write
              STATUS current
              DESCRIPTION
                  "This object identifies the source of the data for
                  which historical data was collected and
                  placed in a media-specific table on behalf of this
                  historyControlEntry.  This source can be any
                  interface on this device.  In order to identify
                  a particular interface, this object shall identify
                  the instance of the ifIndex object, defined
                  in  RFC 1213 and RFC 1573 [4,6], for the desired
                  interface.  For example, if an entry were to receive
                  data from interface #1, this object would be set
                  to ifIndex.1.

                  The statistics in this group reflect all packets
                  on the local network segment attached to the
                  identified interface.

                  An agent may or may not be able to tell if fundamental
                  changes to the media of the interface have occurred
                  and necessitate an invalidation of this entry.  For
                  example, a hot-pluggable ethernet card could be
                  pulled out and replaced by a token-ring card.  In
                  such a case, if the agent has such knowledge of the
                  change, it is recommended that it invalidate this
                  entry.

                  This object may not be modified if the associated
                  historyControlStatus object is equal to valid(1)."
              ::= { rcRateLimitVlanPortHistoryControlEntry 7 }  
              
              rcRateLimitVlanPortHistoryControlStatus OBJECT-TYPE
              SYNTAX EntryStatus
              ACCESS read-write
              STATUS current
              DESCRIPTION
                  "The status of this historyControl entry.

                  Each instance of the media-specific table associated
                  with this historyControlEntry will be deleted by the
                  agent if this historyControlEntry is not equal to
                  valid(1)."
              ::= { rcRateLimitVlanPortHistoryControlEntry 8 }


          -- The History Vlan Port History statics Group

          rcRateLimitVlanPortHistoryTable OBJECT-TYPE
          SYNTAX    SEQUENCE OF RcRateLimitVlanPortHistoryEntry
          MAX-ACCESS  not-accessible
          STATUS      current
          DESCRIPTION
          "A table of vlan Port History statics."
          ::= { rcRateLimit 8}

          rcRateLimitVlanPortHistoryEntry OBJECT-TYPE
          SYNTAX      RcRateLimitVlanPortHistoryEntry
          MAX-ACCESS  not-accessible
          STATUS      current
          DESCRIPTION
          "Table entry for vlan Port History Control."
          INDEX { rcRateLimitVlanPortHistoryVlanIndex,  rcRateLimitVlanPortHistoryPortIndex, rcRateLimitVlanPortHistorySampleIndex}
          ::= { rcRateLimitVlanPortHistoryTable 1 }
          
          RcRateLimitVlanPortHistoryEntry ::= SEQUENCE 
          {
              rcRateLimitVlanPortHistoryVlanIndex     Counter,
              rcRateLimitVlanPortHistoryPortIndex     Counter,
              rcRateLimitVlanPortHistorySampleIndex              INTEGER (1..4294967295),
              rcRateLimitVlanPortHistoryHistoryIntervalStart     TimeTicks,
              rcRateLimitVlanPortHistoryIngressInPkt	           Counter64,             
              rcRateLimitVlanPortHistoryIngressOutPkt            Counter64,
              rcRateLimitVlanPortHistoryIngressInByte	           Counter64,              
              rcRateLimitVlanPortHistoryIngressOutBytes          Counter64,
              rcRateLimitVlanPortHistoryEgressInPkt	             Counter64,             
              rcRateLimitVlanPortHistoryEgressOutPkt             Counter64,
              rcRateLimitVlanPortHistoryEgressInByte	           Counter64,               
              rcRateLimitVlanPortHistoryEgressOutBytes           Counter64
          }
          
          
              rcRateLimitVlanPortHistoryVlanIndex OBJECT-TYPE
              SYNTAX Counter
              ACCESS read-only
              STATUS current
              DESCRIPTION
                  "An index that uniquely identifies an entry in the
                  historyControl table.  Each such entry defines a
                  set of samples at a particular interval for an
                  interface on the device."
              ::= { rcRateLimitVlanPortHistoryEntry 1 }
              
              rcRateLimitVlanPortHistoryPortIndex OBJECT-TYPE
              SYNTAX Counter
              ACCESS read-only
              STATUS current
              DESCRIPTION
                  "An index that uniquely identifies an entry in the
                  historyControl table.  Each such entry defines a
                  set of samples at a particular interval for an
                  interface on the device."
              ::= { rcRateLimitVlanPortHistoryEntry 2 }
          
          rcRateLimitVlanPortHistorySampleIndex OBJECT-TYPE
              SYNTAX INTEGER (1..2147483647)
              ACCESS read-only
              STATUS current
              DESCRIPTION
                  "An index that uniquely identifies the particular
                  sample this entry represents among all samples
                  associated with the same historyControlEntry.
                  This index starts at 1 and increases by one
                  as each new sample is taken."
              ::= { rcRateLimitVlanPortHistoryEntry 3 }
          
          rcRateLimitVlanPortHistoryHistoryIntervalStart OBJECT-TYPE
              SYNTAX TimeTicks
              ACCESS read-only
              STATUS current
              DESCRIPTION
                  "The value of sysUpTime at the start of the interval
                  over which this sample was measured.  If the probe
                  keeps track of the time of day, it should start
                  the first sample of the history at a time such that
                  when the next hour of the day begins, a sample is
                  started at that instant.  Note that following this
                  rule may require the probe to delay collecting the
                  first sample of the history, as each sample must be
                  of the same interval.  Also note that the sample which
                  is currently being collected is not accessible in this
                  table until the end of its interval."
              ::= { rcRateLimitVlanPortHistoryEntry 4 }
              
              rcRateLimitVlanPortHistoryIngressInPkt OBJECT-TYPE
              SYNTAX Counter64
              ACCESS read-only
              STATUS current
              DESCRIPTION
                  "The total number of IngressInPkt."
              ::= { rcRateLimitVlanPortHistoryEntry 5 }
              
              rcRateLimitVlanPortHistoryIngressOutPkt OBJECT-TYPE
              SYNTAX Counter64
              ACCESS read-only
              STATUS current
              DESCRIPTION
                  "The total number of IngressOutPkt."
              ::= { rcRateLimitVlanPortHistoryEntry 6 }
              
              rcRateLimitVlanPortHistoryIngressInByte OBJECT-TYPE
              SYNTAX Counter64
              ACCESS read-only
              STATUS current
              DESCRIPTION
                  "The total number of IngressInByte."
              ::= { rcRateLimitVlanPortHistoryEntry 7 }
              
              rcRateLimitVlanPortHistoryIngressOutBytes OBJECT-TYPE
              SYNTAX Counter64
              ACCESS read-only
              STATUS current
              DESCRIPTION
                  "The total number of IngressOutBytes."
              ::= { rcRateLimitVlanPortHistoryEntry 8 }
              
              rcRateLimitVlanPortHistoryEgressInPkt OBJECT-TYPE
              SYNTAX Counter64
              ACCESS read-only
              STATUS current
              DESCRIPTION
                  "The total number of EgressInPkt."
              ::= { rcRateLimitVlanPortHistoryEntry 9 }
              
              rcRateLimitVlanPortHistoryEgressOutPkt OBJECT-TYPE
              SYNTAX Counter64
              ACCESS read-only
              STATUS current
              DESCRIPTION
                  "The total number of EgressOutPkt."
              ::= { rcRateLimitVlanPortHistoryEntry 10 }
              
              rcRateLimitVlanPortHistoryEgressInByte OBJECT-TYPE
              SYNTAX Counter64
              ACCESS read-only
              STATUS current
              DESCRIPTION
                  "The total number of EgressInByte."
              ::= { rcRateLimitVlanPortHistoryEntry 11 }
              
              rcRateLimitVlanPortHistoryEgressOutBytes OBJECT-TYPE
              SYNTAX Counter64
              ACCESS read-only
              STATUS current
              DESCRIPTION
                  "The total number of EgressOutBytes."
              ::= { rcRateLimitVlanPortHistoryEntry 12 }

