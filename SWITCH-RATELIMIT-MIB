    ::= { rcRateLimitVlanPortEntry 5 }    
    
rcRateLimitVlanPortEbs   OBJECT-TYPE
    SYNTAX    INTEGER
    UNITS     "kB"                
    ACCESS    read-write
    STATUS    current
    DESCRIPTION
      "The value is the input burst(kB) of the vlan on each port."    
    ::= { rcRateLimitVlanPortEntry 6 }
 
