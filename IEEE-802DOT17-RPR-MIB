        ::= { rprFairnessEntry 20 }

    -- 1.0.8802.17.1.1.1.2.2.1.21
    rprFairnessChanges OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
