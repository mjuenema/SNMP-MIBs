        REFERENCE
            "IEEE 802.17 Subclause 10.2.2, stqLowWatermark"
        ::= { rprFairnessEntry 19 }

    -- 1.0.8802.17.1.1.1.2.2.1.20
    rprFairnessLastChange OBJECT-TYPE
        SYNTAX TimeStamp
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The value of sysUpTime at the time when any of the following
