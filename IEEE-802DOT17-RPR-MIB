    -- 1.0.8802.17.1.1.1.3.1.1.7
    rprSpanCurrentInUcastClassBEirFrames OBJECT-TYPE
        SYNTAX HCPerfCurrentCount
        MAX-ACCESS read-only
        STATUS current
