tipsNotification1 NOTIFICATION-TYPE
    OBJECTS { 
              tips1_source_ip, 
              tips1_destination_ip 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event TIPS.1

        Message: 'IP packet beeing processed $1->$2'

        Parameters list:
              $1: tips1_source_ip
              $2: tips1_destination_ip

        packet processed"
    ::= { tipsNotificationsPrefix 1 }

