	SYNTAX      VlanIdOrNone
	MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "The PrimaryVlanId. default the minimum vlan in the vlanlist.
            When rcCfmMaExOamType is ether-oam(1) allowed to be configurated."    
    DEFVAL { 0 }
	::= { rcCfmEthServiceQuickCreateEntry 9 }   

