--CFM multicast  loopback result Table 
--------------------------------------------------------------------------------
rcCfmMulticastLbResultTable 	OBJECT-TYPE
    SYNTAX      SEQUENCE OF RcCfmMulticastLbResultEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "CFM Multicast Loopback Result Table "  
    ::= { rcCfm 26 }
    
rcCfmMulticastLbResultEntry          			OBJECT-TYPE
	SYNTAX      RcCfmMulticastLbResultEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry in the rcCfmMulticastLbResultTable.  "
    INDEX	{ dot1agCfmMdIndex, 
    		  dot1agCfmMaIndex,
    		  dot1agCfmMepIdentifier,
    		  rcCfmMcastLbResultIndex
    		}
    ::= { rcCfmMulticastLbResultTable 1 }   
    
RcCfmMulticastLbResultEntry ::= SEQUENCE {
    rcCfmMcastLbResultIndex         Unsigned32 (1..256),
	rcCfmMcastLbResultRemoteMepId   Unsigned32 (0..8191),
	rcCfmMcastLbResultRecvPort      Unsigned32 (0..65535),
	rcCfmMcastLbResultMacAddress	MacAddress,
	rcCfmMcastLbResultRtt           Unsigned32(0..5000)	
}                    

rcCfmMcastLbResultIndex    OBJECT-TYPE 
	SYNTAX      Unsigned32 (1..256)
	MAX-ACCESS  not-accessible
	STATUS      current
    DESCRIPTION
            "The fourth index to the multicast loopback table."
	::= { rcCfmMulticastLbResultEntry 1 }   
	
rcCfmMcastLbResultRemoteMepId    OBJECT-TYPE 
	SYNTAX      Unsigned32 (0..8191)
	MAX-ACCESS  read-only
	STATUS      current
    DESCRIPTION
            "The remote mep id."
	::= { rcCfmMulticastLbResultEntry 2 }  

rcCfmMcastLbResultRecvPort    OBJECT-TYPE 
	SYNTAX      Unsigned32 (0..65535)
	MAX-ACCESS  read-only
	STATUS      current
    DESCRIPTION
            "The lbr reveived port,0 means no lbr received."
	::= { rcCfmMulticastLbResultEntry 3 } 
	  
rcCfmMcastLbResultMacAddress    OBJECT-TYPE 
	SYNTAX      MacAddress
	MAX-ACCESS  read-only
	STATUS      current
    DESCRIPTION
            "The remote mep mac address,0000.0000.0000 means no remote mep mac."
	::= { rcCfmMulticastLbResultEntry 4 }  

rcCfmMcastLbResultRtt    OBJECT-TYPE 
	SYNTAX      Unsigned32
	MAX-ACCESS  read-only
	STATUS      current
    DESCRIPTION
            "lb rtt time,0 means rtt <1ms,5000 means timeout."
	::= { rcCfmMulticastLbResultEntry 5 }  

--------------------------------------------------------------------------------
--CFM MEP Extended Mpls-tp Loopback Table
--------------------------------------------------------------------------------
rcCfmMepExMplstpPingTable 	OBJECT-TYPE
    SYNTAX      SEQUENCE OF RcCfmMepExMplstpPingEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "CFM MEP Extended Mpls-tp Loopback Table "  
    ::= { rcCfm 27 }
    
rcCfmMepExMplstpPingEntry          			OBJECT-TYPE
	SYNTAX      RcCfmMepExMplstpPingEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry in the rcCfmMepExMplstpPingTable.  "
    INDEX	{ dot1agCfmMdIndex, 
    		  dot1agCfmMaIndex,
    		  dot1agCfmMepIdentifier
    		}
    ::= { rcCfmMepExMplstpPingTable 1 }   
    
RcCfmMepExMplstpPingEntry ::= SEQUENCE {
    rcCfmMepExMplstpPingType        INTEGER,
	rcCfmMepExMplstpPingDestMepId   Unsigned32 (0..8191),
	rcCfmMepExMplstpPingMipId	    Unsigned32 (0..8191),
	rcCfmMepExMplstpPingDirection   INTEGER,
	rcCfmMepExMplstpPingTtl         Unsigned32 (1..255),
	rcCfmMepExMplstpPingTimeout     Unsigned32 (1..60),  
	rcCfmMepExMplstpPingLbmPadType  INTEGER, 
	rcCfmMepExMplstpPingDestMipICC     OCTET STRING (SIZE(1..6)),	
	rcCfmMepExMplstpPingDestMipNodeId  Unsigned32,
	rcCfmMepExMplstpPingDestMipIfNum   Unsigned32, 
    rcCfmMepExMplstpPingDestMipMode    INTEGER
}                    

rcCfmMepExMplstpPingType    OBJECT-TYPE 
	SYNTAX      INTEGER{
                        cfm_mpls_tp_ping_none(0),
                        cfm_mpls_tp_ping_mep(1),
                        cfm_mpls_tp_ping_mip(2),
                        cfm_mpls_tp_ping_direction(3)
                }
	MAX-ACCESS  read-write
	STATUS      current
    DESCRIPTION
            "The type of the mpls-tp ping action."  
    DEFVAL {cfm_mpls_tp_ping_none}
	::= { rcCfmMepExMplstpPingEntry 1 }   
	
rcCfmMepExMplstpPingDestMepId    OBJECT-TYPE 
	SYNTAX      Unsigned32 (0..8191)
	MAX-ACCESS  read-write
	STATUS      current
    DESCRIPTION
            "The mep Identifier of another MEP in the same Ma to which the LBM
            is to be sent.This address will be used if the value of the column 
            rcCfmMepExMplstpPingType is equal to 1."
	::= { rcCfmMepExMplstpPingEntry 2 }  

rcCfmMepExMplstpPingMipId    OBJECT-TYPE 
	SYNTAX      Unsigned32 (0..8191)
	MAX-ACCESS  read-write
	STATUS      deprecated
    DESCRIPTION
            "The destination mip Identifier to which the LBM is to be sent.
             This address will be used if the value of the column 
             rcCfmMepExMplstpPingType is equal to 2."
	::= { rcCfmMepExMplstpPingEntry 3 } 
	  
rcCfmMepExMplstpPingDirection    OBJECT-TYPE 
	SYNTAX      INTEGER{
	                    cfm_ping_none(0), 
	                    cfm_ping_ingress(1),
                        cfm_ping_egress(2)
                }
	MAX-ACCESS  read-write
	STATUS      current
    DESCRIPTION
            "Set the direction of the ping action which used only in mpls layer.
             This address will be used if the value of the column 
             rcCfmMepExMplstpPingType is equal to 3."  
    DEFVAL {cfm_ping_none}
	::= { rcCfmMepExMplstpPingEntry 4 }  

rcCfmMepExMplstpPingTtl    OBJECT-TYPE 
	SYNTAX      Unsigned32(1..255)
	MAX-ACCESS  read-write
	STATUS      current 
    DESCRIPTION
            "The LBM TTL field. Default value, if not specified, is 64.The TTL field
            indicates the number of hops remaining to the LBM. Decremented by 1 when
            each LBM handler. The value returned in the LBR is one less
            than that received in the LBM."   
    DEFVAL {64}
	::= { rcCfmMepExMplstpPingEntry 5 }     
	
	rcCfmMepExMplstpPingTimeout    OBJECT-TYPE 
	SYNTAX      Unsigned32(1..60)
	MAX-ACCESS  read-write
	STATUS      current
    DESCRIPTION
            "The time wait for LBR once sending LBM,when out of time, it indicates that 
            mpls-tp ping operation has falied."  
    DEFVAL {5}        
	::= { rcCfmMepExMplstpPingEntry 6 }  
	
	rcCfmMepExMplstpPingLbmPadType    OBJECT-TYPE 
	SYNTAX      INTEGER{
                         cfm_mpls_tp_ping_invalid(0),
                         cfm_mpls_tp_ping_prbs(1),
                         cfm_mpls_tp_ping_prbs_crc(2),  
                         cfm_mpls_tp_ping_null(3),
                         cfm_mpls_tp_ping_null_crc(4)
                }
	MAX-ACCESS  read-write
	STATUS      current
    DESCRIPTION
            "The tlv type of lbm packet to send.when the value equals to cfm_mpls_tp_ping_invalid,
            the tlv type is data tlv"  
    DEFVAL {cfm_mpls_tp_ping_invalid}
	::= { rcCfmMepExMplstpPingEntry 7 }    
rcCfmMepExMplstpPingDestMipICC   OBJECT-TYPE 
	SYNTAX      OCTET STRING (SIZE(1..6))
    MAX-ACCESS  read-write
	STATUS      current
    DESCRIPTION
            "The ICC of MPLS-TP LB target MIP."
	::= {rcCfmMepExMplstpPingEntry 8}

rcCfmMepExMplstpPingDestMipNodeId   OBJECT-TYPE 
	SYNTAX      Unsigned32 
    MAX-ACCESS  read-write
	STATUS      current
    DESCRIPTION
            "The Node-ID of MPLS-TP LB target MIP."
	::= {rcCfmMepExMplstpPingEntry 9}

rcCfmMepExMplstpPingDestMipIfNum   OBJECT-TYPE 
	SYNTAX      Unsigned32 
    MAX-ACCESS  read-write
	STATUS      current
    DESCRIPTION
            "The If-Num of MPLS-TP LB target MIP."
	::= {rcCfmMepExMplstpPingEntry 10}

rcCfmMepExMplstpPingDestMipMode   OBJECT-TYPE 
	SYNTAX      INTEGER{
                         mpls_tp_mip_mode_none(0),
                         mpls_tp_mip_mode_node(1),
                         mpls_tp_mip_mode_interface(2)
                }
    MAX-ACCESS  read-write
	STATUS      current
    DESCRIPTION
            "The Mode of MPLS-TP LB target MIP."
	::= {rcCfmMepExMplstpPingEntry 11}
	
