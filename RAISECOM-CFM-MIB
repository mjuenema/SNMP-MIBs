--Y.1731 Ma Extern LCK table
rcCfmMaExLckTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF RcCfmMaExLckEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Y.1731 MA Extended LCK table"
    ::= { rcCfm 22 }
    
rcCfmMaExLckEntry OBJECT-TYPE
	  SYNTAX      RcCfmMaExLckEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry in the rcCfmMaExLckTable. "
    INDEX	{ dot1agCfmMdIndex, 
    		  dot1agCfmMaIndex
    		}
    ::= { rcCfmMaExLckTable 1 } 
    
    RcCfmMaExLckEntry ::= SEQUENCE 
    {
		rcCfmMaExLckLevelAdmin Dot1agCfmMDLevelOrNone,  
		rcCfmMaExLckLevelOper Dot1agCfmMDLevelOrNone,
		rcCfmMaExLckPeriod INTEGER,
		rcCfmMaExLckStatus INTEGER,
		rcCfmMaExLckAge Unsigned32,
		rcCfmMaExLckStatisticsTx Unsigned32,
		rcCfmMaExLckStatisticsRx Unsigned32,
		rcCfmMaExLckLspName      OCTET STRING (SIZE (0..32)),  
		rcCfmMaExLckVcid         INTEGER,
		rcCfmMaExLckVlan         VlanIdOrNone,
        rcCfmMaExLckPwPeerAddrType   InetAddressType,
        rcCfmMaExLckPwPeerAddr       InetAddress  
    }
 
rcCfmMaExLckLevelAdmin OBJECT-TYPE
    SYNTAX      Dot1agCfmMDLevelOrNone
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
       "Set the level of AIS in this MA,then send ais 
       packet when some errors occur."
    DEFVAL{-1}
    ::= { rcCfmMaExLckEntry 1 }

