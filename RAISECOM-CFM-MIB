                  rcCfmMaExEgressLspId,       
                  rcCfmMaExVcid,               
                  rcCfmMaExPwPeerAddrType,  
                  rcCfmMaExPwPeerAddr,      
                  rcCfmMaExSectionPort,      
                  rcCfmMaExSncLsridAddrType, 
                  rcCfmMaExSncLsridAddr,             
                  dot1agCfmMepIfIndex, 
                  rcCfmMepExY1731DefectList
                }
    STATUS      current
    DESCRIPTION
       "Y1731-based fault LOC recover"
    ::= { rcCfmNotifications 20 }


rcCfmY1731FaultRDI
NOTIFICATION-TYPE
    OBJECTS     { 
                  rcCfmMaExOamType,
                  rcCfmMaExPrimaryVlanId,      
                  rcCfmMaExIngressLspId,      
                  rcCfmMaExEgressLspId,       
                  rcCfmMaExVcid,               
                  rcCfmMaExPwPeerAddrType,  
                  rcCfmMaExPwPeerAddr,      
                  rcCfmMaExSectionPort,      
                  rcCfmMaExSncLsridAddrType, 
                  rcCfmMaExSncLsridAddr,             
                  dot1agCfmMepIfIndex, 
                  rcCfmMepExY1731DefectList
                }
    STATUS      current
    DESCRIPTION
       "Y1731-based fault RDI happen"
    ::= { rcCfmNotifications 21 }

