		rcCfmMaExVlanList           OCTET STRING (SIZE (512))  ,
		rcCfmMaExCcmInterval        Dot1agCfmCcmInterval,
		rcCfmMaExCostumerVlan       VlanIdOrNone,
		rcCfmMaExPduPriority        Unsigned32 (0..7),
        rcCfmMaExRowStatus          RowStatus,
		rcCfmMaExPrimaryVlanId      VlanIdOrNone,
		rcCfmMaExOamType            INTEGER,  
		rcCfmMaExVcid               INTEGER,
		rcCfmMaExIngressLspId       OCTET STRING (SIZE (0..32)),
		rcCfmMaExEgressLspId        OCTET STRING (SIZE (0..32)),
	    rcCfmMaExSection	        TruthValue,
	    rcCfmMaExMipLabel	        INTEGER, 
	    rcCfmMaExSectionMac         MacAddress,
	    rcCfmMaExAisLckSrcClear     ClearVar,                                                               
        rcCfmMaExPwPeerAddrType     InetAddressType,
        rcCfmMaExPwPeerAddr                      InetAddress,
        rcCfmMaExSncTtl                          INTEGER(0..255),
        rcCfmMaExSncLsridAddrType                InetAddressType,
        rcCfmMaExSncLsridAddr                    InetAddress,
        rcCfmMaExTransitForwardPwVcid            INTEGER,
        rcCfmMaExTransitForwardPwPeerAddrType    InetAddressType,
        rcCfmMaExTransitForwardPwPeerAddr        InetAddress,
        rcCfmMaExTransitBackwardPwVcid           INTEGER,
        rcCfmMaExTransitBackwardPwPeerAddrType   InetAddressType,
        rcCfmMaExTransitBackwardPwPeerAddr       InetAddress,
        rcCfmMaExForwardMipIfNum                 Unsigned32,
        rcCfmMaExBackwardMipIfNum                Unsigned32, 
        rcCfmMaExCcmStatisticClear               ClearVar,
        rcCfmMaExAisStatisticClear               ClearVar,
        rcCfmMaExLckStatisticClear               ClearVar,
        rcCfmMaExCsfStatisticClear               ClearVar,
        rcCfmMaExLmResponseEnable                EnableVar,
        rcCfmMaExSectionPort                     InterfaceIndexOrZero
	}
      
