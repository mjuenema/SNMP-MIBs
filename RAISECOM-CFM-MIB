--Y.1731 Ma Extern CSF table
rcCfmMaExCsfTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF RcCfmMaExCsfEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Y.1731 MA Extended CSF table"
    ::= { rcCfm 24 }
    
rcCfmMaExCsfEntry OBJECT-TYPE
	  SYNTAX      RcCfmMaExCsfEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry in the rcCfmMaExCsfTable. "
    INDEX	{ dot1agCfmMdIndex, 
    		  dot1agCfmMaIndex
    		}
    ::= { rcCfmMaExCsfTable 1 } 
    
RcCfmMaExCsfEntry ::= SEQUENCE 
    {
		rcCfmMaExCsfTxPeriod INTEGER,
		rcCfmMaExCsfRxTrapEnable TruthValue,
		rcCfmMaExCsfRxLatestType INTEGER,
		rcCfmMaExCsfRxStats INTEGER  		
	}
     
rcCfmMaExCsfRxTrapEnable OBJECT-TYPE
    SYNTAX  TruthValue
  
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
       "Whether trap will send or not when CSF occurs."   
    DEFVAL {false}
    ::= { rcCfmMaExCsfEntry 1}

rcCfmMaExCsfTxPeriod OBJECT-TYPE
    SYNTAX  INTEGER{
	          csfTxPeriod_1s(1),
	          csfTxPeriod_60s(60)
	        }
  
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
       "When setting the period of sending CSF in this MA,it's periodically 
       send csf when the timer arrived because of some errors occur."   
    DEFVAL {csfTxPeriod_1s}
    ::= { rcCfmMaExCsfEntry 2 }
  
rcCfmMaExCsfRxLatestType OBJECT-TYPE
    SYNTAX  INTEGER{              
              csf_los(0),
	          csf_fdi(1),
	          csf_rdi(2),
	          csf_clr(3),
	          csf_invalid(255)
	        }
  
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "The lateset CSF type recieved from the peer mep." 
    DEFVAL {csf_invalid}
    ::= { rcCfmMaExCsfEntry 3 }

 rcCfmMaExCsfRxStats OBJECT-TYPE
    SYNTAX  INTEGER  
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "The total CSF packets recieved from the peer mep."
    ::= { rcCfmMaExCsfEntry 4 }
 
rcCfmMaExMepCsfTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF RcCfmMaExMepCsfEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Y.1731 MA Extended  MEP CSF  table"
    ::= { rcCfm 25 }
    
rcCfmMaExMepCsfEntry OBJECT-TYPE
	  SYNTAX      RcCfmMaExMepCsfEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry in the rcCfmMaExCsfTable. "
    INDEX	{ dot1agCfmMdIndex, 
    		  dot1agCfmMaIndex,
    		  dot1agCfmMepIdentifier
    		}
    ::= { rcCfmMaExMepCsfTable 1 } 
    
RcCfmMaExMepCsfEntry ::= SEQUENCE 
    {  
		rcCfmMaExMepCsfTxAdmin TruthValue,  
		rcCfmMaExMepCsfTxStatus INTEGER,                        
		rcCfmMaExMepCsfTxStats INTEGER
	}

  rcCfmMaExMepCsfTxAdmin OBJECT-TYPE
    SYNTAX  TruthValue 
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
       "Whether CSF function is enabled or not."  
    DEFVAL {false}
    ::= { rcCfmMaExMepCsfEntry 1 }
  
  rcCfmMaExMepCsfTxStatus OBJECT-TYPE
    SYNTAX  INTEGER{              
              csf_los(0),
	          csf_fdi(1),
	          csf_rdi(2),
	          csf_clr(3),
	          csf_invalid(255)
	        }
  
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "The current CSF type."    
    DEFVAL {csf_invalid}
    ::= { rcCfmMaExMepCsfEntry 2 }

 rcCfmMaExMepCsfTxStats OBJECT-TYPE
    SYNTAX  INTEGER  
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "The total CSF packets sent to the peer mep."
    ::= { rcCfmMaExMepCsfEntry 3 }

