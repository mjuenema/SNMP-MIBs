--------------------------------------------------------------------------------
--ETHERNET Service Quick Destroy Table
--------------------------------------------------------------------------------
rcCfmEthServiceQuickDestroyTable 					OBJECT-TYPE
    SYNTAX      SEQUENCE OF RcCfmEthServiceQuickDestroyEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Ethernet Service Quick Destroy Table "  
        
    ::= { rcCfm 35 }
    
rcCfmEthServiceQuickDestroyEntry          			OBJECT-TYPE
	SYNTAX      RcCfmEthServiceQuickDestroyEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry in the rcCfmEthServiceQuickDestroyTable. "
    INDEX	{
              dot1agCfmMdIndex,
              dot1agCfmMaIndex 
            }
    ::= { rcCfmEthServiceQuickDestroyTable 1 } 
    
    RcCfmEthServiceQuickDestroyEntry ::= SEQUENCE 
    {     
        rcCfmEthServiceRowStatus           RowStatus 
 	}
      	
rcCfmEthServiceRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
       "The status of the row."
    ::= {rcCfmEthServiceQuickDestroyEntry 1 }
		  	
END
