rcCfmMepExMplstpLtTable 	OBJECT-TYPE
    SYNTAX      SEQUENCE OF RcCfmMepExMplstpLtEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Operation table of MPLS-TP LT"                              
    ::= { rcCfm 29 }
    
rcCfmMepExMplstpLtEntry  	OBJECT-TYPE
	SYNTAX      RcCfmMepExMplstpLtEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry in the rcCfmMepExMplstpLtTable."
    INDEX	{ dot1agCfmMdIndex, 
    		  dot1agCfmMaIndex,
    		  dot1agCfmMepIdentifier
    		}
    ::= { rcCfmMepExMplstpLtTable 1 }   
    
RcCfmMepExMplstpLtEntry ::= SEQUENCE {
    rcCfmMepExMplstpLtLtmStatus           TruthValue,
	rcCfmMepExMplstpLtLtmType             INTEGER,
	rcCfmMepExMplstpLtLtmMode             INTEGER,
	rcCfmMepExMplstpLtLtmTargetMepId	  Unsigned32 (0..8191),
	rcCfmMepExMplstpLtLtmTargetMipICC     OCTET STRING (SIZE(1..6)),	
	rcCfmMepExMplstpLtLtmTargetMipNodeId  Unsigned32,
	rcCfmMepExMplstpLtLtmTargetMipIfNum   Unsigned32,
	rcCfmMepExMplstpLtLtmMaxTtl           Unsigned32 (0..255),
	rcCfmMepExMplstpLtLtmTimeout          Unsigned32 (0..60),
	rcCfmMepExMplstpLtLtmResult           INTEGER
}	

rcCfmMepExMplstpLtLtmStatus   OBJECT-TYPE 
	SYNTAX      TruthValue
    MAX-ACCESS  read-write
	STATUS      current
    DESCRIPTION
            "A Boolean flag set to true by the bridge 
             port to indicate that another LTM may be transmitted."
	::= {rcCfmMepExMplstpLtEntry 1 }

rcCfmMepExMplstpLtLtmType   OBJECT-TYPE 
	SYNTAX      INTEGER{
	                   mep(1),
	                   mip(2),
	                   ttl(3)
	                   }
    MAX-ACCESS  read-write
	STATUS      current
    DESCRIPTION
            "The type of MPLS-TP LT."
	::= {rcCfmMepExMplstpLtEntry 2 }	

rcCfmMepExMplstpLtLtmMode   OBJECT-TYPE 
	SYNTAX      INTEGER{
	                   node(1),
	                   interface(2)
	                   }
    MAX-ACCESS  read-write
	STATUS      current
    DESCRIPTION
            "The mode of MPLS-TP LT."
	::= {rcCfmMepExMplstpLtEntry 3 }
	
