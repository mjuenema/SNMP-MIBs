    --  
    --  hpxAgent Evaluation Scenario Table Definition:
    --
    --  This table pertains to the state of the monitored machines according
    --  to the Performance Evaluation neugent.
    --

    hpxEvalScenarioTable OBJECT-TYPE
        SYNTAX SEQUENCE OF HpxEvalScenarioEntry
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "&<hpxAgent.138>The hpxEvalScenarioTable contains one row per evaluated host that contains attributes related to the current state of the host.  If no hosts have been configured for evaluation or the Historical Performance Evaluation option is not installed, this table is empty."
        ::= {  hpxAgent 5 }
    HpxEvalScenarioEntry OBJECT-TYPE
        SYNTAX HpxEvalScenarioEntry
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "&<hpxAgent.139>A row in the evaluation scenario table."
        INDEX {
            hpxEvalScenarioHostName
        }
        ::= { hpxEvalScenarioTable 1 }
    HpxEvalScenarioEntry ::= SEQUENCE {
    	hpxEvalScenarioHostName
	        DisplayString,
        hpxEvalScenarioId
            INTEGER,
        hpxEvalScenarioModeIndex
            DisplayString,
        hpxEvalScenarioTimestamp
            DisplayString,
        hpxEvalScenarioProblem
            DisplayString,
        hpxEvalScenarioProbabilityOfProblem
            DisplayString,
        hpxEvalScenarioTimeToProblem
            DisplayString,
        hpxEvalScenarioAdditionalInfo
            DisplayString
    }
    hpxEvalScenarioHostName OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "&<hpxAgent.140>The name of the host to which this configuration information applies.  This allows for proxy Performance Evaluation for remote machines and devices in addition to the local host."
        ::= { HpxEvalScenarioEntry 1 }
    hpxEvalScenarioId OBJECT-TYPE
        SYNTAX INTEGER {
            nominal(1),
            nearNominal(2),
            someParamsUnusualButNominal(3),
            manyParamsUnusual(4),
            beyondHistoricalLimitsButNearNominal(5),
            beyondHistoricalLimits(6),
            highlyUnusual(7),
            cannotCategorise(8),
            notEvaluated(9),
            predictedSeverity1(10),
            predictedSeverity2(11),
            currentSeverity1(12),
            currentSeverity2(13)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "&<hpxAgent.141>The identity of the evaluated scenario for this host.  The 'notEvaluated' value is used as the default prior to the first evaluation, and if the evaluation fails.
Associated traps:
hpxEvalScenarioNominalTrap (6:501)
hpxEvalScenarioNearNominalTrap (6:502)
hpxEvalScenarioSomeParamsUnusualButNominalTrap (6:503)
hpxEvalScenarioManyParamsUnusualTrap (6:504)
hpxEvalScenarioBeyondHistoricalLimitsButNearNominalTrap (6:505)
hpxEvalScenarioBeyondHistoricalLimitsTrap (6:506)
hpxEvalScenarioHighlyUnusualTrap (6:507)
hpxEvalScenarioCannotCategoriseTrap (6:508)
hpxEvalScenarioNotEvaluatedTrap (6:509)
hpxEvalScenarioPredictedSeverity1 (6:510)
hpxEvalScenarioPredictedSeverity2 (6:511)
hpxEvalScenarioCurrentSeverity1 (6:512)
hpxEvalScenarioCurrentSeverity2 (6:513)
Note:  Another trap, hpxEvalScenarioTableUpdatedTrap (6:599), is issued when all changes to the scenario table have been completed after an evaluation."
        ::= { HpxEvalScenarioEntry 2 }
    hpxEvalScenarioModeIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "&<hpxAgent.142>The mode that the Historical Performance Evaluation neugent deems the machine's current behaviour to be closest to.  If the scenario Id is 'notEvaluated', this value should be ignored."
        ::= { HpxEvalScenarioEntry 3 }
    hpxEvalScenarioTimestamp OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "&<hpxAgent.143>The date and time that the evaluation was performed, resulting in the current scenario Id.  If the scenario Id is 'notEvaluated', this value can be blank.  The timestamp reflects the system clock of the host on which the neugent resides."
        ::= { HpxEvalScenarioEntry 4 }
    hpxEvalScenarioProblem OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "&<hpxAgent.144>A description of any predicted problem."
        ::= { HpxEvalScenarioEntry 5 }
    hpxEvalScenarioProbabilityOfProblem OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "&<hpxAgent.145>The probability of occurrence of any predicted problem."
        ::= { HpxEvalScenarioEntry 6 }
    hpxEvalScenarioTimeToProblem OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "&<hpxAgent.146>The projected time to occurrence for any predicted problem."
        ::= { HpxEvalScenarioEntry 7 }
    hpxEvalScenarioAdditionalInfo OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "&<hpxAgent.147>Supplemental information about the scenario."
        ::= { HpxEvalScenarioEntry 8 }

    --  
    --  hpxAgent Evaluation Exceptions Table Definition:
    --
    --  This table pertains to exceptional conditions detected by the
    --  Performance Evaluation neugent.
    --

--    hpxEvalExceptionTable OBJECT-TYPE
--        SYNTAX SEQUENCE OF HpxEvalExceptionEntry
--        ACCESS read-only
--        STATUS mandatory
--        DESCRIPTION
--        "&<hpxAgent.148>The hpxEvalExceptionTable contains multiple rows per evaluated host that contain attributes related to exceptional conditions.  If no hosts have been configured for evaluation, the Historical Performance Evaluation option is not installed, or no exceptions have been detected during the last evaluation, this table is empty.  Note:  the hpxEvalExceptionDetectedTrap (6:601) trap is issued each time that an exception is reported by the neugent.  In addition the hpxEvalExceptionTableUpdatedTrap (6:699) trap is issued once all exceptions have been reported after an evaluation."
--        ::= {  hpxAgent 6 }
--    HpxEvalExceptionEntry OBJECT-TYPE
--        SYNTAX HpxEvalExceptionEntry
--        ACCESS read-only
--        STATUS mandatory
--        DESCRIPTION
--        "&<hpxAgent.149>A row in the evaluation exception table."
--        ::= { hpxEvalExceptionTable 1 }
--    HpxEvalExceptionEntry ::= SEQUENCE {
--    	hpxEvalExceptionHostName
--	        DisplayString,
--        hpxEvalExceptionResourceTypeName
--	        DisplayString,
--        hpxEvalExceptionResourceSubtypeName
--	        DisplayString,
--        hpxEvalExceptionResourceInstanceName
--	        DisplayString,
--        hpxEvalExceptionResourceValue
--	        DisplayString,
--        hpxEvalExceptionLowerHistoricalBound
--	        DisplayString,
--        hpxEvalExceptionUpperHistoricalBound
--	        DisplayString,
--        hpxEvalExceptionLowerModeBound
--	        DisplayString,
--        hpxEvalExceptionUpperModeBound
